<script type="text/javascript">
$(".hr-menu > li > a").click(function() {
	var $li = $(this).parent();
	if ($li.hasClass("active")) {
		$li.removeClass("active");
	} else {
		$li.addClass("active");
	}
});

$(".hr-menu ul a").click(function() {
	var $this = $(this), $lis = $(".hr-menu ul a"), $cont;
	if ($this.hasClass("current")) {
		return false;
	} else {
		$lis.removeClass("current");
		$(".hr-content").removeClass("show");
		$cont = $("#" + $this.addClass("current").attr("wid")).addClass("show");
		$("html, body").animate({
			scrollTop: $cont.offset().top
		});
	}
});
</script>

