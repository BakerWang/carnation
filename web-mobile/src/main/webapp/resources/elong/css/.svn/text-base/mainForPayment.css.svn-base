/* === Preloader === */
.preloader {
  display: inline-block;
  width: 20px;
  height: 20px;
  -webkit-transform-origin: 50%;
  -ms-transform-origin: 50%;
  transform-origin: 50%;
  -webkit-animation: spin 1.58s linear infinite;
  -moz-animation: spin 1.58s linear infinite;
  animation: spin 1.58s linear infinite;
}

.modal .preloader {
  width: 34px;
  height: 34px;
}

.preloader-indicator-overlay {
  visibility: visible;
  background-color: rgba(0, 0, 0, 0.1);
  position: absolute;
  top: 0px;
  width: 100%;
  height: 100%;
  z-index: 10600;
}

.android .preloader-indicator-overlay {
  position: fixed;
}

.preloader-indicator-modal {
  display: block;
  background: #fff;
  height: 80px;
  width: 80px;
  border-radius: 10px;
  background-image: url("img/loading3.gif");
  background-size: auto 61px;
  background-repeat: no-repeat;
  background-position-x: center;
  background-position-y: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -40px;
  margin-top: -40px;
  z-index: 12000;
  box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.3);
}
.preloader-indicator-modal .preloader {
  display: block;
  width: 34px;
  height: 34px;
}

.android .preloader-indicator-modal {
  position: fixed;
}

.preloader:after {
  display: block;
  content: "";
  width: 100%;
  height: 100%;
  background-position: 50%;
  background-size: 100%;
  background-repeat: no-repeat;
}

@-webkit-keyframes spin {
  0% {
    -webkit-transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
  }
}
@keyframes spin {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
/* === Pull To Refresh === */
.show-refresh-layer .pull-to-refresh-layer {
  visibility: visible;
}

.pull-to-refresh-layer {
  position: absolute;
  margin-top: 20px;
  left: 0;
  top: 6px;
  width: 100%;
  height: 30px;
}
.pull-to-refresh-layer .preloader {
  position: absolute;
  left: 24px;
  top: 50%;
  margin-top: -10px;
  visibility: hidden;
}
.pull-to-refresh-layer .pull-to-refresh-arrow {
  width: 13px;
  height: 20px;
  position: absolute;
  left: 24px;
  top: 50%;
  margin-top: -10px;
  background: no-repeat center;
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 26 40'><polygon points='9,22 9,0 17,0 17,22 26,22 13.5,40 0,22' fill='%238c8c8c'/></svg>");
  background-size: 13px 20px;
  -webkit-transform: rotate(0deg) translate3d(0, 0, 0);
  -ms-transform: rotate(0deg) translate3d(0, 0, 0);
  transform: rotate(0deg) translate3d(0, 0, 0);
  -webkit-transition: 300ms;
  -moz-transition: 300ms;
  transition: 300ms;
}
.pull-to-refresh-layer .pull-to-refresh-label {
  height: 30px;
  line-height: 30px;
  color: #8c8c8c;
  text-align: center;
}
.pull-to-refresh-layer.show-refresh-layer {
  visibility: visible;
}
.pull-to-refresh-layer.refreshing .pull-to-refresh-arrow {
  visibility: hidden;
  -webkit-transition: 0ms;
  -moz-transition: 0ms;
  transition: 0ms;
}
.pull-to-refresh-layer.refreshing .preloader {
  visibility: visible;
}
.pull-to-refresh-layer.pull-up .pull-to-refresh-arrow {
  -webkit-transform: rotate(180deg) translate3d(0, 0, 0);
  -ms-transform: rotate(180deg) translate3d(0, 0, 0);
  transform: rotate(180deg) translate3d(0, 0, 0);
}

.pull-to-refresh-content.transitioning {
  -webkit-transition: 400ms;
  -moz-transition: 400ms;
  transition: 400ms;
}
.pull-to-refresh-content.refreshing {
  -webkit-transform: translate3d(0, 44px, 0);
  -ms-transform: translate3d(0, 44px, 0);
  transform: translate3d(0, 44px, 0);
}
.pull-to-refresh-content.refreshing .pull-to-refresh-arrow {
  visibility: hidden;
  -webkit-transition: 0ms;
  -moz-transition: 0ms;
  transition: 0ms;
}
.pull-to-refresh-content.refreshing .preloader {
  visibility: visible;
}
.pull-to-refresh-content.pull-up .pull-to-refresh-arrow {
  -webkit-transform: rotate(180deg) translate3d(0, 0, 0);
  -ms-transform: rotate(180deg) translate3d(0, 0, 0);
  transform: rotate(180deg) translate3d(0, 0, 0);
}

