.comp-wrap {
  background: #FFF;
  border-left: 1px solid #DCDCDC;
  border-right: 1px solid #DCDCDC;
  margin: 0 10px;
  padding: 0 10px;
  color: #555;
}
.comp-wrap .sucess {
  padding: 30px 0 15px;
  text-align: center;
  font-size: 1.2rem;
}
.comp-wrap .sucess .correct {
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  background: #3EBB6A;
  width: 32px;
  height: 32px;
  border-radius: 16px;
  position: relative;
}
.comp-wrap .sucess .correct:after {
  content: "";
  width: 13px;
  height: 8px;
  border: 1px solid #FFF;
  border-width: 0 0 3px 3px;
  position: absolute;
  top: 9px;
  left: 8px;
  -webkit-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  transform: rotate(-45deg);
}
.comp-wrap .sucess .tip {
  color: #F55;
  font-size: .813rem;
}
.comp-wrap .sucess .tip .star {
  display: inline-block;
  width: 18px;
  height: 18px;
  background-image: url("img/finishorder_star.png");
  background-size: 100% 100%;
}
.comp-wrap .progress {
  border-bottom: 1px dashed #F2F2F2;
  padding: 10px 5px 0;
}
.comp-wrap .progress h2 {
  font-size: 1rem;
  font-weight: normal;
  margin-bottom: 20px;
}
.comp-wrap .progress ul {
  margin-left: 20px;
}
.comp-wrap .progress ul li {
  border-left: 1px solid #DEDEDE;
  font-size: .9rem;
  line-height: 1rem;
  padding-bottom: 12px;
  position: relative;
  padding-left: 20px;
}
.comp-wrap .progress ul li i {
  width: 15px;
  height: 15px;
  display: block;
  border: 1px solid #DDD;
  background: #F7F7F7;
  border-radius: 7px;
  position: absolute;
  left: -8px;
}
.comp-wrap .progress ul li:last-child {
  border-color: #FFF;
}
.comp-wrap .progress ul .on {
  color: #3EBB6A;
}
.comp-wrap .progress ul .on i {
  border-color: #3EBB6A;
  background: #CEFFDF;
}
.comp-wrap .progress ul .on i:after {
  content: "";
  width: 5px;
  height: 5px;
  background: #3EBB6A;
  border-radius: 3px;
  position: absolute;
  top: 4px;
  left: 4px;
}
.comp-wrap .info {
  border-bottom: 1px dashed #F2F2F2;
  padding: 10px 5px;
  overflow: hidden;
  font-size: .9rem;
}
.comp-wrap .info dl {
  overflow: hidden;
  margin: 4px 0;
}
.comp-wrap .info dl dt {
  float: left;
  width: 80px;
  text-align: right;
  color: #777;
}
.comp-wrap .info dl dd {
  margin-left: 80px;
}
.comp-wrap .info dl dd span {
  margin-left: 10px;
}
.comp-wrap .consume {
  padding: 10px 5px;
}
.comp-wrap .consume .price {
  font-size: 1rem;
  color: #555;
  margin-bottom: 4px;
}
.comp-wrap .consume .price span {
  color: #E55749;
}
.comp-wrap .consume .tip {
  font-size: .9rem;
  color: #777;
  margin: 0;
}
.comp-wrap .consume .tip span {
  margin-left: 10px;
}
.comp-wrap .division {
  height: 20px;
  overflow: hidden;
  color: #CBCBCB;
  word-wrap: break-word;
  font-size: 1.2rem;
}
.comp-wrap .b-curve {
  height: 5px;
  display: block;
  background-image: url("img/comp-bottom.png");
  background-size: 10px 5px;
  margin-left: -1px;
  position: relative;
  bottom: -2px;
  margin: 0 -10px;
}

.comp-link {
  text-align: center;
  padding-bottom: 25px;
  padding-top: 15px;
}
.comp-link a {
  color: #4D7BE1;
  padding: 0 12px 0 20px;
  background-position: bottom left;
  margin-left: 12px;
  display: inline-block;
  line-height: 16px;
}
.comp-link .see {
  border-right: 1px solid #C9C9C9;
  background: url("img/comp-b-see.png") no-repeat;
  background-size: auto 16px;
}
.comp-link .share {
  border-right: 1px solid #C9C9C9;
  background: url("img/comp-b-share.png") no-repeat;
  background-size: auto 16px;
}
.comp-link .return {
  background: url("img/comp-b-return.png") no-repeat;
  background-size: auto 16px;
}
.comp-link .book {
  background: url("img/comp-b-book.png") no-repeat;
  background-size: auto 16px;
  padding-left: 22px;
}

@media only screen and (min-height: 37.5em) and (orientation: landscape), only screen and (min-width: 37.5em) and (orientation: portrait) {
  .comp-wrap {
    width: 700px;
    margin: 20px auto 0;
    padding: 0 20px;
  }
  .comp-wrap .t-shaft {
    margin: 0 -25px;
    display: block;
    height: 10px;
    background-color: #515151;
    border-radius: 4px;
    position: relative;
  }
  .comp-wrap .t-shaft:after, .comp-wrap .t-shaft:before {
    content: "";
    width: 8px;
    height: 15px;
    border-radius: 3px;
    background: #434242;
    position: absolute;
    top: -1px;
    z-index: -1;
  }
  .comp-wrap .t-shaft:after {
    left: 0;
  }
  .comp-wrap .t-shaft:before {
    right: 0;
  }
  .comp-wrap .sucess {
    padding: 40px 0 20px;
  }
  .comp-wrap .progress {
    padding: 10px 10px 0;
  }
  .comp-wrap .info {
    padding: 10px;
  }
  .comp-wrap .info dl {
    float: left;
  }
  .comp-wrap .info dl:nth-child(odd) {
    width: 38%;
  }
  .comp-wrap .info dl:nth-child(even) {
    margin-left: 2%;
    width: 60%;
  }
  .comp-wrap .consume {
    padding: 10px;
  }
  .comp-wrap .change {
    padding: 10px 10px 30px;
  }
  .comp-wrap .b-curve {
    margin: 0 -20px;
  }
}
