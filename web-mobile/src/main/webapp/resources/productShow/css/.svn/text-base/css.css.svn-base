.page {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background: #FFFFFF;
}

   

.pub-passdown {
    position: absolute;
    left: 20px;
    top: 50%;
    width: 35px;
    height: 21px;
    margin-top: -17.5px;
    -webkit-animation:slide 1.5s infinite;
    -ms-animation:slide 1.5s infinite;
    animation:slide 1.5s infinite;
    -webkit-animation-name: slide;
    -webkit-animation-timing-function: ease;
    -webkit-animation-duration: 1.5s;
    -webkit-animation-iteration-count: infinite;
    background: url(../images/arrow1.png) center no-repeat;
    background-size: 100%;
    -webkit-transform-style: preserve-3d;
    transform-style: preserve-3d;
}

    

@keyframes slide {
    0% {
        -webkit-transform: translate3d(15px,0, 0) rotate(270deg);
        transform: translate3d(15px,0, 0) rotate(270deg);
        opacity: 0;
    }

    80% {
        -webkit-transform: translate3d(-20px,0,0) rotate(270deg);
        transform: translate3d(-20px, 0,0) rotate(270deg);
        opacity: 1;
    }

    100% {
        -webkit-transform: translate3d(-25px, 0,0) rotate(270deg);
        transform: translate3d(-25px,0,0) rotate(270deg);
        opacity: 0;
    }
}

@-webkit-keyframes slide {
    0% {
        -webkit-transform: translate3d(15px, 0,0) rotate(270deg);
        opacity: 0;
    }

    80% {
        -webkit-transform: translate3d(-20px, 0,0) rotate(270deg);
        opacity: 1;
    }

    100% {
        -webkit-transform: translate3d(-25px, 0,0) rotate(270deg);
        opacity: 0;
    }
}

@-ms-keyframes slide {
    0% {
        -ms-transform: translate3d(15px, 0,0) rotate(270deg);
        opacity: 0;
    }

    80% {
        -ms-transform: translate3d(-20px, 0,0) rotate(270deg);
        opacity: 1;
    }

    100% {
        -ms-transform: translate3d(-25px, 0,0) rotate(270deg);
        opacity: 0;
    }
}


.page img {
    display: block;
    width:100%;
    max-width: 100%;
    max-height: 100%;
    margin: auto;
    pointer-events: none;
}


.page:nth-child(n+3) {
    display: none;
}

#page1, #page21 {
    display: block;
    
}

    #page21::after {
        display: none;
    }

    #page21 .share-guide {
        position: absolute;
        top: 10px;
        right: 0;
        width: 320px;
    }

    #page21 .download {
        position: absolute;
        z-index: 1;
        top: 0;
        right: auto;
        bottom: auto;
        left: 0;
        width: 100%;
        height: 100%;
        padding: 0;
    }


.play{
	background-image: url(../images/play.png);
    animation: rotate 10s linear infinite;
    -webkit-animation: rotate 10s linear infinite;
    -moz-animation: rotate 10s linear infinite;
    -ms-animation: rotate 10s linear infinite;
}
.pause{
 	background-image: url(../images/pause.png);
  	animation-play-state: paused;
	-webkit-animation-play-state: paused;
	-moz-animation-play-state: paused;
	-ms-animation-play-state: paused;  
}

#voice {
	width: 40px;
	height: 40px;
	position: fixed;
	top: 10px;
	right: 10px;
	background-size: 100% 100%;
	z-index: 10000;
}


@-webkit-keyframes rotate {
	from {
		-webkit-transform: rotate(0deg)
	}

	to {
		-webkit-transform: rotate(360deg)
	}
}

@-moz-keyframes rotate {
	from {
		-moz-transform: rotate(0deg)
	}

	to {
		-moz-transform: rotate(360deg)
	}
}

@-ms-keyframes rotate {
	from {
		-ms-transform: rotate(0deg)
	}

	to {
		-ms-transform: rotate(360deg)
	}
}

@keyframes rotate {
	from {
		transform: rotate(0deg)
	}

	to {
		transform: rotate(360deg)
	}
}


.goHome{
    position: absolute;
    right: 10px;
    bottom: 10px;
    font-size: 14px;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    text-align: center;
    line-height: 20px;
    background: #C3BCBC;
    padding: 5px;
    box-sizing: content-box;
    color: #4a4a4a;
    z-index: 9;
    display: none;
}










  
