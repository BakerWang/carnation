.questionnaire .fixedTop{position: fixed;top: 0;left: 0;width: 100%;background: #f7f7f7;border-bottom:solid 1px #ddd;z-index: 999;}
.questionnaire .fixedTop .title{font-size: 20px;margin-top: 0.3rem;text-align: center;color:#4a4a4a}
.fixedTop .processBar{margin: 0.3rem 0 0.3rem 0;height: 0.6rem;line-height: 0.6rem;font-size: 0px;}
.processBar .wraper{border-radius: 0.32rem;background: #f0f0f0;margin-left: 0.48rem;width: 6rem;height: 0.6rem;float: left;}
.processBar .wraper i{display: block;height: 0.6rem;background: #19bf96;width: 0%;border-radius: 0.32rem;transition: all 0.2s ease-out;}
.processBar .number{width: 0.3rem;font-size: 14px;float: left;line-height: 0.6rem;color: #19bf96;margin: 0 0 0 5px;}
.questionArea{overflow: hidden;margin: 2.1rem 0 0 0;color: #4a4a4a;}
.questionArea .question{overflow: hidden;font-size: 14px;margin: 0.2rem 0 0 0;}
.questionArea .question .title{padding: 0 0.36rem;line-height: 18px;}
.questionArea .question ul{margin-left: 0.76rem;overflow: hidden;}
.questionArea .question ul li{float: left;min-width: 3.2rem;margin: 0.3rem 0 0 0;}
.questionArea .question ul li span{text-indent: 0.6rem;position: relative;display: inline-block;height: 0.5rem;line-height: 0.5rem;}
.questionArea .question ul li span i{position: absolute;left: 0;top: 0;width: 0.44rem;height: 0.44rem;border-radius: 50%;border:solid 1px #c7c7cd;box-sizing: border-box;}
.questionArea .question ul li span.curr i{background: #19bf96;border-color: #19bf96;}
.questionArea .question ul li span.curr i::after{content: '';color: #fff;position: absolute;font-weight: bold;text-indent: 0;background: url(data:image/png;base64,) no-repeat;background-size:contain;
width: 0.3rem;height: 0.24rem;top: 0.1rem;left: 0.05rem;
}
.btnArea{margin: 0.2rem;}
.btnArea a{width: 2rem;height: 0.6rem;line-height: 0.6rem;font-size: 16px;text-align: center;margin: 0 auto;background: #19BF96;border-radius: 5px;display: block;color: #fff;}

.circle{width: 22px;height: 22px;border-radius: 50%;border:solid 1px #19BF96;display: block;position: relative;box-sizing: border-box;}
.circle:before{position: absolute;width: 22px;height: 22px;border:solid 1px #19BF96;background: #19BF96;border-radius: 50%;left: -1px;top: -1px;content: "";transform: scale(0.01);transform-origin: 50% 50%;box-sizing: border-box;transition: all 0.2s;}
.curr .circle:before{transform: scale(0.7);}
