<script type="text/javascript">
jQuery(document).ready(function(){
	$("input[name='isAvailableBalance']").click(function (){
		var selt=$(this);
		var isChecked= selt.is(":checked");
		var availableBalance=parseFloat(selt.attr("availableBalance"));
		var allOfPrice=parseFloat(selt.attr("allOfPrice"));
		if(isChecked){
			if(availableBalance >= allOfPrice){
				$("#span_all_of_price").text("0");
			}else{
				$("#span_all_of_price").text(parseFloat(allOfPrice - availableBalance).toFixed(2));
			}
			$("#div_payment_password").show();

            $("input[name='paymentPassword']").val("").rules("add", {
                required:true,
                remote:{
                    url: "$!webPath/buyer/money_password_validate.htm",     //后台处理程序
                    type: "post",               //数据发送方式
                    dataType: "json",           //接受数据格式
                    data: {                     //要传递的数据
                        "paymentPassword": function(){return jQuery("input[name='paymentPassword']").val();}
                    }
                },
                messages: {
                    required:"支付密码不能为空",remote:"支付密码不正确,<a href='$webPath/buyer/money_password.htm' target='_blank' class='c_blue'>找回密码</a>"
                }
            })
		}else{
			$("#span_all_of_price").text(allOfPrice);
			$("#div_payment_password").hide();
            $("input[name='paymentPassword']").rules("remove");
		}
	});
	$("li[data-tabname]").click(function() {
        var $cont = $("li[data-tabcontent="+ $(this).data("tabname") +"]");
        if ($cont.length > 0) {
            $(this).addClass("current").siblings().removeClass("current");
            $cont.show().siblings().hide();
        }
    });
	
});
</script>
