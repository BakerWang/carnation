function goods_count_sub(){
   var goods_count= jQuery("#goods_count").val();
   if(goods_count>1){
		goods_count--;
		jQuery("#goods_count").val(goods_count);
	}
}
function goods_count_add(){
   var goods_count= jQuery("#goods_count").val();
   if(goods_count < goods_inventory){
		goods_count++;
		jQuery("#goods_count").val(goods_count);
	 }
}
