                    jQuery.each(jQuery(this).find("a[class=this]"),function(){									
					  gsp=jQuery(this).attr("gsp")+","+gsp;
					  if(count==0){
					      sname="“"+jQuery(this).attr("sname")+"“";	  
						  }else{
						  sname="“"+jQuery(this).attr("sname")+"”,"+sname;	  
							  }
					  count++;
					});
	            });
