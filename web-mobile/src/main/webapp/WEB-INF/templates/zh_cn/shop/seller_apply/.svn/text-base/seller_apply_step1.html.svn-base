<body>
$!httpInclude.include("$!webPath/top.htm")
	#if($config.websiteLogo)
    #set($img="$!{config.websiteLogo.path}")
    #else
    #set($img="$!webPath/resources/style/system/front/default/images/logo.png")
    #end
<div class="head">
  <h1 class="logo" title=""> <a href="$!webPath/group/index.htm"><img src="$img" border="0" width="209" height="70"/></a>  </h1>
  <span class="head_into">商家入驻</span> <a href="$!webPath/doc.htm?mark=apply_help" target="_blank" class="head_help">入驻帮助</a> </div>
<div class="nav_into">
  <ul>
    <li><a href="javascript:void(0)">入驻须知</a></li>
    <li><a href="javascript:void(0)" class="this" >商户信息提交</a></li>
    <li><a href="javascript:void(0)">审核日志查询</a></li>
  </ul>
</div>

<div class="main clearfix">
    <form id="thenForm" action="$webPath/seller_apply/group/step1_save.htm" autocomplete="off" method="post">
        <ul class="join-form">
            <li>
                <span class="txt-label"><span class="c_red">*</span> 商户名称：</span>
                <input type="text" name="storeName" class="txt" value="$!store.storeName"/>
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 商户全称：</span>
                <input type="text" name="licenseCName" class="txt" value="$!store.licenseCName"/>
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 身份证号码：</span>
                <input type="text" name="storeOwerCard" class="txt" value="$!store.storeOwerCard"/>
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 所属行业：</span>
                <input type="text" class="txt" name="industry" value="$!applyInfo.industry" />
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 经营范围：</span>
                <textarea rows="3" cols="60" name="licenseBusinessScope" >$!applyInfo.licenseBusinessScope</textarea>
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 公司简介：</span>
                <textarea rows="3" cols="60"  name="licenseCDesc" >$!applyInfo.licenseCDesc</textarea>
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 主营类目：</span>
                <select name="groupMainId" id="main_category">
	                <option value="">请选择主营类目</option>
                	#foreach($gc in $!groupClassList)
	                    <option value="$gc.id" #if($gc.id == $store.groupMainId) selected="selected" #end>$gc.gcName</option>
                	#end
                </select>
            </li>
            <li id="category_li" #if(!$!subGroupClassList) style="display: none;" #end>
                <span class="txt-label"><span class="c_red">*</span> 详细类目：</span>
                <div class="category-chks" id="groupDetailInfo">
                	#foreach($gc in $!subGroupClassList)
                		<label class='item'>
                			<input type='checkbox' name='groupDetailInfo' #if($!groupDetailInfo.contains($gc.id)) checked="checked" #end value='$gc.id' />$gc.gcName
                		</label>
                	#end
                </div>
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 商户所在地区：</span>
		        <select name="area1" id="area1" level="2" class="area">
		          <option value="">省份/自治区</option>
		          #foreach($area in $areas)           
		          <option value="$!area.id" #if($!area.id==$!areaOneId) selected="true" #end>$!area.areaName</option>
		           #end          
		        </select>
		        <select name="area2" id="area2" level="3" class="area">
	        	 #if(!$!areas2)
	        		<option value="">市</option>
	        	#end
		         #foreach($area in $areas2)                     
		          <option value="$!area.id" #if($!area.id==$!areaTowId) selected="true"#end>$!area.areaName</option>
		         #end                    
		        </select>
		        <select name="area3" id="area3" level="4">
		        #if(!$!areas3)
		        	<option value="">县/区</option>
		        #end
		         #foreach($area in $areas3)                     
		          <option value="$!area.id" #if($!area.id==$!store.areaId) selected="true"#end>$!area.areaName</option>
		         #end                    
		        </select>
		        <input id="areaId" name="areaId" type="hidden" value="$!store.areaId" />
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 详细地址：</span>
                <input type="text" class="txt" id="address" name="storeAddress" value="$!store.storeAddress" />
                <a href="javascript:void(0)" class="f-line c_blue"  dialog_uri="$!current_webPath/static?name=marker_map.html" dialog_title="标记地址" dialog_width="640" dialog_height="400" dialog_id="marker_dialog">标记</a>
                <span id="marker_map_state" class="c_gray ml-20"></span>
                <input type="hidden" id="maker_pos_N" name="storeLat" value="$!store.storeLat">
                <input type="hidden" id="maker_pos_E" name="storeLon" value="$!store.storeLon" >
            </li>
            <li>
                <span class="txt-label"><span class="c_red">*</span> 固定电话：</span>
                <input type="text" class="txt" name="storeTelephone" value="$!store.storeTelephone" />
            </li>
            <li>
