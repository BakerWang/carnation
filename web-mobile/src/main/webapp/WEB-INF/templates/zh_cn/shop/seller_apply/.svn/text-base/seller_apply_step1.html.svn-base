                <input type="text" class="txt" name="licenseCMobile" value="$!store.licenseCMobile" />
            </li>
<!--             <li> -->
<!--                 <span class="txt-label">所属身份及编号：</span> -->
<!--                 <input type="text" class="txt" name="inviterNumber" value="$!applyInfo.inviterNumber" /> -->
<!--             </li> -->
