        function time()

        {
          var now= new Date();
          var hh = now.getHours();
          var mm = now.getMinutes();
          var clock = "";
          if(hh < 10)
              clock += "0";
          clock += hh + ":";
          if (mm < 10) clock += '0'; 
          clock += mm; 
          document.getElementById("msgtime1").innerHTML=clock;
          document.getElementById("msgtime2").innerHTML=clock;
          document.getElementById("msgtime3").innerHTML=clock;
          document.getElementById("msgtime4").innerHTML=clock;
          document.getElementById("msgtime5").innerHTML=clock;
        } 
