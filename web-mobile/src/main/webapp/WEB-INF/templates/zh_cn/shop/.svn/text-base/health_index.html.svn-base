	<div class="main">
		<div class="app">
			<div id="head">
				<!-- banner -->
				 <div class="row" style="overflow: hidden;height: 4rem;width: 100%">
			        <div id="slide" style="overflow: hidden;height: 4rem;width: 100%;">
			            <div class="hd">
			                <ul><li class="on">1</li><li class="on">2</li><li class="on">3</li></ul>
			            </div>
			            <div class="bd" style="overflow: hidden;height: 4rem;width: 100%;">
			                <div class="tempWrap" style=" position:relative;overflow: hidden;height: 4rem;width: 100%;">
			                	<ul style=" width: 100%; position: relative; overflow: hidden; padding: 0px; margin: 0px; transition-duration: 200ms; transform: translateX(-768px);">
				                	
				                	#foreach($adv in $advList)
				                	<li style="display: table-cell; vertical-align: top; width: 768px; overflow: hidden;">
				   	                    	<a href="$adv.adUrl" >
				   	                    		<img src="$adv.adImgPath" alt="">
				   	                    	</a>
				   	                 </li>
				   	                #end
				                	</ul>
			                </div>
			            </div>
			        </div>
    			</div>
    			
		#set($divIdEnd=1)
        #foreach($obj in $goodscases)
			#set($case_list=$!goodsCaseViewTools.queryCaseGoods($obj.caseContent))
    			<div class="box"><!-- float0$!divIdEnd --> 
    			#set($divIdEnd=$divIdEnd+1)
	    			<div class="menu-title">
	    				<img data-original="$!webPath/resources/shop/images/Line@2x.png" alt="">
<!-- 标题   -->
          					$!obj.caseName
          				<img data-original="$!webPath/resources/shop/images/Line@2x.png" alt="">
          			</div>
<!-- 商品详情bg   -->
