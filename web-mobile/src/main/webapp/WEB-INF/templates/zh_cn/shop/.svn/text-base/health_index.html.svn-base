	    				<img src="$!webPath/resources/shop/images/Line@2x.png" alt="">
	    			</div>
		    		<div class="lanmu-goods" >
		    			<div class="menu-title-img">
		    				<!-- <img src="$!webPath/resources/shop/images/WeChat_1468665331@2x.png" alt=""> -->
		    				$!gf_tools.generic_adv("$!url","$!floor.gfLeftAdv")
		    			</div>
						<div class="sanjiao">
							<img src="$!webPath/resources/shop/images/jiantou.png" alt="">
						</div>
		    			<div style="">
						  	<div id="Marquee_x">
							    <ul>
							      <li>
							      #set($map=$!gf_tools.generic_goods_list("$!floor.gfListGoods"))
							      #foreach($goods in $!map.get("goodsList"))
									      #if($!goods.goodsMainPhotoPath)
									         #set($img="$!{goods.goodsMainPhotoPath}")
									      #else
									         #set($img="$!{config.goodsImage.path}")
									      #end
								        
								          #set($goods_url="$!webPath/goods_$!{goods.id}.htm")
								
								          #if($!config.secondDomainOpen && $!goods.goodsType==1 && $!goods.storeSecondDomain!="")
								          	#set($goods_url="http://$!{goods.storeSecondDomain}.$!{domainPath}/goods_$!{goods.id}.htm")
							          	  #end
								        <div>
								        	<a href="$!goods_url"  data-title="商品详情">
									        	<img  data-original="$!{img}" alt=""/>
												<span class="lun-title">$!goods.goodsName</span>
											</a>
								        	<span class="lun-price">¥  <span class="lun-price-2">$!{goods.goodsShowPrice}</span> </span>
								        </div>
							      #end
							      </li>
							    </ul>
							</div>
						</div>   
					</div>
			</div>
		#end
		</div>
		
	 </div>
	</div>
</div>
	
	
	
	<div class="actGotop">
	<a href="javascript:void(0);" onclick="go_cart()">
		<img src="$!webPath/resources/shop/images/addcar.png" alt="" style="display: block;">
	</a>
	<a href="javascript:;" title="返回顶部">
		<img src="$!webPath/resources/shop/images/dinzhi.png" alt="">
	</a> 
	</div>
	
	<script src="$!webPath/resources/js/jquery.min.js" type="text/javascript"></script>
	<script src="$!webPath/resources/js/common.js"></script>
	<script src="$!webPath/resources/js/jQuery1.7.js" type="text/javascript" ></script>
	<script src="$!webPath/resources/js/srcolltop.js" type="text/javascript"></script>
	<script src="$!webPath/resources/js/TouchSlide.js" type="text/javascript"></script>
	<script src="$!webPath/resources/js/jquery.lazyload2.js"></script>
	<script src="$!webPath/resources/js/goToHtml.js"></script>
	<script src="$!webPath/resources/js/tools.js"></script>
	<script src="$!webPath/resources/js/tipsbox.js"></script>
	
	
	<script type="text/javascript">
	 $('img').lazyload();
	 var arr_tab=[];
	 $('#tab ul li div.fn').each(function(){
		 var left=$(this).offset().left;
		 arr_tab.push(left);
	 });
	$('#tab ul li div.fn').click(function() {
		$(this).addClass('selected').siblings().removeClass('selected');
		var index=$('#tab ul li div.fn').index(this);
		$("#tab_menu").stop(true,true).animate({"scrollLeft":arr_tab[index]},500);
		
	});
	
	
	<!-- 定位导航 -->
	var processor = {  
	    timeoutId: null, 		 
	    performProcessing: function(){
	    	var nowscrollTop=document.documentElement.scrollTop||document.body.scrollTop;
			for(var i=arr.length;i>=0;i--){
				if(nowscrollTop>=arr[i]){
					$("#tab ul li div.fn").eq(i).addClass("selected").siblings("div.fn").removeClass("selected");
					return;
				}
			}
	    },  		 
	    process: function(){
	        clearTimeout(this.timeoutId);  
	        this.timeoutId  = setTimeout(function(){ 
	            processor.performProcessing();  
	        }, 100);  
	    }  
	}; 

	
	var arr=[];
    $(".box").each(function(){
    	var top=parseInt($(this).offset().top)-parseInt(window.getComputedStyle(document.getElementById("tab")).height);
    	arr.push(top);
    });
	$(window).on("scroll",function(){
		processor.process();
	});
	
	$("#tab ul li div.fn").click(function(){
		var index=$("#tab ul li div.fn").index(this);
		$("html,body").animate({scrollTop:arr[index]},200);
	});
	   

	TouchSlide({
		slideCell:"#slide",
		titCell:".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
		mainCell:".bd ul",
		effect:"left",
		autoPlay:true,//自动播放
		autoPage:true, //自动分页
		switchLoad:"_src" //切换加载，真实图片路径为"_src"
	});
	$(".old-price").each(function(){			
		var html=$(this).html().split('.').join(".</span><span class=\"small\">");
		$(this).html(html);
	});
	$(".lun-price-2").each(function(){			
		var html=$(this).html().split('.').join(".</span><span class=\"small\">");
		$(this).html(html);
	});
	
	$(".huijia").each(function(){			
		var html=$(this).html().split('.').join(".</span><span class=\"small\">");
		$(this).html(html);
	});
