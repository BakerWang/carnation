			$(".footer-fc").hide();
			}

	}); */
	
	/* 进入购物车 */
	function go_cart(){
		#if($!user)
			location.href="$!webPath/goods_cart1_load.htm";
		#else
			tipsbox("请先登录！",function(){
				   location.href="$!webPath/user/login.htm";
			 });
		#end
	}
	</script>
#end

