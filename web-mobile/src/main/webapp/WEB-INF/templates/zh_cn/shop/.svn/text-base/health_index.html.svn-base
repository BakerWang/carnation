			    			</div>
			    			<div class="goods-price">
			    				<span class="meiy">¥</span><span class="old-price">$!casegoods.goodsCurrentPrice </span>
			    				<span class="tejia"></span>
	    						#if($!casegoods.goodsType==0)
				                	#set($thisPrice=$!{casegoods.goodsPrice})
				                #else
					                #set($thisPrice=$!{casegoods.goodsPrice}-$!{casegoods.selfAddPrice})
				                #end
			    				<span class="huijia">¥$!thisPrice</span>
			    				<!-- 隐藏BV值  -->
			    				<!-- #if($!casegoods.selfAddPrice.equals("0.00") || $!casegoods.selfAddPrice==0)
								#else
	     						<span class="bvzhi" style="display: none;">$!casegoods.selfAddPrice</span>
	     						#end -->
			    			</div>
	    			</div>
					#set($forNu=$forNu+1)
					#if($forNu>1)
						#break
					#end
<!-- 商品详情ed   -->
				#end 
					<!-- <div class="jk-more"><a href="">更多<img src="$!webPath/resources/shop/images/0.png" alt=""></a></div> -->
