				$(".main1").addClass("rightIn");
				loadImg($(".main1 .page img").eq(1));
				loadImg($(".main1 .page img").eq($(".main1 .page img").length-1));
			});
			
			$(".fruitPowder").on("touchstart",function(){
				$(".goHome").show();
				$(".main2").addClass("rightIn");
				loadImg($(".main2 .page img").eq(1));
				loadImg($(".main2 .page img").eq($(".main2 .page img").length-1));
			});
			
			$(".bzjn").on("touchstart",function(){
