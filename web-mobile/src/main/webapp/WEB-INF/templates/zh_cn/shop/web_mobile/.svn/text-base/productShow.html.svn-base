 	<script>
        (function () {
            function setTranslate(node, positionX, useAnimate) {
                var cssText = [
                    'display: block',
                    'transform: translate3d(' + positionX + 'px,0,0)',
                    '-webkit-transform: translate3d(' + positionX + 'px,0,0)',
                ].join(';');
                if (useAnimate) {
                    cssText = [
                        cssText,
                        'transition: .5s transform ease',
                        '-webkit-transition: .5s -webkit-transform ease'
                    ].join(';');
                }
                node.style.cssText = cssText;                
            }
            var jsWriteln=document.getElementById("jsWriteln");
            var main1=document.getElementById("main1");
            main1.removeChild(jsWriteln);
            
            var jsMain6=document.getElementById("jsMain6");
            var main6=document.getElementById("main6");
            main6.removeChild(jsMain6);
            
            var jsMain8=document.getElementById("jsMain8");
            var main8=document.getElementById("main8");
            main8.removeChild(jsMain8);
            
            var isTouchPad=(/hp-tablet/gi).test(navigator.appVersion),
            hasTouch='ontouchstart' in window && !isTouchPad;
            loadImg($(".main1 .page img").eq(0));
            loadImg($(".main2 .page img").eq(0));
            loadImg($(".main3 .page img").eq(0));
            loadImg($(".main4 .page img").eq(0));
            loadImg($(".main5 .page img").eq(0));
            loadImg($(".main6 .page img").eq(0));
            loadImg($(".main7 .page img").eq(0));
            loadImg($(".main8 .page img").eq(0));
            loadImg($(".main9 .page img").eq(0));
			$(".anthocyanin").on("touchstart",function(){
