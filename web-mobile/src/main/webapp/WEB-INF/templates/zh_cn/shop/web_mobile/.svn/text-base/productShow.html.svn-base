				$(".main4").addClass("rightIn");
				loadImg($(".main4 .page img").eq(1));
				loadImg($(".main4 .page img").eq($(".main4 .page img").length-1));
			});
			$(".tdy").on("touchstart",function(){
				$(".goHome").show();
				$(".main6").addClass("rightIn");
				loadImg($(".main6 .page img").eq(1));
				loadImg($(".main6 .page img").eq($(".main6 .page img").length-1));
			});
			$(".lrrjn").on("touchstart",function(){
				$(".goHome").show();
				$(".main7").addClass("rightIn");
				loadImg($(".main7 .page img").eq(1));
				loadImg($(".main7 .page img").eq($(".main7 .page img").length-1));
			});
			$(".ylk").on("touchstart",function(){
				$(".goHome").show();
				$(".main8").addClass("rightIn");
				loadImg($(".main8 .page img").eq(1));
				loadImg($(".main8 .page img").eq($(".main8 .page img").length-1));
			});
			$(".hzqwf").on("touchstart",function(){
				$(".goHome").show();
				$(".main9").addClass("rightIn");
				loadImg($(".main9 .page img").eq(1));
				loadImg($(".main9 .page img").eq($(".main9 .page img").length-1));
			});
			
			$(".goHome").on("touchstart",function(){
				$(this).hide();
				$(".main1,.main2,.main3,.main4,.main5,.main6,.main7,.main8,.main9").removeAttr("style").removeClass("rightIn");
				$(".main1,.main2,.main3,.main4,.main5,.main6,.main7,.main8,.main9").find(".page").removeAttr("style");		
				$(".main1,.main2,.main3,.main4,.main5,.main6,.main7,.main8,.main9").each(function(){
					$(this).find(".page").eq(0).css("z-index","1");						
				});					
			});			
			
            
            function loadImg($imgobj){
				var img=document.createElement("img");
				var dataSrc=$imgobj.attr("data-src");
				img.src=dataSrc;
				if(img.complete){
					$imgobj.attr("src",dataSrc);
				}else{						
					img.onload=function(){						
						$imgobj.attr("src",dataSrc);							
					}
				}
			}

            function slide(node) {
                setTranslate(node, 0, true);
                zIndex += 1;
                node.style.zIndex = zIndex;
                //node.className += " animated";
                $(node).addClass("animated").siblings("div").removeClass("animated");
            }

            
            var pages1 = Array.prototype.slice.call(document.querySelectorAll('.main1 .page')),
            	pages2 = Array.prototype.slice.call(document.querySelectorAll('.main2 .page')),
            	pages3 = Array.prototype.slice.call(document.querySelectorAll('.main3 .page')),
            	pages4 = Array.prototype.slice.call(document.querySelectorAll('.main4 .page')),
            	pages5 = Array.prototype.slice.call(document.querySelectorAll('.main5 .page')),
            	pages6 = Array.prototype.slice.call(document.querySelectorAll('.main6 .page')),
            	pages7 = Array.prototype.slice.call(document.querySelectorAll('.main7 .page')),
            	pages8 = Array.prototype.slice.call(document.querySelectorAll('.main8 .page')),
            	pages9 = Array.prototype.slice.call(document.querySelectorAll('.main9 .page')),
                viewHeight = document.documentElement.clientHeight,
                viewWidth=document.documentElement.clientWidth;

            var zIndex = 1;

            [pages1,pages2,pages3,pages4,pages5,pages6,pages7,pages8,pages9].forEach(function(pages,j){
            	pages.forEach(function (page, i) {
	                var prevPage, nextPage;
	                var loadpre,loadnext;
	                if (i === 0) {
	                    prevPage = pages[pages.length - 1];
	                    nextPage = pages[i + 1];
	                } else if (i === pages.length - 1) {
	                    prevPage = pages[i - 1];
	                    nextPage = pages[0];
	                } else {
	                    prevPage = pages[i - 1];
	                    nextPage = pages[i + 1];
	                }	               
	                touch.on(page, 'swipestart', function (e) {
	                    e.originEvent.preventDefault();
	                    setTranslate(prevPage, -viewWidth);
	                    setTranslate(nextPage, viewWidth);
	                });
	                touch.on(page, 'swipeleft swiperight', function (e) {//去掉swipedown参数
	                    e.originEvent.preventDefault();
	
	                    var toPage = e.direction === 'left' ? nextPage : prevPage;	                    
	                    slide(toPage);
						if($(toPage).prev(".page").length==0){							
							loadImg($(toPage).siblings('.page:last').find("img"));
						}else{
							loadImg($(toPage).prev(".page").find("img"));
						}
						if($(toPage).next(".page").length==0){
							loadImg($(toPage).siblings('.page:first').find("img"));
						}else{
							loadImg($(toPage).next(".page").find("img"));
						}
	                    //location.hash = toPage.id.replace('page', '');
	                });
	            });
            });
            
            //var hashPageIndex = +location.hash.substring(1);
            //if (hashPageIndex > 0 && hashPageIndex < 10) {
            //    $('#bg' + hashPageIndex).attr('src', "images/bg" + hashPageIndex + ".jpg");
            //    slide(document.querySelector('#page' + hashPageIndex));                   
            //}
        }());
    </script>
    <script type="text/javascript">
        //添加类
        var addClass = function (ele, strClass) {
            var reg = new RegExp("(^| )" + strClass + "( |$)");
            if (reg.test(ele.className)) {
                //如果此类样式已经存在，则什么也不需要做
            } else { //不存在
                ele.className = ele.className.trim() + " " + strClass;
            }
        };
        //删除类
        var removeClass = function (ele, strClass) {
            if (!(ele && ele.nodeType == 1)) {
                alert('第一参数ele需要是一个DOM元素对象');
                throw new Error('第一参数ele需要是一个DOM元素对象');
            }
            if (typeof strClass != 'string') {
                alert('第二参数必须为string类型');
                throw new Error('第二参数必须为string类型');
            }
            var reg = new RegExp("(^| )" + strClass + "( |$)", "g");
            ele.className = ele.className.replace(reg, '').trim();
        };

        var audio = document.getElementById('audio');
        var oVoice = document.getElementById("voice");
        var music = document.getElementById("audio");
        function playAudio() {
            // Check for audio element support.
            if (!window.HTMLAudioElement) return;
            try {
                audio.currentTime = 0;
                if (audio.paused) {
                    audio.play();
                    removeClass(oVoice, 'pause');
                    addClass(oVoice, 'play');
                } else {
                    audio.pause();
                    removeClass(oVoice, 'play');
                    addClass(oVoice, 'pause');
                }
            } catch (e) { }
        };
        //播放暂停音乐   
        oVoice.onclick = function () {
            if (music.paused) {
                music.play();
                removeClass(oVoice, 'pause');
             //   addClass(oVoice, 'play');
            } else {
                music.pause();
            //    removeClass(oVoice, 'play');
                addClass(oVoice, 'pause');
            };
        };
        addClass(oVoice, 'play');
        playAudio();
    </script>
