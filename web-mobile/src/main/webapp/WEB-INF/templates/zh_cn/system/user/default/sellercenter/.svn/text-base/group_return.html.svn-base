<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>商家后台-线上服务退款列表- $!config.title - $!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
<meta name="copyright" content="$!{config.copyRight}">
<link href="$!webPath/resources/style/system/front/default/css/public2.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/user.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/common/css/overlay_blue.css" type="text/css" rel="stylesheet" />
<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
<script src="$!webPath/resources/js/jquery.shop.common.js"></script>
</head>
<body>
$!httpInclude.include("/seller/top.htm")
<div class="main">
  <div class="seller_center">
    <div class="seller_center_left"> $!httpInclude.include("/seller/nav.htm?op=group_return") </div>
    <div class="seller_right">
      <div class="seller_position"><span><b><a href="$!webPath/seller/index.htm">商家中心</a></b>><b>客户服务</b>><b>线上服务退款管理</b></span></div>
      <div class="seller_list">
        <div class="seller_list_title">
          <ul class="seller_title_ul">
            <li class="this"><a href="$!webPath/seller/group_return.htm"><i></i>退款列表</a></li>
          </ul>
        </div>
        <div  class="user_sp_right">
          <form action="$!webPath/seller/group_return.htm" method="post" name="ListForm" id="ListForm" >
            <div  class="seller_title_sp"> <span>服务名称</span> <span class="size150">
              <input name="groupName" type="text" id="groupName" value="$!vo.groupName" />
              </span> <span>服务单号</span> <span class="size150">
              <input name="orderId" type="text" id="orderId" value="$!vo.orderId" />
              </span> <span>申请者</span> <span>
              <input name="userNickName" type="text" id="userNickName" value="$!vo.userNickName" />
              </span> <span>
              <input name="" type="submit"  value="搜索" style="cursor:pointer;"  class="seller_sear_btn"/>
              </span> </div>
        </div>
         #if($objs.size()>0)
        <div class="seller_list_box">
            <div class="operation">
              <table width="100%" border="0" cellspacing="0" cellpadding="0" id="opertable" class="user_order_table">
                <tr id="opertitle">
                  <th width="15%">服务单号</th>
                  <th width="10%">服务图片</th>
                  <th width="25%">服务名称</th>
                  <th width="10%">服务价格</th>
                  <th width="20%">消费码</th>
                  <th width="10%">申请者</th>
                  <th width="10%">退单状态</th>
                </tr>
                #foreach($obj in $objs)
                  #set($goods_url="$!webPath/group/view_$!{obj.goodsId}.htm")
                 #set($map = $!groupOrderTools.queryGoodsInfo("$!obj.groupInfo"))
                  #set($goods_price = "$!{map.get('goods_price')}")
                     #set($goods_name = "$!{map.get('goods_name')}")
                  #set($goods_mainphoto_path = "$!{map.get('goods_mainphoto_path')}")
                  #set($img="$!{goods_mainphoto_path}_70*70")
                <tr >
                  <td align="center">$!obj.orderStr</td>
                  <td align="center"><a href="$!goods_url" target="_blank"><img src="$!img" width="70" height="70" /></a></td>
                  <td align="center">$!{goods_name}</td>
                  <td align="center">$!{goods_price}</td>
                  <td align="center">$!obj.groupSn</td>
                  <td align="center">$!obj.userNickName</td>
                  #if($!obj.status==3)
                   #set($status="审核中")
                  #end
                  #if($!obj.status==5)
                   #set($status="审核通过")
                  #end
                   #if($!obj.status==6)
                   #set($status="审核不通过")
                  #end
                   #if($!obj.status==7)
                   #set($status="已退款")
                  #end
                  <td align="center">$!{status}</td> 
                </tr>
                #end
              </table>
            </div>
            <input id="currentPage" name="currentPage" type="hidden" value="$!currentPage"/>
            <div class="seller_list_page">
              <div  class="seller_page_sp"> $!gotoPageFormHTML</div>
            </div>
          </form>
        </div>
        #else
        <table>
          <tr>
            <td colspan="8" align="center"><div class="seller_center_nothing">
                <div class="seller_center_nothing_left"><img src="$!webPath/resources/style/system/front/default/images/seller_center_nothing.png" /></div>
                <div class="seller_center_nothing_right">暂时没有数据</div>
              </div></td>
          </tr>
        </table>
        #end </div>
    </div>
  </div>
</div>
$!httpInclude.include("/footer.htm")
</body>
</html>
