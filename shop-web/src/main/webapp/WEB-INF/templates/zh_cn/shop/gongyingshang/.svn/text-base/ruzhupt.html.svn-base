			function validateSeller(event) {
				var flag=true;
				$.ajax({
			          type: "post",
			          async:false,
			          url: "$!webPath/seller_apply/validateUserSellerType.htm",
			          dataType: "json",
			          success: function (data) {
			        	 
			        	  if (data == 1) {//020
			        		  if(event != 1) {
			        			  alertBoxFn('提示','很抱歉，您已是商户身份，无法同时入驻供应商哦！有疑问可拨打客服电话400-888-2933',1);
			        			  flag=false;
			        		  } 
			        		  
			        	  }
			        	  if (data==2 ) {//shop
			        		  if(event != 2) {
			        			  alertBoxFn('提示','很抱歉，您已是供应商身份，无法同时入驻商户哦！有疑问可拨打客服电话400-888-2933',1);
			        			  flag=false;
			        		  }
			        	  }
			                 
			          },
			          error: function (XMLHttpRequest, textStatus, errorThrown) {
			                  alert(errorThrown);
			          }
	      		});
				if(flag){
					if(event==1){
						location.href="$!webPath/seller_apply/group.htm";
					}else{
						location.href="$!webPath/seller/ruzhu_xuzhi.htm";
					}
				}
			}
			
			
			


			
			
