					<script>
    jQuery(document).ready(function(){
   	var order_goods_tabone=document.getElementById("order_goods_tabone");
   	if(order_goods_tabone.firstChild.nodeName=="#text"){
   		order_goods_tabone.firstChild.remove();
   	}
	jQuery("select[id^=coupon_id_]").val("");								
	jQuery(".result_count_ul li b").click(function(){
	var display = jQuery(this).parent().find(".result_coupon").css("display");
	if(display=="none"){
		jQuery(this).parent().parent().find("img").attr("src","$!webPath/resources/style/system/front/default/images/result_cut.jpg");
		jQuery(this).parent().find(".result_coupon").css("display","block");
		}else{
		jQuery(this).parent().parent().find("img").attr("src","$!webPath/resources/style/system/front/default/images/result_add.jpg");
		jQuery(this).parent().find(".result_coupon").css("display","none");
			}
	});	
	
	
	
	  jQuery("select[id^='coupon_id_']").change(function (){
		  calculatedOrderTotal(jQuery(this).attr("mark"));
	  });
	  
		$("input[id^='integral_']").click(function (){
	   		var integrals=$("input[id^='integral_']:checked");
	   		var useIntegral=0;
	   		integrals.each(function (i,n){
	   			useIntegral=useIntegral+parseInt($(this).val()) * parseInt($(this).attr("count"));
	   		});
	   		
	   		var totalIntegral=parseInt($("#total_integral").text() || 0) ;
	   		if(totalIntegral < useIntegral){
	   			alert("您的积分不够!");
	   			return false;
	   		}
	   		
	   		//计算店铺金额
	   		calculatedStoreTotal($(this).attr("store"));
	   		//计算订单金额
	   		calculatedOrderTotal($(this).attr("store"));
	   		
	   		$("#use_integral").html(" - "+useIntegral + " 剩余：" +(totalIntegral-useIntegral));
	   	});
	});
    
   	//计算店铺总价
   	function calculatedStoreTotal(store){
   		var prices=$("span[id^='goods_price_'][store='"+store+"']");
   		var total=0.00;
   		prices.each(function (i,n){
   			var price=$(this);
   			var cartPrice=calculatedCartTotal(price);
   			total=total+cartPrice;
   		});
   		total=total.toFixed(2);
   		$("#store_total_price_"+store).text(total);
   		$("#store_total_price_"+store).attr("price",total);
   	}
   	
   	//计算商品总价
   	function calculatedCartTotal(price,type){
   		var count=parseInt(price.attr("count"));
   		var isIntegral=$("#integral_"+price.attr("gcId")+":checked");
   		var cartTotal;
   		if(isIntegral.length > 0){
   			cartPrice= count * parseFloat(price.attr("integralPrice"));
   			price.text(parseFloat(price.attr("integralPrice")));
   		}else{
   			cartPrice= count * parseFloat(price.attr("price"));
   			price.text(parseFloat(price.attr("price")));
   		}
   		$("#goods_total_price_"+price.attr("gcId")).text(cartPrice.toFixed(2));
   		return cartPrice;
   	}
  	//计算定单总价
    function calculatedOrderTotal(mark){
		 var coupon_amount=parseFloat(jQuery("#coupon_id_"+mark).find("option:selected").attr("coupon_amount"));
		 if(isNaN(coupon_amount)){
			coupon_amount=0;
		 }//优惠券金额
		   var ship_price=parseFloat(jQuery("#ship_price_"+mark).val());
	      if(isNaN(ship_price)){
		   ship_price=0; 
	      }//运费
		  
			var text=jQuery("#ship_price_"+mark).find("option:selected").text();
			var transport=text.substring(0,text.indexOf("["));
			jQuery("#transport_"+mark).val(transport);  //设置物流
		  
		  var store_goods_price=parseFloat(jQuery("#store_total_price_"+mark).attr("price"));
	      if(isNaN(store_goods_price)){
		   store_goods_price=0; 
	      }//店铺总价格
		  var store_total_price =store_goods_price+ship_price-coupon_amount;
		  jQuery("#store_total_price_"+mark).html(store_total_price);//设置单个店铺总价格
		 jQuery("#result_coupon_note_"+mark).html("优惠"+coupon_amount+"元");//设置优惠券优惠信息
		/*  var order_ship_price =0.00;
		 jQuery("select[id^=ship_price_]").each(function(){
		  jQuery(this).parent().find("strong").html(jQuery(this).val());											 
		  order_ship_price = order_ship_price + parseFloat(jQuery(this).val());
		  
		  });
		  jQuery("#bottom_ship_price").html(order_ship_price);//设置底部所有运费 */
		  
		 var bottom_coupon_price =0.0;
		  jQuery("select[id^='coupon_id_']").each(function(){//计算订单优惠券总计												 
		  bottom_coupon_price = bottom_coupon_price + parseFloat(jQuery(this).find("option:selected").attr("coupon_amount"));
		  });
		 jQuery("#bottom_coupon_price").html(bottom_coupon_price);
		 
		 var order_total_price =0.0;
		  jQuery("b[id^=store_total_price_]").each(function(){//计算订单总费用											
		  order_total_price = order_total_price + parseFloat(jQuery(this).html());
		  });
		 var sc_total_price =0.0;
		  jQuery("span[id^='goods_total_price_']").each(function(){//计算商品总价										
			  sc_total_price = sc_total_price + parseFloat(jQuery(this).html());
		  });
		 jQuery("#sc_total_price").html("¥"+sc_total_price);
		 jQuery("#order_pay_fee").html("¥"+order_total_price);
	  }

	#set($freeShipAmount = 2000)
	
  	//精选专区选择支付方式
  	$(function(){
  		$(".choose_pay_jx .choose .type").click(function(){
  			var len=$(".choose_pay_jx .choose .readonly").length;
  			if(len>=2){
  				alertBoxFn('提示','很抱歉，您的账户余额不足|请先充值',1);
  				return;
  			}else{
  				if($(this).hasClass("readonly")) return;
  				$(".submit_info .price .content").show();
  				$(this).addClass("curr").siblings().removeClass("curr");
  				var payvalue=$(this).find(".payvalue").html();
  				$("#paytype_name").html($(this).attr("data-text"));
  				$("#pay_value").html(payvalue);
  				/* var fn_rmb  = $(".goods_total_price .fn-rmb").find('span').html();
  				var ml4 = $(".goods_total_freight .ml4").html();
  				$("#cashPayNum").html(parseFloat(fn-rmb )+parseFloat(ml4) ); */
  				 $("#cashPayNum").html(Number($(this).attr("data-cash-pay")).toFixed(2)); 
  				$("#jx_paytype").val($(this).attr("data-paytype"));
  				$(".jx_submit_btn").addClass("curr");
  				if($(this).attr('data-paytype')=='1'){
  					$("#text_yf").html('含运费');
  				}else{
  					$("#text_yf").html('含运费');	
  				}
  			}
  		});
  		
  		
  	})
    
    </script>
					#if($!goodsBoFlag && $!goodsBoFlag.isYgb ==1)
					
