    $("input[name='nocheck']").each(function(){//循环绑定事件  
        if(this.checked){  
            old = this; //如果当前对象选中，保存该对象  
        }  
        this.onclick = function(){  
            if(this == old){//如果点击的对象原来是选中的，取消选中  
                this.checked = false;  
                old = null;  
            } else{  
                old = this;  
            }  
        }  
    });  
	}); 

	
	</script>
	
</body>

</html>
