		 </div>
		 #set($num = 0)
	#foreach($floor in $floorList)
		 #set($num = $num+1)
	     <div id="float0$num" class="ppg-float">
	     	<!-- 健康美食 -->
	     	<div id="jkshq-tab">
	     		<ul class="jkshq-tab_menu">
			        <span class="shg-tit-jkms"><img src="$!webPath/resources/shop/ysysgo/images/shg-tit-jkms.png" alt="$!floor.gfName" />$!floor.gfName</span>
			    </ul>
			    <div class="jkshq-tab_box">
			        <div class="fn">
			        	<!-- 推荐的内容 -->
			        	<ul>
			        	#set($groupNum = 0)
			        		#set($groupList = $groupViewTools.caseGroup($city,$num))
       		            	#foreach($group in $groupList)
			        			#set($groupNum = $groupNum+1)
								<li>
				        			<a href="$webPath/group/view_$!{group.id}.htm" target="blank">
				        				<img class="gos-img" src="$!{group.groupAccPath}_269x180" alt="" />
				        				<div class="shg-goods-tit">$!CommUtil.substring("$!group.ggName",12)  </div>
				        				<div class="shg-goods-price">
				        					<span class="yishou">已售$!group.selledCount</span>
				        					<span class="price">¥ $!group.groupPrice</span>
				        				</div>
				        				#set($storeScore = $groupViewTools.storeScore($!group.storeId))
				        				<div class="pingjia">
				        					<span class="pj-right">
				        						#if(!$!storeScore)
				        							#set($rate = 100)
									            	<div><div class="five_star"><b title="好评率：${rate}%" style="width:${rate}%;"></b></div></div>
				        						#else
					        						#set($rate = $groupViewTools.storeGoodScore($!storeScore)*100/5)
										            <div><div class="five_star"><b title="好评率：${rate}%" style="width:${rate}%;"></b></div></div>
									            #end
				        						<!-- <img src="$!webPath/resources/shop/ysysgo/images/shg-pj-dj.png" alt="" /> -->
				        						#if($!storeScore.num)$!storeScore.num #else 0 #end 评价
				        					</span>
				        						#set($store = $groupViewTools.storeInfo($!group.storeId))
				        					<span class="diqu">$!store.areaInfo.split("市")[1]</span>
				        				</div>
				        			</a>
				        		</li>
			        			#if($groupNum>5)
			        				#break
			        			#end
			        		#end
			        	</ul>
			        </div>
			    </div>
	     	</div>
	     	
	     	<div class="shq-tit-right">
	     		<div class="right-title">商家推荐</div>
	     		<div class="right-goods">
	     			<ul>
	     				#if($!floor.stores)
		     				#foreach($store in $floor.stores)
								#if($!store.storeInfoPath)
									#set($img=$store.storeInfoPath + "")
								#else
									#set($img=$config.storeImage.path)
								#end
			     				<li>
			     					<a href="$webPath/group/store.htm?id=$!store.id">
			     						<img src="$!{img}_100x100" alt="" />
			     						<div class="right-goods-r-mes">
			     							<div class="r-tit">$!CommUtil.substring("$!store.storeName",8)</div>
			     							<div class="zhegou">
			     							#if($store.groupDiscount)
									            #set($!store.groupDiscount = $store.groupDiscount*100)
									            #if($math.mod($!store.groupDiscount,10) == 0)
									            	<span class="zhe">折</span><span class="zheprice">$number.format("#0",$math.div($!store.groupDiscount,10))折</span>
									            #else
									            	<span class="zhe">折</span><span class="zheprice">$number.format("#0.0",$math.div($!store.groupDiscount,10))折</span>  
									            #end
								            #end
			     							</div>
			     						</div>
			     					</a>
			     				</li>
		     				#end
		     			#end
	     			</ul>
	     		</div>
	     	</div>
	     </div>
	     #if($num>3)
	     	#break
	     #end
	   #end
    </div>
     <div class="floatCtro" style="display: none;">
