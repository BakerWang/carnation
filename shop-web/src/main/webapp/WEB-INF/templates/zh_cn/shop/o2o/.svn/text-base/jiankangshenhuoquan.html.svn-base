	 <script src="$!webPath/resources/js/jquery.SuperSlide.2.1.1.js"></script>
<script>

   $(function(){

	   $('#jkshq-tab ul li.uli').click(function() {
			$(this).addClass('selected').siblings().removeClass('selected');
			var index = $('#jkshq-tab ul li.uli').index(this);
			$('div.jkshq-tab_box > div').eq(index).show().siblings().hide();
		});	
		$("#jkshq-tab ol li.genduo").click(function() {
			$(this).removeClass('selected');
		});
   		
		
		$('.jkshq-top ul li').hover(function() {
			$(this).addClass('jkshq-img-crr');
		}, function() {
			$(this).removeClass('jkshq-img-crr');
		});
		
		var processor = {  
			    timeoutId: null, 		 
			    performProcessing: function(){
			    	scrolls();
			    },  		 
			    process: function(){
			        clearTimeout(this.timeoutId);  
			        this.timeoutId  = setTimeout(function(){ 
			            processor.performProcessing();  
			        }, 100);  
			    }  
			};
		
		/*楼层*/
	   var AllHet = $(window).height();
	   var mainHet= $('.floatCtro').height();
	   var fixedTop = (AllHet - mainHet)/2;
	
	    $('div.floatCtro p').click(function(){
			var i = $('div.floatCtro p').index(this);
		 	var topVal = $('#float0'+parseInt(i+1)).offset().top;
		 	$(this).addClass('cur').siblings().removeClass('cur');
			$('body,html').animate({scrollTop:topVal},1000);	 
		});
	   
	    $(window).on("scroll",function(){
			processor.process();
		});

	   function scrolls(){
		  	var sTop = $(window).scrollTop();
		   	var divs=$('[id^="float"]');
		   	for(var i=0;i<divs.length;i++){

				var f=$('#float0'+parseInt(i+1)).offset().top;
			   	 if(sTop>=f-100){
			   	 	$('div.floatCtro p').eq(i).addClass('cur').siblings().removeClass('cur');
			   	 }
		   	}
		 	if((document.body.scrollTop||document.documentElement.scrollTop)>600){
		   		$(".floatCtro").show();
		   		$("#Fixed").show();
		   	}else{
		   		$(".floatCtro").hide();
		   		$("#Fixed").hide();
		   	}	
	   }	   	
			   	
   });
   

</script>
</body>
</html>
