				  success:function(data){
					  if(data.ret=="true"){
						  clearInterval();
						   window.location.href="$!webPath/qr_login_confirm.htm?user_id="+data.user_id+"&qr_log_mark="+data.qr_log_mark;						   
					  }
				  }
		});
	}

