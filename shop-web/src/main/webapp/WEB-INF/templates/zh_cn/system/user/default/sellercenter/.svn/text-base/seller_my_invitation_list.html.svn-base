$!httpInclude.include("/seller/top.htm")
<div class="main">
  <div class="seller_center"> 
  $!httpInclude.include("/seller/nav.htm?op=seller_my_invitationCode")
    <div class="seller_right">
      <div class="seller_position"> <span> <b><a href="$!webPath/seller/index.htm">商家中心</a></b>> <b>邀请明细</b></span> </div>
      
      <div class="seller_list">
       <div class="seller_list_title">
	      <ul class="seller_title_ul">
	        <li ><a href="$!webPath/seller/my_invitationInfo.htm">我的专属码</a></li>
	        <li  class="this"><a href="$!webPath/seller/my_invitation_list.htm">汇总明细</a></li>
	      </ul>
	    </div>
      </div>
      
     <div class="user_sp_right" style="border-bottom:none;">
	      <div class="common-container">
	        <div class="inviteList">
	            <ul>
	            
	            <form method="post" name="ListForm" id="ListForm" action="$!webPath/seller/my_invitation_list.htm">
	              #if($!objs.size()>0)
	             	 #foreach($obj in $objs)
		              	  <li>
		                    <div class="content">
		                        <div class="addtime">注册时间：&nbsp;$!date.format('yyyy-MM-dd',$!obj.registerTime)</div>
		                        <div class="basic">
		                        
	                        	  #if($!userCustomer.headImgPath)
					               <span class="headimg"><img src="$!userCustomer.headImgPath" /></span>
					              #else
					             	<span class="headimg"><img src="$!webPath/resources/style/shop/images/store_invitationInfo/headimg.png" /></span>
					              #end 
						              
		                            <div class="nameCode">
		                                <span class="nickname">$!obj.nickname</span>
		                                <span class="code">手机号：<span class="inviteCode">$!obj.mobile</span></span>
		                            </div>
		                        </div>
		                    </div>
		                </li>
		              #end
	              #else
	              	<span>暂无汇总明细数据</span>
	              #end
	            
	                #if($!objs.size()>0)
				          <div class="userlist_allop">
				            <div class="seller_list_page">
				              <div  class="seller_page_sp">
				                <input name="currentPage" type="hidden" id="currentPage" value="$!currentPage" />
				              
				                $!gotoPageFormHTML</div>
				            
				            </div>
				          </div>
				          
				          
		          #end
	            </form>
	            </ul>
	        </div>
	    </div>
    </div>  
    
    </div>
    
    <input  type="hidden" id="appShareInfoId" value="$!userAccount.appShareInfo" />
     <input  type="hidden" id="invitationCodeId" value="$!user.invitationCode" />
