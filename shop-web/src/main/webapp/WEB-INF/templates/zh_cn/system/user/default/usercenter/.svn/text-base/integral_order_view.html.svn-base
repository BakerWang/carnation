<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>积分兑换详情 - $!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
<meta name="copyright" content="$!{config.copyRight}">
<link href="$!webPath/resources/style/system/front/default/css/public2.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/public_auto.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/user.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/common/css/overlay.css" type="text/css" rel="stylesheet" />
<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
<script src="$!webPath/resources/js/jquery-ui-1.8.21.js"></script>
<script src="$!webPath/resources/js/jquery.shop.common.js"></script>
<script>
jQuery(document).ready(function(e) {
    #if($!query_ship)
	jQuery.ajax({
		type:'POST',
  		url: "$!webPath/buyer/integral_ship_ajax.htm",
		data:{"order_id":"$!{obj.id}"},
		beforeSend:function(){
					 jQuery("#integral_ship").html("<div style='text-align:center;'> <img src='$!webPath/resources/style/common/images/loader.gif'/>	</div>");
				  },
  		success: function(data){
   		 jQuery("#integral_ship").html(data);
  	    }
	});
	#end
});
</script>
</head>
<body>
$!httpInclude.include("/top.htm")
$!httpInclude.include("/head.htm")
$!httpInclude.include("/nav1.htm")
<div class="main clearfix">
  <div class="usercenter">
  <div class="usercenter_left">
  $!httpInclude.include("/buyer/nav.htm?op=integral")  
  </div>
    <div class="usercenter_right">
      <div class="myorder">
        <h4><strong>积分兑换</strong></h4>
      </div>
      <div class="user_list">
        <div class="user_list_title">
          <ul class="user_title_ul">
          	<li><i></i><a href="$!webPath/buyer/integral_order_list.htm">兑换记录</a></li>
            <li class="this"><i></i><a href="javascript:void(0)">兑换详情</a></li>
          </ul>
        </div>
        <div class="user_list_box">
          <div class="complaint_one">
            <h1 class="complaint_h">兑换信息</h1>
            <div class="comp_goodetails">
              <div class="comp_goodl">
                  <ul class="comp_gdul">
                    <li> 兑换单号：<strong style="color:orange">$!{obj.igoOrderSn}</strong></li>
                    #if($!obj.igoStatus==-1)
                    #set($status="已取消")
                    #end
                    #if($!obj.igoStatus==0)
                    #set($status="待付款")
                    #end
                    #if($!obj.igoStatus==20)
                    #set($status="已付款，待发货")
                    #end
                    #if($!obj.igoStatus==30)
                    #set($status="已发货")        
                    #end                
                    #if($!obj.igoStatus==40)
                    #set($status="已收货完成")
                    #end 
                    <li> 状态：<b>$!status</b> </li>
                    <li> 兑换积分：$!{obj.igoTotalIntegral}</li>
                    <li> 运费：$!{obj.igoTransFee}</li>
                    <li> 兑换时间：$!CommUtil.formatShortDate($!obj.addTime)</li>
                  </ul>
              </div>
            </div>
          </div>
          <div class="complaint_one">
            <h1 class="complaint_h">会员信息</h1>
            <div class="comp_goodetails">
              <div class="comp_goodl">
                  <ul class="comp_gdul">
                    <li> 会员名称：<strong>$!{obj.userCustomer.nickname}</strong></li>
                    <li> 会员等级：$!integralViewTools.query_user_level_name("$!obj.igoUserId") </li>
                    <li> 会员Email：$!{obj.userCustomer.email}</li>
                    <li> 兑换留言：#if($!{obj.igoMsg}!="")<em>$!{obj.igoMsg}</em>#else无#end</li>
                  </ul>
              </div>    
            </div>
          </div>
          <div class="complaint_one">
            <h1 class="complaint_h">支付信息</h1>
            <div class="comp_goodetails">
            	<div class="comp_goodl">
                  <ul class="comp_gdul">
                    #if($!obj.igoPayment=="alipay")
                        #set($payment="支付宝")
                        #end
                        #if($!obj.igoPayment=="alipay_wap")
                        #set($payment="手机网页支付宝")
                        #end
                        #if($!obj.igoPayment=="alipay_app")
                        #set($payment="手机支付宝APP")
                        #end
                    #if($!obj.igoPayment=="tenpay")
                    #set($payment="财付通")
                    #end
                    #if($!obj.igoPayment=="bill")
                    #set($payment="快钱")
                    #end                 
                    #if($!obj.igoPayment=="chinabank")
                    #set($payment="网银在线")
                    #end              
                    #if($!obj.igoPayment=="balance")
                    #set($payment="云豆支付")
                    #end 
                    #if($!obj.igoPayment=="no_fee")
                    #set($payment="无运费订单")
                    #end 
                    #if($!obj.igoPayment=="wx_pay")
                         #set($payment="网页微信支付")
                        #end
                        #if($!obj.igoPayment=="wx_app")
                         #set($!payment="app微信支付")
                        #end
                    #if(!$!obj.igoPayment)
                    #set($payment="未支付")
                    #end
                    <li> 支付方式：$!payment</li>
                    <li> 支付时间：$!CommUtil.formatLongDate($!obj.igoPayTime)</li>
                    <li> 支付留言：#if($!obj.igoPayMsg!="")<em>$!obj.igoPayMsg</em>#else无#end</li>
                  </ul>
                 </div> 
            </div>
          </div>
          <div class="complaint_one">
            <h1 class="complaint_h">收货人信息</h1>
            <div class="comp_goodetails">
              <div class="comp_goodl">
                  <ul class="comp_gdul">
                    <li> 收货人：<strong>$!{obj.receiverName}</strong></li>
                    <li> 所在地区：$!obj.receiverArea </li>
                    <li> 邮政编码：$!{obj.receiverZip}</li>
                    <li> 详细地址：$!{obj.receiverAreaInfo}</li>
                    <li> 电话号码：#if($!{obj.receiverTelephone}!="")$!{obj.receiverTelephone}#else未填写#end</li>
                    <li> 手机号码：#if($!{obj.receiverMobile}!="")$!{obj.receiverMobile}#else未填写#end</li>
                  </ul>
              </div>    
            </div>
          </div>
          #if($!{obj.igoShipCode})
          <div class="complaint_one">
            <h1 class="complaint_h">物流信息</h1>
            <div class="comp_goodetails">
              <div class="comp_goodl">
                  <ul class="comp_gdul">
                    <li> 物流公司：$!orderFormTools.queryExInfo($!obj.igoExpressInfo, "express_company_name")</li>
                    <li> 物流单号：<strong>$!{obj.igoShipCode}</strong></li>
                    <li> 发货时间：$!CommUtil.formatShortDate($!{obj.igoShipTime})</li>
                    <li> 发货说明：#if($!{obj.igoShipContent}!="")<em>$!{obj.igoShipContent}</em>#else无#end</li>
                  </ul>
              </div>    
            </div>
          </div>
          <div class="complaint_one">
            <h1 class="complaint_h">物流跟踪</h1>
            <div class="ordercon">
            <div class="exchange_w" id="integral_ship">
              
            </div>
          </div>
          </div>
		  #end
          <div class="complaint_one">
            <h1 class="complaint_h">礼品信息</h1>  
  	        <div class="ordercon">
            <div class="exchange_w">
              <table border="0" cellspacing="0" cellpadding="0" class="exchane_table">
                <tr>
                  <td width="100">&nbsp;</td>
                  <td width="330">礼品名称</td>
                  <td width="130" align="center">兑换积分</td>
                  <td width="130" align="center">兑换运费</td>
                  <td align="center">兑换数量</td>
                </tr>
                #set($igo_gcs=$!orderFormTools.query_integral_goodsinfo("$!obj.goodsInfo"))
                #foreach($gc in $igo_gcs)
                <tr class="extable_trlist">
                  <td align="center" valign="middle"><img src="$!{gc.ig_goods_img}" width="60"  height="60"/></td>
                  <td align="left" valign="middle"><a href="$!webPath/integral/view_$!{gc.id}.htm" target="_blank" class="blue2">$!gc.ig_goods_name</a></td>
                  <td align="center">$!gc.ig_goods_integral</td>
                  <td align="center">¥$!CommUtil.null2Double($!gc.ig_goods_tran_fee)</td>
                  <td align="center">$!gc.ig_goods_count</td>
                </tr>
                #end
              </table>
            </div>
          </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

$!httpInclude.include("/footer.htm")
</body>
</html>
