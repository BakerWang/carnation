<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>商家后台-线上服务订单详情-$!config.title - $!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
<meta name="copyright" content="$!{config.copyRight}">

<link href="$!webPath/resources/style/system/front/default/css/public.css?v=2" type="text/css" rel="stylesheet"/>
    <link href="$!webPath/resources/style/common/css/overlay.css" type="text/css" rel="stylesheet"/>
    <link href="$!webPath/resources/style/system/front/default/css/user.css" type="text/css"
          rel="stylesheet"/>
    <link href="$!webPath/resources/style/service/public.css" type="text/css" rel="stylesheet"/>
    <script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
    <script src="$!webPath/resources/js/jquery.shop.common.js"></script>

</head>
<body>
$!httpInclude.include("/seller/top.htm")
<div class="main clearfix">
  <div class="seller_center">
    <div class="seller_center_left">  $!httpInclude.include("/seller/nav.htm?op=group_order") </div>
    <div class="seller_right">
      <div class="seller_position"><span><b><a href="$!webPath/seller/index.htm">商家中心</a></b>><b>服务交易管理</b>><b>订单详情</b></span></div>
      <div class="seller_list">
        <div class="seller_list_title">
          <ul class="seller_title_ul">
             <li><a href="$!webPath/seller/group_order.htm?statusCode=all">所有订单</a></li>
             <li><a href="$!webPath/seller/group_order.htm?statusCode=order_submit">待付款</a></li>
             <li><a href="$!webPath/seller/group_order.htm?statusCode=order_finish">已消费</a></li>
             <li><a href="$!webPath/seller/group_order.htm?statusCode=order_pay">未消费</a></li>
             <li><a href="$!webPath/seller/group_order.htm?statusCode=order_cancel">已取消</a></li>
              <li class="this"><a href="javascript:void(0);">订单详情</a></li>
          </ul>
        </div>
        <div class="add_usercenter"> 
        
        
        <div class="order_pay">
        <div class="order_pay_note">
        #if($!obj.orderStatus==0)
          #set($status="已取消")
          #end
          #if($!obj.orderStatus==10)
          #set($status="未付款")
          #end
          #if($!obj.orderStatus==20)
          #set($status="未消费")
          #end
          #if($!obj.orderStatus>=30)
          #set($status="已消费")        
          #end 
          #set($payment=$PaymentConstants.getPaymentName($!obj.paymentMark))
         
            <h3><span>当前订单状态：<b style="color:#f60;">$!status</b> </span></h3>
        </div>
    </div>

    <div class="order-view-box">
        <div class="order-view-title">订单信息</div>
        <div class="order-view-cont">
            <div class="col-md-3">订单编号：$!obj.orderId </div>
            <div class="col-md-6">下单时间：$!CommUtil.formatLongDate($!obj.addTime)</div>
            <div class="col-md-3">付款方式：$!payment  </div>
            <div class="col-md-6">付款时间：$!CommUtil.formatLongDate($!obj.payTime)</div>
            <div class="col-md-6">订单金额：¥$!obj.totalPrice</div>
        </div>
    </div>
	#set($map = $!groupOrderTools.queryGoodsInfo("$!obj.groupInfo"))
                  #set($goods_id = "$!{map.get('goods_id')}")	
                  #set($goods_name = "$!{map.get('goods_name')}")
                  #set($goods_price = "$!{map.get('goods_price')}")
                  #set($goods_mainphoto_path = "$!{map.get('goods_mainphoto_path')}")
                  #set($img="$!{goods_mainphoto_path}_108*78")
                  #set($goods_url="$!webPath/group/view_$!{goods_id}.htm")
    <div class="order-view-box">
        <div class="order-view-title">购买信息</div>
        <div class="order-view-cont">
            <div class="col-md-3">消费者：$!obj.userNickname</div>
            <div class="col-md-3">手机号码：$!obj.userCustomer.mobile</div>

            <table width="100%" cellspacing="0" cellpadding="0" border="0" class="user_order_table order_view_table">
                <tr>
                    <th align="left" width="15%">购买项目</th>
                  <!--   <th width="10%">消费码</th> -->
                     <th width="15%">退款申请时间</th>
                     <th width="15%">平台审核时间</th>
                     <th width="20%">退款理由</th>
                    <th width="10%">状态</th>
                    <th width="10%">支付金额</th>
                </tr>
                  #foreach($group in $!obj.groupInfoBOList)
                <tr>
                    <td>
                        <a class="order_img_a" target="_blank"
                           href="$!goods_url" >
                            <img width="108" height="78" 
                                 src="$!img">
                        </a>
                        <div class="order_buy_data">
                            <a href="$!goods_url" class="title ell"  target="_blank"> $!{goods_name}</a>
                            <!--  
                            <div class="c_gray">有效期至：2015-08-08</div>
                            <a href="#" class="c_blue"> 商家信息</a>-->
                        </div>
                    </td>
               <!--      <td align="center">
                       $!group.groupSn
                    </td> -->
                     <td align="center">
                       $!CommUtil.formatLongDate($!group.refundCompleteTime)
                    </td>
                     <td align="center">
                      $!CommUtil.formatLongDate($!group.checkTime)
                    </td>
                     <td align="center" title="$!group.refundReasion">
                     <div style='overflow:hidden;text-overflow:ellipsis;white-space:nowrap;width:100px;'>
				        $!group.refundReasion
				     </div>
                      
                    </td>
                      #if($!group.status==0)
         			  #set($status="未消费")
          			  #end
			          #if($!group.status==1)
			          #set($status="已消费")
			          #end
			          #if($!group.status==-1)
			          #set($status="已过期")
			          #end
			          #if($!group.status==3)
			          #set($status="退款审核中")
			          #end
			          #if($!group.status==5)
			          #set($status="退款审核通过")        
			          #end 
			           #if($!group.status==6)
			          #set($status="退款审核不通过")        
			          #end 
			          #if($!group.status==7)
         			  #set($status="退款完成")
          			  #end
                    <td align="center">
                        $!status
                    </td>
                    <td align="center">
                        <span class="c_blue">¥$!{goods_price}</span>
                    </td>
                </tr>
                #end
            </table>
        </div>
    </div>
        
        
        
        </div>
      </div>
    </div>
  </div>
</div>
$!httpInclude.include("/footer.htm")
</body>
</html>
