                    <span class="order_sp_td">
                      <ul class="order_list">
                        #if($!obj.orderStatus==65)
                        <li>
                          <a href="javascript:void(0);">已结束,不可评价</a>
                        </li>
                        #end
                    #if($!obj.orderStatus==10&&!$!obj.isygOrder)
                        <li>
                          #if($!obj.orderCat==1)
                          <a href="$!webPath/recharge_pay.htm?id=$!obj.id" class="order_button orange" target="_blank">充值</a>
                          #else
                          <a href="$!webPath/order_pay_view.htm?id=$!obj.id" class="order_button orange" target="_blank">付款</a>
                          #end
                        </li>
                        #end
                    #if(($!obj.orderStatus==10||$!obj.orderStatus==18)&&$!obj.isygOrder)
                       
                        #if($!obj.payTypeFlag==2)
                         <li>                      
                          <a href="$!webPath/to_beauty_pay.htm?orderId=$!obj.id" class="order_button orange" target="_blank">付款</a>
                         </li> 
                        #end
                        
                        #if($!obj.payTypeFlag==1)
                         <li>                      
                          <a href="$!webPath/to_yugou_order_pay.htm?orderId=$!obj.id" class="order_button orange" target="_blank">付款</a>
                         </li> 
                        #end
                        
                          #if($!obj.payTypeFlag==3)
                         <li>                      
                          <a href="$!webPath/to_jingxuan_pay.htm?orderId=$!obj.id" class="order_button orange" target="_blank">付款</a>
                         </li> 
                        #end
                        
                        
                        #end       
                    #set($orderStatus=$!orderFormTools.query_orderStatus("$!obj.id"))
                    #if($!obj.orderStatus==30&&$!obj.deliveryType==0)
                        <li>
                          <a  href="javascript:void(0);" dialog_uri="$!webPath/buyer/order_cofirm.htm?id=$!obj.id&currentPage=$!currentPage" dialog_title="确认收货" dialog_width="400" dialog_height="100" dialog_id="order_cancel" class="order_button orange">确认收货</a>
                        </li>
                        #end
                    #if($!orderStatus==35)
                        <li>
                          <a  href="javascript:void(0);" dialog_uri="$!webPath/buyer/order_cofirm.htm?id=$!obj.id&currentPage=$!currentPage" dialog_title="确认收货" dialog_width="400" dialog_height="100" dialog_id="order_cancel" class="order_button orange">确认收货</a>
                        </li>
                        #end
                    #if($!orderStatus>=30)
                    #set($flag=0)
                    #foreach($gc in $obj.gcs)
                    #if($gc.goods.goods_choice_type==1)
                    #set($flag=1)
                    #end
                    #end
                    #if($!flag==1)
                        <li>
                          <a href="javascript:void(0);" dialog_uri="$!webPath/buyer/order_seller_intro.htm?id=$!obj.id" dialog_title="虚拟商品" dialog_width="400" dialog_height="100" dialog_id="order_cancel">虚拟商品</a>
                        </li>
                        #end
                    #end
                    #if($!obj.orderStatus>=40 && $!obj.orderStatus!=47 && $!obj.orderStatus!=50 && $!obj.orderStatus!=60 && $!obj.orderStatus!=65)
                        <li>
                          <a href="$!webPath/buyer/order_evaluate.htm?id=$!obj.id" class="order_button orange">我要评价</a>
                        </li>
                        #end
                      </ul>
                    </span>
                  </td>
                </tr>
              </table>
              <div class="orderlist_bottom">
                <span>
	                #if($!obj.orderStatus==0)
	                #if ($!obj.juanpiOrder==1)
	                	#if ($!obj.isDelete)
  							<a href="javascript:void(0);" order_id="$!obj.id"  juanpi="1" id="delete_order_$!obj.id" class="orderlist_oper_del">删除订单</a>
  						#else
  					
  						#end
	                #else
	                 	<a href="javascript:void(0);" order_id="$!obj.id"  juanpi="0" id="delete_order_$!obj.id" class="orderlist_oper_del">删除订单</a>
	                #end
	                #end #if($!obj.orderStatus>=10 && $!obj.orderStatus
	                <20) <a href="javascript:void(0);" class="orderlist_oper_cancel" dialog_uri="$!webPath/buyer/order_cancel.htm?id=$!obj.id&currentPage=$!currentPage" dialog_title="取消订单" dialog_width="400" dialog_height="100" dialog_id="order_cancel">取消订单</a>
	                #end
	                 #if ($!obj.juanpiOrder==1)
	                   <a href="https://user.juanpi.com/order/detail/orderId/$!obj.orderId" class="order_bottom_btn" target="_blank"/>  查看订单</a>
	                #else
	                <a href="$!webPath/buyer/order_view.htm?id=$!obj.id" class="order_bottom_btn" target="_blank"/>  查看订单</a>
	                #end
              </span>
            </div>
          </div>
          #end

            <div class="fenye">
              <span class="user_page_sp">
                <input name="currentPage" type="hidden" id="currentPage" value="$!currentPage" />
                $!gotoPageFormHTML
              </span>
            </div>

          #else
          <div class="nothing">
            <div class="nothing_left">
              <img src="$!webPath/resources/style/system/front/default/images/nothing.png" />
            </div>
            <div class="nothing_right">
              您目前还没有进行中的订单！这就去挑选商品。
              <a href="$!webPath/index.htm" target="_blank">商城首页</a>
            </div>
          </div>
          #end
        </div>
      </form>
    </div>
  </div>

</div>
  $!httpInclude.include("/footer.htm")
</body>
</html>