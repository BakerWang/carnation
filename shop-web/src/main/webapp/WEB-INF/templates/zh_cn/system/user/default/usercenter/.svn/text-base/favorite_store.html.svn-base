<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>店铺收藏-$!{config.title}-$!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
<meta name="copyright" content="$!{config.copyRight}">
<link href="$!webPath/resources/style/system/front/default/css/public2.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/public_auto.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/user.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/common/css/overlay.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/common/css/jquery-ui-1.8.22.custom.css" type="text/css" rel="stylesheet" />
<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
<script src="$!webPath/resources/js/jquery-ui-1.8.21.js"></script>
<script src="$!webPath/resources/js/jquery.poshytip.min.js"></script>
<script src="$!webPath/resources/js/jquery.shop.common.js"></script>
<script src="$!webPath/resources/js/jquery.validate.min.js"></script>
</head>
<body>
$!httpInclude.include("/top.htm")
$!httpInclude.include("/head.htm")
$!httpInclude.include("/nav1.htm")
<div class="main clearfix">
  <div class="usercenter"> $!httpInclude.include("/buyer/nav.htm?op=favorite")
    <div class="usercenter_right">
      <div class="myorder">
        <h4><span class="myorder_fright"></span><strong>我的收藏</strong></h4>
      </div>
      <div class="user_list">
      <div class="user_list_title">
        <ul class="user_title_ul">
                      <li><i></i><a href="$!webPath/buyer/favorite_goods.htm">收藏商品</a></li>
            <li class="this"><i></i><a href="$!webPath/buyer/favorite_store.htm">收藏店铺</a></li>
            <li><i></i><a href="$!webPath/group/favorite_store.htm">收藏商户</a></li>
            <li><i></i><a href="$!webPath/group/favorite_service.htm">收藏服务</a></li>
        </ul>
      </div>
      #if($!objs.size()>0)
      <form action="$!webPath/buyer/favorite_store.htm" method="post" id="ListForm">
        <div class="show_smallimg">
          <table width="100%" border="0" cellspacing="0" cellpadding="0" class="smallimg_table" >
            <tr>
              <th align="center" width="10%"></th>
              <th width="30%">店铺
                <input name="id" type="hidden" id="mulitId" />
                <input name="type" type="hidden" id="type" value="1" /></th>
              <th width="15%" align="center">收藏时间</th>
              <th align="center">操作</th>
            </tr>
            #foreach($obj in $objs)

<!--             #if($!obj.store_photo)
            #set($store_logo="$!imageWebServer/$!{obj.store_photo}")
            #else
            #set($store_logo="$!imageWebServer/$!config.storeImage.path/$!config.storeImage.name")
            #end
                  
			 #set($store_url="$!webPath/store_$!{obj.store_id}.htm")
       		 #if($!config.second_domain_open && $!obj.store_second_domain!="")
         	 #set($store_url="http://$!{obj.store_second_domain}.$!{domainPath}")
       		 #end -->


           #if($!obj.storeLogoPath)
            #set($store_logo="$!{obj.storeLogoPath}")
            #else
            #set($store_logo="$!{config.storeImage.path}")
            #end

            #set($store_url="$!webPath/store_$!{obj.fid}.htm")
            #if($!config.secondDomainOpen && $!obj.goodsType==1 && $!obj.storeSecondDomain !="")
            #set($store_url="http://$!{obj.storeSecondDomain}.$!{domainPath}")
            #end
            <tr>
              <td><ul class="goodsul">
                  <li style=" height:60px;"><span class="goodsimg" style="margin-top:-7px;"><a href="$!store_url" target="_blank"><img src="$!store_logo" width="100" height="33" /></a></span></li>
                </ul></td>
              <td><ul class="goodsul">
                  <li class="goodsp" style=" height:60px;"><span style=" width:258px; margin-top:17px; text-align:center; "><a href="$!store_url" target="_blank">$!obj.storeName</a></span> </li>
                </ul></td>
              <td style="text-align:center;" >$!CommUtil.formatShortDate($!obj.createTime)</td>
              <td align="center" class="goodsp3"><span class="del"><a href="javascript:void(0);" onclick="showDialog('msg_info','','确定删除该收藏店铺吗?',1,'question',3,function(){window.location.href='$!webPath/buyer/favorite_del.htm?id=$!obj.id&currentPage=$!currentPage&type=4'});" class="blue">删除</a></span></td>
            </tr>  #end
          </table>
        </div>
        <div class="fenye">
          <div class="fenye"><span class="user_page_sp">$!gotoPageFormHTML</span>
          </div>
        </div>
      </form>
      #else
         <div class="nothing">
            <div class="nothing_left"><img src="$!webPath/resources/style/system/front/default/images/nothing.png" /></div>
            <div class="nothing_right">您目前还没有收藏任何店铺！</div>
          </div>
        #end
    </div>
  </div>
</div>

</div>
$!httpInclude.include("/footer.htm")
</body>
</html>
