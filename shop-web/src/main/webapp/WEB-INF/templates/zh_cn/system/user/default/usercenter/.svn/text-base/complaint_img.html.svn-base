<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>投诉图片查看 - $!{config.poweredby}</title>
<meta name="keywords" content="$!config.keywords" />
<meta name="description" content="$!config.description" />
<meta name="generator" content="$!{config.meta_generator}" />
<meta name="author" content="$!{config.meta_author}">
<meta name="copyright" content="$!{config.copyRight}">
<link href="$!webPath/resources/style/system/front/default/css/public2.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/public_auto.css" type="text/css" rel="stylesheet" />
<link href="$!webPath/resources/style/system/front/default/css/user.css" type="text/css" rel="stylesheet" />
<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
</head>
<body>
$!httpInclude.include("/top.htm")
$!httpInclude.include("/head.htm")
$!httpInclude.include("/nav1.htm")
<div class="main">
  <div class="usercenter"> $!httpInclude.include("/buyer/nav.htm?op=complaint")
    <div class="usercenter_right">
      <div class="myorder">
        <h4><span class="myorder_fright"></span><strong>收货地址</strong></h4>
      </div>
      <div class="user_list">
        <div class="user_list_title">
          <ul class="user_title_ul">
            <li><i></i><a href="$!webPath/buyer/complaint.htm">我的投诉</a></li>
            <li class="this"><a href="javascript:void(0);">投诉详情</a></li>
          </ul>
        </div>
        <div class="user_list_box">
          <div class="complaint_one">
            <h1 class="complaint_h">投诉处理图片</h1>
            <div class="comp_goodetails">
              <ul class="comp_tousul">
                #if($!type=="buyer") 
                #if($!obj.fromAcc1)
                <li><span><img src="$!obj.fromAcc1" /><i class="comp_delall"> </i></span></li>
                #end
                #if($!obj.fromAcc2)
                <li><span><img src="$!obj.fromAcc2" /><i class="comp_delall"> </i></span></li>
                #end
                #if($!obj.fromAcc3)
                <li><span><img src="$!obj.fromAcc3" /><i class="comp_delall"> </i></span></li>
                #end
                #end
                #if($!type=="seller")  
                #if($!obj.toAcc1)
                <li><span>
                  <div style="width:100%; height:400px;overflow:scroll;"><img src="$!obj.toAcc1" /></div>
                  <i class="del_all"></i></span></li>
                #end
                #if($!obj.toAcc2)
                <li><span>
                  <div style="width:100%; height:400px;overflow:scroll;"><img src="$!obj.toAcc2" /></div>
                  <i class="del_all"></i></span></li>
                #end
                #if($!obj.toAcc3)
                <li><span>
                  <div style="width:100%; height:400px;overflow:scroll;"><img src="$!obj.toAcc3" /></div>
                  <i class="del_all"></i></span></li>
                #end
                #end
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  $!httpInclude.include("/footer.htm") </div>
