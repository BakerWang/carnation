@charset "utf-8";
body{background: #f8faf9}
html {
	color:#000;
	background:#fff;
	overflow-y:scroll;
	-webkit-text-size-adjust:100%;
	-ms-text-size-adjust:100%;
}
html * {
	outline:0;
	-webkit-text-size-adjust:none;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}
html,body {
	font-family:Helvetica ,sans-serif;
	/*font-family: PingFangSC-Regular;*/
}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section {
	margin:0;
	padding:0;
}
input,select,textarea {
	font-size:100%;
}
table {
	border-collapse:collapse;
	border-spacing:0;
}
fieldset,img {
	border:0;
}
abbr,acronym {
	border:0;
	font-variant:normal;
}
del {
	text-decoration:line-through;
}
address,caption,cite,code,dfn,em,th,var {
	font-style:normal;
	font-weight:500;
}
ol,ul {
	list-style:none;
}
caption,th {
	text-align:left;
}
h1,h2,h3,h4,h5,h6 {
	font-size:100%;
	font-weight:500;
}
q:before,q:after {
	content:'';
}
sub,sup {
	font-size:75%;
	line-height:0;
	position:relative;
	vertical-align:baseline;
}
ins,a {
	text-decoration:none;
}

#docFoot {
    margin: 0;

}

.footBgWrap {
    background: #fff none repeat scroll 0 0;
    border-top:1px solid #19bf96;
}

.footBgWrap .box {
    margin: 0 auto;
    width: 1090px;
}
#btmlist {
    border-bottom: 1px solid #eaeaea;
    padding: 40px 0;
}

.clearfix::after {
    clear: both;
    content: " ";
    display: block;
    font-size: 0;
    height: 0;
    visibility: hidden;
}
#btmlist li {
    float: left;
    width: 214px;
}
#btmlist li i {
    float: left;
    margin: 0 12px 0 0;
    font-style: normal;
    font-weight: 500;
}
.w-icon-23 {
    background:url("../images/gou.png") center center no-repeat;
    height: 44px;
    width: 44px;
}
.w-icon-244 {
    background:url("../images/zheng.png") center center no-repeat;
    height: 44px;
    width: 44px;
}
.w-icon-255 {
    background:url("../images/tui.png") center center no-repeat;
    height: 44px;
    width: 44px;
}
.w-icon-26 {
    background:url("../images/di.png") center center no-repeat;
    height: 44px;
    width: 44px;
}
.w-icon-27 {
    background:url("../images/xiang.png") center center no-repeat;
    height: 44px;
    width: 44px;
}
.w-icon {
    display: inline-block;
    height: 62px;
    width: 62px;
}

#btmlist li .tit {
    font-size: 18px;
    font-weight: bold;
}
#btmlist li .tit, #btmlist li .desc {
    display: block;
    line-height: 22px;
}
#btmlist li .desc {
    font-size: 14px;
}
#btmlist li .tit, #btmlist li .desc {
    display: block;
    line-height: 30px;
}
#guideList {
    padding: 40px 0 90px;
}
.clearfix::after {
    clear: both;
    content: " ";
    display: block;
    font-size: 0;
    height: 0;
    visibility: hidden;
}
#guideList .first {
    padding-left: 30px;
    padding-right: 40px;
    width: 260px;
}
#guideList .bd_none {
    border: medium none;
}
#guideList li {
    border-left: 1px solid #eaeaea;
    float: left;
    height: 128px;
    padding: 0 0 0 25px;
    width: 122px;
}
.bd_none {
    border: medium none;
}
#guideList .iconfont {
    cursor: pointer;
    display: block;
    font-size: 38px;
    height: 43px;
    line-height: 45px;
    margin-bottom: 20px;
    outline: medium none;
    width: 200px;
    z-index: 9;
}

#guideList li dt {
    margin: -4px 0 10px;
}

#guideList li dd {
    line-height: 24px;
}

#guideList li dt em {
    font-size: 16px;
    font-weight: normal;
}
#guideList a{
	color:#333;
	font-size:12px;
}
#guideList a:hover {
    cursor: pointer;
}

#guideList .focuson {
    height: 32px;
    padding-top:10px;
}

#guideList .focuson .itm {
    cursor: pointer;
    margin-left: 10px;
    position: relative;
    display:inline-block;
    vertical-align:middle;
}
.w-icon-24 {
    height: 41px;
    width: 41px;
    background:url('../images/xinl.png') center top;
    background-repeat:no-repeat;
}
.w-icon-25 {
    height: 41px;
