    background:url('../images/weix.png') center top;
    background-repeat:no-repeat;
}
.clearfix .bd_none{
	margin-left:10px;
	float: left;
    padding: 0;
    text-align: center;
    width: 108px;
}
#guideList .bd_none img {
    margin: 0 0 6px;
}
#guideList .bd_none span{
	font-size:14px;
	display:inline-block;
	margin-right:17px;
	color:#333;
}
.bot_text{margin-top:30px;padding-bottom:30px}
.bot_text p{text-align:center}

