.alert{width: 5rem;
    height: 2.2rem;
    background: #fd253e;
    border: solid 0.07rem rgba(0,0,0,0.3);
    background-clip: padding-box;
    color: #fff;
    position: absolute;
    left: 50%;
    top:200px;
    margin-left: -2.5rem;
