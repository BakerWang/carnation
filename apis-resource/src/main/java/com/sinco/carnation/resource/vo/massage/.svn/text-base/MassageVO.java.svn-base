package com.sinco.carnation.resource.vo.massage;

import com.sinco.api.annotation.Param;
import com.sinco.api.util.BasicPageParamVo;

public class MassageVO extends BasicPageParamVo {
	@Param(desc = "建议按摩时长|可选")
	private Integer adviceTime;

	@Param(desc = "总按摩时长|可选")
	private Integer allTime;

	public Integer getAdviceTime() {
		return adviceTime;
	}

	public void setAdviceTime(Integer adviceTime) {
		this.adviceTime = adviceTime;
	}

	public Integer getAllTime() {
		return allTime;
	}

	public void setAllTime(Integer allTime) {
		this.allTime = allTime;
	}

}