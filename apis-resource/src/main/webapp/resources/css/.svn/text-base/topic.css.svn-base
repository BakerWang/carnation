    box-shadow: 0em -3em 0em 0.2em #19bf96, 2em -2em 0 0em #19bf96, 3em 0em 0 -0.5em #19bf96, 2em 2em 0 -0.5em #19bf96, 0em 3em 0 -0.5em #19bf96, -2em 2em 0 -0.5em #19bf96, -3em 0em 0 -0.5em #19bf96, -2em -2em 0 0em #19bf96;
