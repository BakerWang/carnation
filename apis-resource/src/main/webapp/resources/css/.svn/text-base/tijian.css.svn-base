    background-image:linear-gradient(-90deg,  #fa5452 0%, #ffa2a1 100%);
    border-radius: 11px;
}
.tjHolder .content_xinlv span.amount:before{

    border-color: #fa5452 transparent transparent transparent;
   
}
.jb_xueya{
    background-image:linear-gradient(-90deg, #ff6211 0%, #fad961 100%);
}
.jb_xueya span.xueya_span{
    background-image:linear-gradient(-90deg, #ffa932 0%, #fff799 100%);
}
.tingli_icon1_xueya1 #minNum:after{
    border-color: #f5a623 transparent transparent transparent;
}
.tingli_icon1_xueya2 #maxNum:after{
    border-color: #ff6211 transparent transparent transparent;
}
