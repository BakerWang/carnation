.tjHolder{background: #F7F7F7;color: #4a4a4a;overflow: hidden;
   -webkit-transform: translate3d(0, 0, 0);
   -moz-transform: translate3d(0, 0, 0);
   -ms-transform: translate3d(0, 0, 0);
   transform: translate3d(0, 0, 0);
   -webkit-backface-visibility:hidden;
   -webkit-perspective:1000;
   }
canvas{display: block;}
.result{}
.result .title{background:#fff url(../img/titleIcon.png) no-repeat 0.3rem center;background-size: 0.48rem;height: 1rem;line-height: 1rem;border-bottom: solid 1px #ebebeb;font-size: 0.32rem;text-indent: 0.96rem;}
.result .title span{color: #d0021b;}

.result .tbContent{background: #FFF;overflow: hidden;margin-top: 0.12rem;}
.result .content{height: 17px;margin: 39px 6.25% 40px 6.25%;border-radius: 8px;position: relative;font-size: 0.28rem;color: #4a4a4a;border-bottom: solid 1px #ebebeb;}
.result .content span{position: absolute}
.result .content span.low{left: 9px;top: -20px}
.result .content span.high{right: 9px;top: -20px}
.result .content span.zero{left: 9px;top: 20px}
.result .content span.hundred{right: 9px;top: 20px}
.result .content span.amount{height: 100%;width: 1px;background: #fff;left: 0px;top: 0;}
.result .content span.amount:before{
	content: '';
	position: absolute;
	left: -5px;
	top: -7px;
	border-color: #57af1a transparent transparent transparent;
	border-style: solid;
	border-width: 7px 5px 0 5px;	
}


.content_feihuoliang{left: 0;top: 0;height: 17px;
    background-image:linear-gradient(90deg, #99d025 0%, #36b2ff 100%);
	border-radius:11px;
}

.content_xueyang{
	background-image:linear-gradient(90deg, #96b7ff 0%, #5283f0 100%);
	border-radius:11px;
}
.result .content_xueyang span.amount:before{
	border-color: #5283f0 transparent transparent transparent;
}

.result .tbContent .eye_img{width: 3.78rem;height: 2.84rem;display: block;background: url(../img/eye.png);background-size: contain;display: block;margin: 0 auto;}

.result .des{margin:15px 17px 0 17px;color: #a3a3a3;font-size: 0.28rem;line-height: 20px}
.result .des .red{color:#ec1b1b}
.result .des .green{color: #20B757}
.result .askOnline{float: right;margin: 0px 0.34rem 10px 0;color: #36b2ff;font-size: 0.28rem;text-decoration: none}

.tjHolder .history{ margin-top: 0.12rem;background: #fff;border-bottom: solid 1px #EBEBEB;overflow: hidden;width: 100%;}
.tjHolder .history .title{
	font-size:0.32rem;
	color:#4a4a4a;
	text-indent: 0.3rem;
	height: 0.8rem;
	line-height: 0.8rem;
}

.tjHolder .suggest{margin-top: 0.12rem;color: #4a4a4a;}
.suggest .title{
	font-size: 0.32rem;
	background:#fff url(../img/suggestIcon.png) no-repeat 0.3rem center;
	text-indent: 0.96rem;
	background-size: 0.38rem 0.48rem;	
	height: 1rem;
	line-height: 1rem;
	border-bottom: solid 1px #EBEBEB;
}
.suggest .title span{font-size: 0.28rem;float: right;margin-right: 0.28rem;}
.suggest .title span em{background: url(../img/heart1.png) no-repeat;width: 0.36rem;height: 0.32rem;display: inline-block;background-size: contain;
vertical-align: middle;margin: 0 1px;
}
.suggest .taskTit{height: 0.8rem;line-height: 0.8rem;text-indent: 0.96rem;font-size: 0.28rem;border-bottom: solid 1px #36b2ff;}
.suggest .yinshi{background:#fff url(../img/yinshi.png) no-repeat 0.42rem center;background-size: 0.24rem 0.34rem;}
.suggest .yundong{background:#fff url(../img/yundong.png) no-repeat 0.42rem center;background-size: 0.24rem 0.4rem;}
.suggest .shenghuo{background:#fff url(../img/shenghuo.png) no-repeat 0.42rem center;background-size: 0.34rem 0.34rem;}
.suggest .yiliao{background:#fff url(../img/yiliao.png) no-repeat 0.42rem center;background-size: 0.38rem 0.5rem;}
.suggest .more{background:#fff url(../img/more.png) no-repeat 0.42rem center;background-size: 0.34rem 0.3rem;}
.suggest .taskCon{margin-bottom: 0.12rem;border-bottom:solid 1px #EBEBEB;background: #fff;padding-bottom: 0.12rem;}
.suggest .taskCon p{border-bottom:solid 1px #EBEBEB;font-size: 0.28rem;padding: 0.18rem 0.3rem 0.12rem 0;line-height: 0.4rem;margin-left: 0.34rem;}
.suggest .taskCon p:last-child{border-bottom: none;}
.suggest .taskCon p .taskbtn{float: right;background: url(../img/heart1.png) no-repeat;width: 0.36rem;height: 0.32rem;background-size: contain;}
.suggest .taskCon p .curr{background: url(../img/heart2.png) no-repeat;background-size: contain;}

.tjHolder .tuijian{margin-top: 0.12rem;color: #4A4A4A;font-size: 0.24rem;}
.tuijian .title{background:#fff url(../img/jinghuaTj.png) no-repeat 0.3rem center;background-size: 0.46rem 0.46rem;font-size: 0.32rem;text-indent: 0.96rem;border-bottom: solid 1px #efefef;height: 1.08rem;line-height: 1.08rem;}
.tuijian a{color: #4a4a4a;}
/* 触摸 base css */
.m_touch{overflow:hidden;background: #fff;}
.m_touch .touch_content{overflow:hidden; zoom:1;padding-bottom: 0.15rem}
.m_touch .touch_content .touch_con{width:100%; overflow:hidden; zoom:1; float:left;}
.m_touch .touch_content .touch_con .touch_tab{overflow:hidden; zoom:1; float:left;}
/* 触摸 base css end */

/* 筛选 */
.p_filter{border-bottom: solid 1px #f8f8f8;height: 0.96rem;line-height: 0.96rem;background: #fff;}
.p_filter .dd{font-size:0.32rem; width:33.3%; color:#999; position:relative; text-align:center;float: left;}
.p_filter .dd a{display:block; color: #333;text-decoration: none;width: 1rem;margin: 0 auto;}
.p_filter a.selected{color:#36b2ff;}
.p_filter a.selected i{width:1rem; background:#36b2ff; height:1px; position:absolute; left:0.74rem; bottom:0px;}
dl.p_filter_t dt{height:25px; line-height:25px; margin:10px 0; font-size:20px; margin-right:10px; color:#333; text-indent:5px;}
.bz_title dd{width:37%;}
/* 筛选 end */


ul,li{list-style-type: none}
.tiezi{margin-left: 10px;overflow: hidden}
.tiezi li{border-bottom: solid 1px #dcdcdc;position: relative;height: 1.26rem;width: 100%;float: left}
.tiezi li img{width: 0.94rem;height: 0.94rem;border-radius: 0.1rem;position:absolute;left:0;top:0.16rem  }
.tiezi li .content{margin-left: 1.14rem}
.tiezi li .content .tit{font-size: 0.32rem;color: #4a4a4a;line-height: 0.4rem;width: 100%;overflow: hidden;margin-top: 0.2rem}
.tiezi li .content .bot p{height: 0.66rem;font-size: 0.24rem;color: #999;line-height: 0.32rem;overflow: hidden;padding-right: 1.2rem}
.tiezi li .content .bot span{position: absolute;right: 0.2rem;bottom: 0.16rem;font-size: 0.24rem;color: #999}
.goods{overflow: hidden;margin-top: 0.1rem}
.goods li{width: 50%;float: left}
.goods li a div.goodsdiv{width: 100%;border: solid 1px #e3e3e3;border-top: none;border-left: none;text-align: center;overflow: hidden}
.goods li:nth-last-child(1) div.goodsdiv,.goods li:nth-last-child(2) div.goodsdiv {border-bottom: none}
.goods li div img{width: 1.8rem;height: 1.8rem;margin: 0.14rem 0 0.14rem 0;display: block;vertical-align: top;display: inline-block}
.goods li div p.tit{text-align: left;height: 0.72rem;line-height:0.36rem;overflow: hidden;margin: 0 0.1rem 0 0.2rem}
.goods li div span.price{font-size: 0.32rem;color: #fc1818;text-align: left;display: block;margin: 0.12rem 0px 0.12rem 0.2rem}
.fn-rmb {font-family: arial;font-style: normal;padding-right: 0.08rem}
.service{overflow: hidden}
.service li{float: left;height: 1.75rem;margin: 0 0 0 0.2rem;border-bottom: solid 1px #dcdcdc;position: relative;width: 100%}
.service li img{position: absolute;width: 1.2rem;height: 1.3rem;left: 0;top: 8px}
.service li .right{margin-left: 1.4rem;position:relative;padding-right: 0.4rem}
.service li .right .tit{font-size: 0.32rem;margin-top: 0.24rem;overflow: hidden}
.service li .right .star{color: #ffab34;overflow: hidden}
.service li .right .rate_score{margin: 0.14rem 0;height: 0.24rem;width: 1.6rem;display: block;background: url("../img/star.png") no-repeat 0 bottom;background-size: 1.6rem 0.48rem;float: left}
.service li .right .rate_score em{background: url("../img/star.png") no-repeat 0 0;background-size: 1.6rem 0.48rem;display: block;height: 0.24rem;overflow: hidden}
.service li .right .star .star_num{float: left;margin: 0.1rem}
.service li .right .price{color: #fd5656;font-size: 0.28rem;display: block}
.service li .right .pos{position: absolute;right: 0.4rem;bottom: 0;color: #858585;font-size: 0.24rem;text-align: right}
.service li .right .pos p{line-height: 0.3rem;font-size:0.24rem; color:#4a4a4a;}
.service li .right .pos p.icon_p{font-size:0.24rem; color:#9b9b9b; margin-bottom: 0.1rem;display: inline-block;  padding-left: 0.26rem;background: url("data:image/png;base64,") no-repeat left center;background-size:0.2rem 0.24rem;}

.jb_tingli {
    background: -webkit-linear-gradient(left, #0fe6fa , #41c7ff);
    background: -o-linear-gradient(right, #0fe6fa, #41c7ff);
    background: -moz-linear-gradient(right, #0fe6fa, #41c7ff);
    background: linear-gradient(to right, #0fe6fa , #41c7ff);
}
.jb {
    height: 17px;
    margin: 39px 6.25% 40px 6.25%;
    border-radius: 8px;
    position: relative;
}
.jb span.low {
    color: #999;
    left: 9px;
    top: -20px;
    font-size:0.28rem;
    color:#4a4a4a;
}
.jb span {
    position: absolute;
}
.jb span.high {
    color: #999;
    right: 9px;
    top: -20px;
    font-size:0.28rem;
    color:#4a4a4a;
}
.jb span.zero {
    left: 9px;
    top: 20px;
    font-size:0.28rem;
    color:#4a4a4a;
}
.jb span.hundred {
    right: 9px;
    top: 20px;
    font-size:0.28rem;
    color:#4a4a4a;
}
.tingli_span{
	-webkit-transition:all 1s ease-out;
	transition:all 1s ease-out;
}
.tingli_span{
	left: 0;
    top: 0;
    height: 17px;
    background: -webkit-linear-gradient(left, #c8f8ff , #64f2ff);
    background: -o-linear-gradient(right, #c8f8ff, #64f2ff);
    background: -moz-linear-gradient(right, #c8f8ff, #64f2ff);
    background: linear-gradient(to right, #c8f8ff , #64f2ff);
    border-radius: 8px;
    width: 100%;
}
.tingli_icon1{
	top: -15px;
    left: 0px;
    background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAsAAAAPCAYAAAAyPTUwAAAAGXRF…0WDsCxI1Q9h8gzgDiVzBJFizWdgKxPhBfhAYrHAAEGABMOiaoEWygAwAAAABJRU5ErkJggg==") no-repeat;
    background-size: 11px 15px;
    width: 11px;
    height: 15px;
}
.tingli_icon2,.tingli_icon1{
	-webkit-transition: all 1s ease-out;
    -o-transition: all 1s ease-out;
    transition: all 1s ease-out;
}
.tingli_icon1 span {
    left: 0;
    top: -15px;
    font-size: 14px;
}
.tingli_icon2 {
    top: 17px;
    left: 98%;
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
    background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAsAAAAPCAYAAAAyPTUwAAAAGXRF…0WDsCxI1Q9h8gzgDiVzBJFizWdgKxPhBfhAYrHAAEGABMOiaoEWygAwAAAABJRU5ErkJggg==") no-repeat;
    background-size: 11px 15px;
    width: 11px;
    height: 15px;
}

.tingli_icon2 span {
    left: 0;
    top: -15px;
    font-size: 14px
 -webkit-transform:rotate(1deg);
    transform: rotate(1deg);
    font-size:0.28rem;
}
.tingli_icon1 #minNum:after{
	content: '';
    position: absolute;
    top: 0.4rem;
    border-color: #00b1fe transparent transparent transparent;
    border-style: solid;
    border-width: 0.14rem 0.1rem 0 0.1rem;
}
.tingli_icon2 #maxNum:after{
	content: '';
    position: absolute;
    top: 0.4rem;
    right: -0.5rem;
    border-color: #00b1fe transparent transparent transparent;
    border-style: solid;
    border-width: 0.14rem 0.1rem 0 0.1rem;
}

.content_xinlv {
    left: 0;
    top: 0;
