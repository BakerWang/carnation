/*问卷调查*/
.fn-hide{display: none !important;}
.bgWhite{background-color: #fff;}
.progressBar{position: fixed;top: 0.03rem;left: 0;width: 100%;height: 0.02rem;z-index: 10;}
.progressBar .bar{position: absolute;left: 0;top: -0.02rem;width: 0%;height: 0.06rem;background: #36b2ff;transition: all 0.2s ease-out;}
.progressBar .ignore{
    position: absolute;
    right: 0;
    top: 0.1rem;
    color: #9B9B9B;
    font-size: 12px;
    width: 1.2rem;
    height: 0.6rem;
    text-align: center;
    line-height: 0.6rem;
}
.questionContent{margin-top: 0.7rem;font-size: 12px;}
.questionContent .quesTypeArea{margin:0 0.34rem 0 0.34rem;text-align: center;padding-top: 0.4rem;}
.quesTypeArea .typeTit{font-size: 0.4rem;text-align: center;position: relative;}
.quesTypeArea .typeTit:before{position: absolute;content: "";width: 0.76rem;height: 1px;background-color:#9B9B9B;left: -0.94rem;top: 50%;}
.quesTypeArea .typeTit:after{position: absolute;content: "";width: 0.76rem;height: 1px;background-color: #9B9B9B;right:-0.94rem;top: 50%;}
.questionContent .questionList .timu{padding-top: 0.4rem;border-top: solid 1px #fafafa;text-align: left;margin:0 0.34rem 0.2rem 0.34rem;}
.questionContent .questionList div:first-child{border-top: none;}
.questionContent .questionList .timu .subject{text-align: center;}
.questionContent .questionList .timu .subject p{text-align: center;display: inline-block;font-size: 0.32rem;color: #9B9B9B;line-height: 0.36rem;padding: 0 0.2rem;}
.questionContent .questionList .curr .subject p{color: #4A4A4A;}
.questionContent .questionList .timu .option{display: inline-block;margin-left: 30%;}
.questionContent .questionList .timu .option a{display: block;text-align: left;color: #4a4a4a;font-size: 0.28rem;position: relative;padding: 0.2rem 0 0 0.5rem;line-height: 0.32rem;}
.questionContent .questionList .timu .option a span{width: 0.44rem;height: 0.44rem;position: absolute;left: -0.14rem;top: 0.13rem;border-radius: 50%;border:solid 1px #36b2ff;box-sizing: border-box;}
.questionContent .questionList .timu .option a span:before{
	content: "";
	position: absolute;
	width: 0.44rem;
	height: 0.44rem;
	border:solid 1px #36b2ff;
	background: #36b2ff;
	border-radius: 50%;
	left: -1px;
	top: -1px;
	transform: scale(0.01);
	-webkit-transform: scale(0.01);
	-moz-transform: scale(0.01);
	-ms-transform: scale(0.01);
	transform-origin: 50% 50%;
	box-sizing: border-box;
	transition: all 0.2s;
}
.questionContent .questionList .timu .option a.curr span:before{
	transform: scale(0.7);
	-webkit-transform: scale(0.7);
	-moz-transform: scale(0.7);
	-ms-transform: scale(0.7);
}

.questionContent .questionList .timu .option-horizontal{display: inline-flex;display: -webkit-inline-flex;flex-direction: row;justify-content: center;align-items: center;width: 100%;position: relative;left: 0.2rem;}
