html,body{
	height: 100%;
}
body{background: #FFFFFF;}
.taskHolder{
	background: #F7F7F7;color: #4a4a4a;
   -webkit-transform: translate3d(0, 0, 0);
   -moz-transform: translate3d(0, 0, 0);
   -ms-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0);
   -webkit-backface-visibility:hidden;
   -webkit-perspective:1000;
   height: 100%;
   padding-top: 0.9rem;	
}
.appbar{height:0.9rem;line-height: 0.9rem;background: #119aee;overflow: hidden;position: fixed;left: 0;top: 0;width: 100%;z-index: 1;}
.appbar .return{width: 1rem;height: 0.9rem;float: left;
	background: url(data:image/png;base64,) no-repeat 0.3rem center;
	background-size: 0.24rem 0.4rem;
}
.appbar #search{
	width: 6rem;
	height: 0.56rem;
	font-size: 0.28rem;
	border-radius:4px;
	outline: none;
	border: none;
	box-sizing: border-box;
	padding: 0.1rem 0 0.1rem 0.14rem;
	background:#fff url(data:image/png;base64,) no-repeat 0.16rem center;
	background-size:0.26rem;
	display: block;
}
.appbar .search{
	position: relative;
	float: left;
	margin: 0.17rem 0 0 0;	
}
.appbar .search .clear{
	position: absolute;
	width: 0.6rem;
	height: 0.56rem;
	right: 0;
	top: 0;
	display: none;
}
.appbar .search .clear:after{
	content: "";
	width: 0.28rem;
	height: 0.28rem;
	position: absolute;
	left: 0.16rem;
	top: 0.14rem;
	background: url(data:image/png;base64,) no-repeat center center;
	background-size: contain;
}
.appbar .cancle{float: left;font-size: 0.36rem;color: #FFFFFF;width: 0.8rem;text-align: center;margin-left: 0.1rem;display: none;}
.appbar .centerTitle{position: absolute;left:0;top:0;line-height: 0.9rem;text-align: center;font-size: 0.36rem;color: #FFFFFF;left: 50%;
	transform: translate3d(-50%,0,0);
	-webkit-transform: translate(-50%,0);
	-moz-transform: translate(-50%,0);
}
.appbar .submit{float: right;font-size: 0.36rem;padding:0 0.2rem;color: #FFFFFF;}
.appbar .edit{float: left;width: 1.04rem;font-size: 0.36rem;color: #FFFFFF;text-align: center;}
.appbar .add{float: right;color: #FFFFFF;
	border-radius: 50%;width: 0.44rem;height: 0.44rem;margin: 0.23rem 0.16rem 0 0;
	background: url(data:image/png;base64,) no-repeat center center;
	background-size:contain;
}

.appbar #search.bg_none{
	background: #fff;
}
.taskList{
	margin-top: 0.12rem;
	background: #FFFFFF;
}
.taskList ul li{
	border-bottom: solid 1px #f5f5f5;
	height: 1rem;
	line-height: 1rem;
	overflow: hidden;
}
.taskList ul li .imgHolder{
	float: left;
	width: 0.48rem;
	height: 0.48rem;
	margin: 0.26rem 0 0 0.26rem;
}
.taskList ul li .imgHolder img{
	width: 100%;
	height: 100%;
	display: block;
}
.taskList ul li .taskCon{
	float: left;
	margin-left: 0.18rem;
	width: 5.3rem;
}
.taskList ul li .taskCon .title{float: left;font-size: 16px;height: 0.6rem;line-height: 0.6rem;width: 5.3rem;}
.taskList ul li .taskCon .total{float: left;font-size: 10px;color: #9b9b9b;height: 0.4rem;line-height: 1;}
.taskList ul li .join{float: left;font-size: 14px;background: #fff;width: 1.1rem;height: 0.52rem;line-height: 0.52rem;color: #3fb6ff;border:solid 1px #3fb6ff;box-sizing:border-box;border-radius: 0.26rem;text-align: center;margin-top: 0.24rem;}


.searchCon{position: relative;}
.suggestion{background: #FFFFFF;position: absolute;left: 0;top: 0;width: 100%;min-height: 100%;display: none;}
.suggestion ul li{overflow: hidden;font-size: 0.32rem;height: 1rem;line-height: 1rem;text-indent: 0.3rem;font-size: 16px;border-bottom: solid 1px #f5f5f5;}
.suggestion ul li span{float: right;margin-right:0.24rem;color: #9B9B9B;font-size: 12px;}

.taskTit{background: #FFFFFF;overflow: hidden;height: 0.8rem;}
.taskTit .icon{
	float: left;
	width: 0.48rem;
	height: 0.48rem;
	margin: 0.16rem 0 0 0.28rem;
}
.taskTit .icon img{width: 100%;height: 100%;display: block;}
.taskTit .taskName{float: left;margin-left: 0.28rem;font-size: 18px;line-height: 0.8rem;}
.taskTit .switch{float: right;line-height: 0.8rem;height: 0.8rem;width: 2rem;}
.remindTime{overflow: hidden;margin-top: 0.12rem;background: #FFFFFF;}
.remindTime .tit{font-size: 16px;margin: 0.4rem 0 0 0.4rem;float: left;width: 100%;}
#choose{
	width: 5.5rem;margin: 0.28rem 1rem;float: left;padding: 0;font-size: 14px;line-height: 0.8rem;
}
.mui-btn-blue, .mui-btn-primary, input[type=submit] {
	color: #fff;
	border: 1px solid #43b8ff;
	background-color: #43b8ff;
}
.cycle{margin-top: 0.12rem;background: #FFFFFF;overflow: hidden;}
.cycle .tit{font-size: 16px;float: left;width: 100%;margin: 0.4rem 0 0 0.4rem}
.cycle .week{height: 50px;line-height: 50px;width: 100%;float: left;}
.cycle .week ul{width: 100%;overflow: hidden;margin-left: 3%}
.cycle .week ul li{width: 10%;float: left;height: 50px;line-height: 50px;margin-left: 3%;text-align: center;font-size: 14px}
.cycle .week ul li.curr{color: #43b8ff}

.dotask{background: #FFFFFF;overflow: hidden;}
.dotask .timenow{margin: 0.36rem auto;font-size: 0.36rem;text-align: center;}
.dotask .undone{width: 4.32rem;height: 4.32rem;margin: 0 auto;border:solid 0.28rem #F7F7F7;border-radius: 50%;-webkit-border-radius: 50%;box-sizing: border-box;transition:all 1s;-webkit-transition:all 1s}
.dotask .undone:after{content: "";width: 3.36rem;height: 3.36rem;border-radius:9999px;-webkit-border-radius: 9999px;-moz-border-radius:9999px;box-sizing: border-box;background-clip: padding-box;background: #f7f7f7 url(../img/checked.png) no-repeat center center;background-size: 2.52rem 2.02rem;display: block;margin: 0.2rem;}
.dotask .tips{margin: 0.28rem 0 0.32rem 0;font-size: 0.28rem;color: #9B9B9B;text-align: center;}
.dotask .done{border-color: #43b8ff;}
.dotask .done:after{background-color:#43b8ff;}
.tongji{margin-top: 0.12rem;background: #fff url(../img/tongji_icon.png) no-repeat 0.28rem center;background-size: 0.44rem 0.36rem;color: #272536;font-size: 12px;text-indent: 0.88rem;line-height: 0.6rem;overflow: hidden;}
.tongji a{float: right;margin-right: 0.28rem;background: url(../img/arraw-right.png) no-repeat right center;background-size: 0.14rem 0.24rem;text-align: right;text-indent: 0;padding-right: 0.32rem;color: #9B9B9B;}
.weekly{margin-top: 2px;background: #FFFFFF;}
.weekly .week{overflow: hidden;}
.weekly .week ul{width: 100%;overflow: hidden;margin-left: 3%;height: 80px;}
.weekly .week ul li{width: 10%;float: left;height: 50px;line-height: 50px;margin-left: 3%;text-align: center;font-size: 14px;position: relative;}
.weekly .week ul li.curr{color: #43b8ff}
.weekly .week ul li:after{content: "";position: absolute;top: 0.8rem;left: 50%;transform: translate(-50%,0);width: 0.4rem;height: 0.4rem;background: #F7F7F7 url(../img/checked.png) no-repeat center center;background-size: 0.25rem 0.25rem;border-radius: 50%;}
.weekly .week ul li.curr:after{background-color: #43b8ff;}

.taList{transition: all 0.5s;}
.taList ul{overflow: hidden;}
.taList ul li{height: 0.8rem;line-height: 0.8rem;margin-bottom: 0.12rem;background: #FFFFFF;}
.taList ul li a.remove{width: 0.44rem;height: 0.44rem;text-align: center;line-height: 0.44rem;background: #fe3824;float: left;margin: 0.18rem 0 0 0.24rem;border-radius: 50%;position: relative;display: none;}
.taList ul li a.remove:before{content:"";width: 0.3rem;height: 2px;background:#FFC0C0;;position: absolute;left: 50%;top: 50%;transform: translate(-50%,-50%);}          
.taList ul li .icon{
	float: left;
	width: 0.48rem;
	height: 0.48rem;
	margin: 0.16rem 0 0 0.28rem;
}
.taList ul li .icon img{width: 100%;height: 100%;}
.taList ul li .taskName{float: left;margin-left: 0.28rem;font-size: 18px;line-height: 0.8rem;}
.taList ul li .dayCount{float: right;font-size: 10px;color: #9B9B9B;margin-right: 0.28rem;}
.hide{display: none;}
.no-task-center{text-align: center;line-height: 20px;font-size: 18px;padding: 10px 0;}

.mui-switch.mui-active {
    border-color: #43b8ff;
    background-color: #43b8ff;
}





