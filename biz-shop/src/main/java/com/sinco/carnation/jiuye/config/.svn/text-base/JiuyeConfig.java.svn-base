package com.sinco.carnation.jiuye.config;

/**
 * 酒业远程接口配置文件
 * 
 * @author yuanmin.feng
 * 
 */
public class JiuyeConfig {

	// 远程服务调用路径
	private static String remoteServiceDomain;

	// 白名单IP
	private static String whiteListIp;

	// 订单详情url
	private static String orderDetailUrl;

	// 酒业首页url
	private static String indexUrl;

	public static String getRemoteServiceDomain() {
		return remoteServiceDomain;
	}

	public static void setRemoteServiceDomain(String remoteServiceDomain) {
		JiuyeConfig.remoteServiceDomain = remoteServiceDomain;
	}

	public static String getWhiteListIp() {
		return whiteListIp;
	}

	public static void setWhiteListIp(String whiteListIp) {
		JiuyeConfig.whiteListIp = whiteListIp;
	}

	public static String getOrderDetailUrl() {
		return orderDetailUrl;
	}

	public static void setOrderDetailUrl(String orderDetailUrl) {
		JiuyeConfig.orderDetailUrl = orderDetailUrl;
	}

	public static String getIndexUrl() {
		return indexUrl;
	}

	public static void setIndexUrl(String indexUrl) {
		JiuyeConfig.indexUrl = indexUrl;
	}

}
