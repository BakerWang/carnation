.scroll-body { overflow: hidden;}
#wrapper,#scroll-x {
    position: absolute;
    z-index: 1;
    top: 0;
    bottom: 62px;
    left: 0;
    width: 100%;
    overflow: hidden;
}
#scroller,.scroll-box {
    position: absolute; top: 0;
    z-index: 1;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    width: 100%;
    -webkit-transform: translateZ(0);
    -moz-transform: translateZ(0);
    -ms-transform: translateZ(0);
    -o-transform: translateZ(0);
    transform: translateZ(0);
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-text-size-adjust: none;
    -moz-text-size-adjust: none;
    -ms-text-size-adjust: none;
    -o-text-size-adjust: none;
    text-size-adjust: none;
}

#scroll-x { height: 59px; line-height: 56px; bottom: auto; }
.scroll-box { width: auto; height: 100%;}
.scroll-box ul { width: 100%; height: 100%; text-align: center;}
.scroll-box li { display: table-cell; height: 56px; background-color: #fff; }

#pullUp { text-align: center;}

#wrapper.no-bot,#scroll-x.no-bot { bottom: 0;}




/*———————————————————————————------Task------———————————————————————————————*/
.task { padding: 10% 15px 0;}
.task .title { text-align: center;}
.task .pic { padding: 15px 0; text-align: center;  position: relative;}
.task .pic i { position: absolute; font-size: 32px; color: #cbcbcb; top: 50%; margin-top: -15px;}
.task .icon-chevron-left { left: 0px;}.task .icon-chevron-right { right: 0px;}

.task-01 img { max-width: 60%;}

.task .note { margin: 0 auto; padding: 20px; max-width: 640px; line-height: 1.3; font-size: 0.9rem; text-align: justify;}
.w-max img { max-width: 85%;}
.no-pad .pic { padding-bottom: 0;}.no-pad .note { padding-top: 0;}

.step-task li { display: none;}.step-task li.active { display: block;}
.step-label { margin-bottom: 8px; border-radius: 4px; border: 1px #0493cd solid; width: 54px;line-height: 0.9;}.step-label label { display: inline-block; padding: 2px 3px;  margin-right: 3px; color: #fff; background-color: #0493cd;}

.p-canvas { position: absolute; bottom: 0px; right: 0px; width: 18%;}
.mask { position: fixed; top: 0; bottom: 0; width: 100%; text-align: center; background-color: rgba(0,0,0,0.4);}
.startBtn { position: absolute; left: 50%; top: 50%; width: 28%; border-radius: 50%; margin: -20% 0 0 -14%;}

.task-blue { background-color: #59c4f0; color: #fff; line-height: 1.5;}
.task-showBox {}
.task-showBox .title { font-size: 1.2rem; padding: 0 10% 15px;  text-align: center;}
.task-showBox .pic { text-align: center; padding: 20% 0 10px;}
.task-showBox .note { padding: 0 10%;  font-size: 0.9rem;}
.task-showBox img { width: 70%; padding: 4px; background-color: #fff; box-shadow: 0 0 4px #333;}

.openEye { text-align: center; padding-top: 20%;}.openEye li { display: none;}
.openEye img { max-width: 48%;}.openEye .active { display: block;}
ul.openEye { padding-top: 45%;}

.task-grid { background-image: url("../images/grid-bg.png"); background-size: cover; background-repeat: no-repeat;}
.ball { position: absolute; width: 40px; height: 40px; background-color: #000; border-radius: 20px; margin-top: -20px;}
#leftBall { left: 0; top: 50%; animation:leftMove 20s linear; -webkit-animation:leftMove 20s linear;}
#rightBall { right: 0; top: 50%; animation:rightMove 20s linear; -webkit-animation:rightMove 20s linear;}
@keyframes leftMove {
    0%{
        left: 0;
    }
    50%{
        left: 50%;
        margin-left: -40px;
    }
    100%{
        left: 0;
    }
}
@keyframes rightMove {
    0%{
        right: 0;
    }
    50%{
        right: 50%;
        margin-right: -40px;
    }
    100% {
        right: 0;
    }
}
@-webkit-keyframes leftMove /* Safari and Chrome */
{
    0%{
        left: 0;
    }
    50%{
        left: 50%;
        margin-left: -40px;
    }
    100%{
        left: 0;
    }
}
@-webkit-keyframes rightMove /* Safari and Chrome */
{
    0%{
        right: 0;
    }
    50%{
        right: 50%;
        margin-right: -40px;
    }
    100%{
        right: 0;
    }
}

.green-ball { position: absolute; background-color: #009900; border-radius: 15px; width: 30px; height: 30px; left: 10px; top: 20px; visibility: hidden;}

#pic-c { position: absolute; width: 10%; left: 45%; top: 45%; display: none;}

