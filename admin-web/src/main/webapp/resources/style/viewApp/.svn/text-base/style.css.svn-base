body { color: #777; font-family: "冬青黑体简体中文", "Microsoft YaHe";}
a,a:hover { color: #777; text-decoration: none;}
img { max-width: 100%;}

.pt-0 { padding-top: 0 !important;}
.pt-10 { padding-top: 10px;}.pt-p10 { padding-top: 10%;}.w-pm90 { max-width: 90%;}
.pl-10 { padding-left: 10px;}
.pr-10 { padding-right: 10px;}
.c-blue { color: #59c4f0;}

.p-gray-b { box-shadow: 0 6px 0 #efefef; margin-bottom: 6px;}

.fz-s { font-size: 0.9rem;}.fz-l { font-size: 1.1rem;}

.fl { float: left;} .fr { float: right;} .f-b { font-weight: bold;}

.col-3 { float: left; width: 33.33%;}

.btn { border: none; padding: 0 20px; height: 40px; border-radius: 6px; box-sizing: border-box;}
.btn-blue { background-color: #59c4f0; color: #fff;}
.double-btn-bar { padding-left: 2%;}
.double-btn-bar .btn { width: 43%; margin: 0 2%;}

.text-left { text-align: left;}.text-right { text-align: right;}.text-center { text-align: center;}

.list { margin: 0; padding: 0; list-style: none;}
.clearfix:before,.clearfix:after{
    content:"";
    display:table;
}
.clearfix:after{clear:both;}

.question-header { overflow: hidden; padding: 25px 15px; background: url("../images/temp/q-bg.jpg") no-repeat;}



.qh-info { float: left; padding-right: 20px; overflow: hidden;border-radius: 54px; background-color: rgba(0,0,0,0.4); color: #fff;}
.qh-info img { float: left; width: 80px; border: 4px #59c4f5 solid; border-radius: 50%;}
.qh-name { padding: 15px 0 0 90px; line-height: 200%; white-space: nowrap; min-width: 80px;}.qh-name .name { font-size: 1.1rem;}

.qh-praise { height: 30px; line-height: 30px; float: right; border-radius: 15px; background-color: #fff; padding: 0 10px;}
.qh-praise i { color: #59c4f5;}

.qh-title { line-height: 200%; padding: 10px 15px 6px; border-bottom: 1px #e0e0e0 solid; font-size: 1.1rem;}.qh-title i { color: #999;}
.qh-cont { padding: 10px 15px; border-bottom: 1px #dfdfdf solid;} .qh-date { padding: 15px; text-align: right;}

.cmt-pop { position:relative; padding:10px; background:#fff; border-radius:10px; box-shadow: 0 0 2px #666; color: #96a3af; line-height: 180%; word-break:break-all; }
.cmt-reply .cmt-pop { margin: 0 15px 0 15px; background:#f3fcff; }
.cmt-question .cmt-pop { margin: 0 15px 0 5px; background:#fff;}
.cmt-pop .icon-triangle-right, .cmt-pop .icon-triangle-left { font-size: 28px; position: absolute; top: 10px; right: -10px; color: #fff; text-shadow: 1px 0 1px #aaa;}
.cmt-pop .icon-triangle-left { right: auto; left: -10px; text-shadow: -1px 0 1px #aaa; color: #f3fcff}

.cmt-padding { padding: 0 10% 0 10%;}.cmt-list .cmt-question .cmt-padding { padding: 0 10% 0 0;}
.cmt-list { padding: 25px 15px; background-color: #fcfcfc; }.cmt-list > li { padding-bottom: 5px; overflow: hidden;}
.cmt-list .pic { float: left; width: 10%; border-radius: 50%;}.cmt-list .cmt-question .pic { float: right;}
.cmt-date { padding: 15px; text-align: center;}
.cmt-date span { display: inline-block; padding: 2px 8px; background-color: #d4d5d6; font-size: 0.8rem; color: #fff;}

.reply-box { position: absolute; bottom: 0;box-sizing: border-box; width: 100%; padding: 10px 20px; border-top: 1px #ebeced solid;}
.reply-btn { display: block; width: 100%; padding: 8px; text-align: left; border-radius: 10px; border: 1px #e7e7e7 solid; background-color: transparent;}
.reply-btn i { color: #c4c4c4; font-size: 1.2rem; vertical-align: middle;}


/*-------------------bbs----------------------*/
.bbs-header { position: relative; padding: 15px 10px; background-color: #fafafa; border-top: 6px #efefef solid; border-bottom: 1px #d7d7d7 solid;}
.bbs-user-info { position: relative;}
.bbs-user-info .pic { width: 20%;max-width: 80px; border-radius: 50%; vertical-align: middle; border: 2px #fff solid;}
.bbs-user-info .info { display: inline-block; vertical-align: middle;  margin: 6px 0 0 10px; }
.bbs-user-info .name {max-width: 220px; white-space: nowrap; text-overflow: ellipsis; overflow: hidden;}

.bbs-header .date { position: absolute; top: 52px; right: 15px;}
.bbs-user-info .color-pop { display: block; width: 40px;}
.up-border-arrow { position: absolute; left: 9%; bottom: -11px; color: #fff; text-shadow: 0 -1px 0 #d7d7d7; font-size: 24px;}
.color-pop { position: relative; margin: 5px 0; font-size: 0.9rem; padding: 2px 10px; border-radius: 3px; color: #fff; background-color: #fe6000; line-height: 150%;}
.color-pop:before{
    position:absolute;content:"\00a0";width:0px;height:0px;
    border-width:5px 5px 5px 0;border-style:solid; border-color:transparent #fe6000 transparent transparent;top:6px;left:-5px;
}

.bbs-cont { padding: 10px; line-height: 160%;}
.bbs-cont .title { margin-top: 0; padding: 4px 0; border-bottom: 1px #e0e0e0 solid; line-height: 180%; font-size: 1.2rem;}
.bbs-cont .reply { color: #999; font-size: 0.9rem; text-align: right;}.bbs-cont .reply i { vertical-align: middle;}
.bbs-action { padding: 10px; background-color: #f7f7f7; border-top: 1px #d7d7d7 solid; border-bottom: 1px #d7d7d7 solid; color: #999;}

.bbs-list .bbs-user-info .name { margin-top: 0;}
.bbs-list .bbs-user-info .pic { width: 18%; max-width: 80px;}
.bbs-list > li { }
.bbs-list .date { float: right;}
.bbs-reply-cont { padding: 10px; line-height: 160%;}
.bbs-reply-Btn { border: 1px solid #d0d0d0; padding: 3px 15px; background-color: #fff; border-radius: 8px;}
.reply-quotes { margin-bottom: 10px; padding: 10px; background-color: #f3f3f3;}
.reply-quotes .title { font-size: 0.9rem; color: #999;}

/*----------------------------------bbs End-----------------------------------*/
/*----------------notice-------------------*/
.notice { background-color: #fff; margin-top: 6px; border-top: 1px #d7d7d7 solid; border-bottom: 1px #d7d7d7 solid; padding: 0 10px; line-height: 1.4rem;}
.notice .title { padding: 10px 0; border-bottom: 1px #e0e0e0 solid;}
.notice .cont { padding: 10px 0 20px 0; font-size: 0.9rem;}.notice .cont a { color: #59c4f0; text-decoration: underline;}
.notice .date { padding: 10px 0; font-size: 0.7rem; line-height: 1.8; border-top: 1px #e0e0e0 solid;}
/*------------------notice End-----------------------*/
.self-header { padding: 20px; border-bottom: 1px #c4c4c4 solid; background-color: #fff;}
.progress-bar { margin-bottom: 10px; height: 8px; background-color: #d4d4d4; border-radius: 4px;}
.progress-bar i {
    display: block;
    height: 8px;
    border-radius: 4px;
    background-image: -moz-linear-gradient(left, #5ac4f1, #43fff5); /* Firefox */
    background-image: -webkit-gradient(linear, left top, right top, color-stop(0, #5ac4f1), color-stop(1, #43fff5)); /* Saf4+, Chrome */
}
.progress-text { font-size: 0.9rem;}
.self-text-wrap { padding: 20px;}
.self-text-wrap .title { padding-bottom: 20px;}
.self-text-list {}
.self-text-list label { display: block; padding: 8px;}
.gray-bg { background-color: #f5f5f5;}
.gray-dark-bg { background-color: #efefef;}
.progress-text { text-align: center;}

/*--------------------progress END---------------------------*/

.scroll-x-tab { display: table; width: 100%;}
.scroll-x-tab li { padding: 0 10px; white-space: nowrap; box-sizing: border-box;}
.scroll-x-tab li.action { border-bottom: 3px #5ac4f1 solid; color: #5ac4f1;}

#wrapper.info-wrapper { top: 60px;}
.info-content-title { padding: 0 15px; font-size: 1.2rem; color: #333; height: 40px; line-height: 40px; background-color: #efefef; border-bottom: 1px #d7d7d7 solid;}
.info-content { padding: 10px 15px; font-size: 0.9rem; line-height: 1.5;}
.fixed-bottom-bar { position: absolute; bottom: 0; height: 60px; background-color: #efefef; width: 100%; border-top: 1px #d7d7d7 solid; box-sizing: border-box;}

