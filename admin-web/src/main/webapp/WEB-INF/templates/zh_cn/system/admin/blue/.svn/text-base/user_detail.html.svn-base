<style>
	th{text-align: right; width: 110px;}
</style>
<table style="float: left; text-align: left;" width="100%" border="0" cellspacing="0" cellpadding="0">
	<tbody>
		<tr>
			<th>姓名：</th>
			<td>$!customer.name</td>
			<th>性别：</th>
			<td>#if($!customer.sex == 1) 男 #elseif($!customer.sex == 2) 女 #else 保密 #end</td>
			<th>昵称：</th>
			<td>$!customer.nickname</td>
		<tr>
			<th>手机号码：</th>
			<td>$!customer.loginName</td>
			<th>邮箱：</th>
			<td>$!customer.email</td>
			<th>生日：</th>
			<td>$!CommUtil.formatShortDate($!customer.birthdate)</td>
		</tr>
		<tr>
			<th>身份证号码：</th>
			<td>$!customer.idCard</td>
			<th>体重：</th>
			<td>$!customer.weight KG</td>
			<th>血型：</th>
			<td>$!customer.blood</td>
		</tr>
		<tr>
			<th>身高：</th>
			<td>$!customer.height MM</td>
			<th>云豆：</th>
			<td>$!customer.availableBalance</td>
			<th>积分：</th>
			<td>$!customer.integral</td>
		</tr>
		<tr>
			#if($!refreeUser)
			<th>注册人手机号码：</th>
			<td>$!refreeUser.loginName</td>
			<th>注册人昵称：</th>
			<td>$!refreeUser.nickname</td>
			#else
			<th>注册人手机号码：</th>
			<td>没有注册人</td>
			<th>注册人昵称：</th>
			<td>没有注册人</td>
			#end
			<th>隶属商户：</th>
			<td>$!customer.storeName</td>
		</tr>
		<tr>
			<th>药物过敏史家：</th>
			<td>$!customer.allergicHistory</td>
			<th>家族药物过敏史：</th>
			<td>$!customer.transHistory</td>
			<th>家族遗传病史：</th>
			<td>$!customer.transGenetic</td>
		</tr>
		<tr>
			<th>注册时间：</th>
			<td>$!CommUtil.formatShortDate($!customer.createTime)</td>
			<th>最后登录IP：</th>
			<td>$!customer.lastLoginIp</td>
			<th>最后登录时间：</th>
			<td>$!CommUtil.formatLongDate($!customer.createTime)</td>
		</tr>
		<tr>
			<th>邀请码：</th>
			<td>$!customer.invitationCode</td>
		</tr>
	</tbody>
</table>
