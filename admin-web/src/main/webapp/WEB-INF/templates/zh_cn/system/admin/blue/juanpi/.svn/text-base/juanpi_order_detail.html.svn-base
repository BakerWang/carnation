<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title></title>
<link
	href="$!webPath/resources/style/system/manage/$!{config.websiteCss}/template.css"
	rel="stylesheet" type="text/css" />
<link
	href="$!webPath/resources/style/common/css/jquery-ui-1.8.22.custom.css"
	type=text/css rel=stylesheet />
<script src="$!webPath/resources/js/jquery-1.6.2.js"></script>
<script src="$!webPath/resources/js/jquery.validate.min.js"></script>
<script src="$!webPath/resources/js/jquery.shop.common.js"></script>
<script src="$!webPath/resources/js/jquery.poshytip.min.js"></script>
<script src="$!webPath/resources/js/jquery-ui-1.8.21.js"></script>
<script src="$!webPath/resources/js/jquery.zh.cn.js" charset="utf-8"></script>
<script src="$!webPath/resources/js/smallalert.js" charset="utf-8"></script>
<style>
.juanpi_wrap{
	padding:20px;
}
.juanpi_orderinfo{
	margin-top:10px;
	margin-bottom:10px;
	overflow: hidden;
}
.juanpi_orderinfo_li{
	float:left;
	width: 350px;
	height:30px;
	line-height: 30px;
}
.juanpi_h{
	line-height: 40px;
}
</style>
<script>

</script>
</head>
<body>
	
	<div class="cont">
		<h3 class="seth">订单管理</h3>
		<div class="nav_list">
		  	<ul>
		    	<li><a href="$!webPath/admin/juanpi_order.htm"><b>所有订单</b></a></li>
		    	<li><a href="javascript:void(0)" class="this"><b>订单详情</b></a></li>
		    </ul>
		</div>
		<div class="juanpi_wrap">
			<h3>订单详情</h3>
			#foreach($obj in $objs)
			<div class="juanpi_orderinfo">
				<div class="juanpi_orderinfo_li">订单号：$obj.orderNo</div>
				<div class="juanpi_orderinfo_li">订单下单时间：$!CommUtil.formatLongDate($obj.addOrderTime)</div>
				<div class="juanpi_orderinfo_li">来源：#if($!obj.platId==1)
								#set($platidName="PC")
						    #end
						    #if($!obj.platId==2)
								#set($platidName="M站")
						    #end
						    #if($!obj.platId==3)
								#set($platidName="卷皮Android")
						    #end
						    #if($!obj.platId==4)
								#set($platidName="卷皮iPhone")
						    #end
						    #if($!obj.platId==5)
								#set($platidName="卷皮iPad")
						    #end
						    #if($!obj.platId==6)
								#set($platidName="九块邮Android")
						    #end
						    #if($!obj.platId==7)
								#set($platidName="九块邮iPhone")
						    #end
						    #if($!obj.platId==8)
								#set($platidName="九块邮iPad")
						    #end
						    #if($!obj.platId==9)
								#set($platidName="微信特卖")
						    #end
						    #if($!obj.platId==10)
								#set($platidName="安卓pad")
						    #end
						    $!platidName</div>
			</div>
			#break;
			#end
			<div class="fshoptb">
				<div class="juanpi_h">商品信息</div>
				<table width="100%" border="0" cellspacing="0" cellpadding="0"
					class="fshop_table">
					<tr>
						<th width="16%"><span class="form_btna">序号</span></th>
						<th  align="center"  width="20%"><span class="form_btna">商品名称</span></th>
						<th width="16%"><span class="form_btna">商品数量</span></th>
						<th width="16%"><span class="form_btna">商品价格</span></th>
						<th width="16%"><span class="form_btna">结算比例</span></th>
						<th width="16%"><span class="form_btna">可得佣金</span></th>
						<th width="16%"><span class="form_btna">结算状态</span></th>
					</tr>
					#set($num=0)
					#foreach($obj in $objs)
					<tr>
						#set($num=$num+1)
						<td align="center">$!num</td>
						<td align="center">$obj.goodstitle</td>
						<td align="center">$obj.goodsnumber</td>
						<td align="center">$obj.goodsmoney</td>
						<td align="center">$number.format("0.00",$!obj.brokerageratiocale)%</td>
						<td align="center">$obj.yunCommission</td>
						<td align="center">
							#if($!obj.status==1)
								#set($statusName="待处理")
						    #end
						    #if($!obj.status==2)
								#set($statusName="待结算")
						    #end
						    #if($!obj.status==3)
								#set($statusName="已结算")
						    #end
						    #if($!obj.status==4)
								#set($statusName="不可结算")
						    #end
						    $!statusName
						</td>
					</tr>
					#end	

				</table>
			</div>
		</div>
	</div>
</body>
</html>
