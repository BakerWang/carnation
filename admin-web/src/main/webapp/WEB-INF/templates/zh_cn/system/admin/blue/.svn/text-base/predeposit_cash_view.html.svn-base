<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title></title>
<link href="$!webPath/resources/style/system/manage/$!{config.websiteCss}/template.css"  rel="stylesheet" type="text/css"/>
</head>
<body>
<div class="cont">
  <h1 class="seth">云豆管理</h1>
  <div class="nav_list">
  
  <ul>
   <li><a href="$!webPath/admin/predeposit_list.htm"><b>充值管理</b></a></li>
   <li><a href="$!webPath/admin/predeposit_cash.htm"><b>提现管理</b></a></li>
<!--    <li><a href="$!webPath/admin/predeposit_modify.htm"><b>人工操作</b></a></li> -->
   <li><a href="javascript:void(0);" class="this"><b>提现申请详情</b></a></li>
  </ul>
   </div>
    <div class="edit">
   <div class="editul setcont">
     <ul class="set1">
       <li class="setcont_bg">提现编号</li>
       <li><span class="webname">
         $!obj.cashSn
       </span></li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">会员名称</li>
       <li>$!obj.nickName</li>
     </ul>
          <ul class="set1">
       <li class="setcont_bg">手机</li>
       <li>$!obj.mobile</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">提交时间</li>
       <li>$!CommUtil.formatShortDate($!obj.addTime)</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">提现方式</li>
        #if($!obj.cashPayment=="alipay")
          #set($cash_payment="支付宝")
        #elseif($!obj.cashPayment=="alipay_app")
          #set($cash_payment="支付宝")
        #elseif($!obj.cashPayment=="tenpay")
          #set($cash_payment="财付通")
        #elseif($!obj.cashPayment=="bill")
          #set($cash_payment="快钱")
        #elseif($!obj.cashPayment=="chinabank")
          #set($cash_payment="网银在线")
        #else
        	#set($cash_payment=$!obj.cashPayment) 
        #end
       <li>$!cash_payment</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">提现金额</li>
       <li>$!obj.cashAmount</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">收款账号</li>
       <li>$!obj.cashAccount</li>
     </ul>
     #if($!obj.cashPayment=="chinabank")
     <ul class="set1">
       <li class="setcont_bg">收款人姓名</li>
       <li>$!obj.cashUserName</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">汇款银行</li>
       <li>$!obj.cashBank</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">汇款支行</li>
       <li>$!obj.cashSubbranch</li>
     </ul>
     #end
     <ul class="set1">
       <li class="setcont_bg">支付状态</li>
       #if($!obj.cashPayStatus==0)
          #set($cash_pay_status="等待支付")
        #end  
        #if($!obj.cashPayStatus==1)
          #set($cash_pay_status="支付完成")
        #end       
       
       <li>$!cash_pay_status</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">提现状态</li>
      
        #if($!obj.cashStatus==0)
          #set($cash_status="等待审核")
        #end
        #if($!obj.cashStatus==1)
          #set($cash_status="审核完成")
        #end
        #if($!obj.cashStatus==-1)
          #set($cash_status="审核拒绝")
        #end
       <li>$!cash_status</li>
     </ul>
     <ul class="set1">
       <li class="setcont_bg">提现备注</li>
       <li>$!obj.cashInfo</li>
     </ul>
     #if($!obj.adminName)
     <ul class="set1">
       <li class="setcont_bg">操作管理员</li>
       <li>$!obj.cash_admin.userName</li>
     </ul>
     #end
     #if($!obj.cashAdminInfo)
     <ul class="set1">
       <li class="setcont_bg">操作备注</li>
       <li>$!obj.cashAdminInfo</li>
     </ul>
     #end
   </div>
   </div>
  <div class="submit">
    <span class="pad120"><input name="save" type="button" value="返回列表" onclick="window.location.href='$!webPath/admin/predeposit_cash.htm'" /></span>
  </div>
</div>
</body>
</html>
