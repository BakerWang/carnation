  <FORM name="ListForm" id="ListForm"  action="$!webPath/admin/reportList.htm" method="post">
  <div class="fshoptb">
    <table width="100%" border="0" cellspacing="0" cellpadding="0" class="fshop_table">
      <tr>
        <th><span class="form_btna">昵称</span></th>
        <th><span class="form_btna">手机号</span></th>
        <th><span class="form_btna">邮箱</span></th>
        <th><span class="form_btna">生日</span></th>
        <th><span class="form_btna">性别</span></th>
        <th><span class="form_btna">体检报名</span></th>
        <th><span class="form_btna">提交时间</span></th>
        <th><span class="form_btna">状态</span></th>
        <th><span class="form_btna">操作</span></th>
      </tr>
	  #foreach($obj in $objs)
      <tr>
        <td align="center">$!obj.nickname</td>
        <td align="center">$!obj.mobile</td>
        <td align="center">$!obj.email</td>
        <td align="center">$!CommUtil.formatShortDate($!obj.birthdate)</td>
        <td align="center">#if($!obj.sex == 1) 男 #else 女 #end</td>
        <td align="center">$!obj.resultName</td>
        <td align="center">$!CommUtil.formatLongDate($!obj.createTime)</td>
        <td align="center">#if($!obj.status == 1) 未提交 #elseif($!obj.status == 2) 待处理 #else 已处理 #end</td>
        <td align="center">
          <a href="$!webPath/admin/reportDetailed.htm?id=$!obj.id&self=true">查看体检报告</a>
          <!-- <a dialog_uri="$!webPath/admin/saveReportResultView.htm?id=$!obj.id" class="blue" dialog_title="处理" dialog_width="400" dialog_height="300">处理</a> -->
        </td>
      </tr>
      #end
    </table>
