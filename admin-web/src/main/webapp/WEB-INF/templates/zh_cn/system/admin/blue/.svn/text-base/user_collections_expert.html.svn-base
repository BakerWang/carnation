      </span> </div> -->
    <div class="fshoptb">
      <table width="100%" border="0" cellspacing="0" cellpadding="0" class="fshop_table">
        <tr>
          <th width="10%" align="center"><span class="form_btna">收藏人昵称</span></th>
          <th width="10%" align="center"><span class="form_btna">专家姓名</span></th>
          <th width="10%" align="center"><span class="form_btna">职称</span></th>
          <th width="6%" align="center"><span class="form_btna">性别</span></th>
          <th width="13%" align="center"><span class="form_btna">电话</span></th>
          <th width="20%" align="center"><span class="form_btna">注册邮箱</span></th>
          <th width="10%" align="center"><span class="form_btna">收藏时间</span></th>
        </tr>
        #foreach($obj in $objs)
        <tr>
        <td>$!obj.nickName</td>
          <td class="lightred">$!obj.name</td>
          <td class="lightred">$!obj.title</td>
          <td class="lightred">
          #if($!obj.sex=='1')
                              男
          #elseif($!obj.sex=='2')
                              女                    
          #end
          </td>
          <td class="lightred">$!obj.mobile</td>
          <td align="center">$!obj.email</td>
          <td align="center">$!CommUtil.formatShortDate($!obj.createTime)</td>
        </tr>
        #end
      </table>
    </div>
    <div class="operate_bar">
      <div class="fenye">
        <input type="hidden" name="currentPage" id="currentPage" value="$!currentPage" />
        <input name="mulitId" type="hidden" id="mulitId" />
        $!gotoPageFormHTML </div>
