<body>
<div class="cont">
  <h1 class="seth">体检项</h1>
  <div class="nav_list">
    <ul>
     <!--  <li><a href="$!webPath/admin/reportGroupList.htm" class="this"><b>管理</b></a></li>
      <li><a href="$!webPath/admin/reportGroupAddView.htm" ><b>新增</b></a></li> -->
    </ul>
  </div>
  <FORM name="ListForm" id="ListForm"  action="$!webPath/admin/reportGroupList.htm" method="post">
  <div class="fshoptb">
    <table width="100%" border="0" cellspacing="0" cellpadding="0" class="fshop_table">
      <tr>
        <th width="5%"><span class="form_btna">序号</span></th>
        <th width="15%"><span class="form_btna">体检项名称</span></th>
        <th width="5%"><span class="form_btna">单位</span></th>
        <th width="30%"><span class="form_btna">简介</span></th>
        <th><span class="form_btna">注意事项</span></th>
      </tr>
	  #foreach($obj in $list)
      <tr>
     	
        <td align="center">$!obj.id</td>
        <td align="center">$!obj.typeName</td>
        <td align="center">$!obj.unit</td>
        <td align="center">$!obj.profile</td>
        <td align="center">$!obj.memo</td>
        #foreach($objs in $tools.getValuationByVO($!obj.id,$!obj.id,$!obj.id,$!obj.id,$!obj.id))
      		<td align="center">$objs.rangeMax	$objs.rangeMin</td>
			<td align="center">$objs.resultKeyWord</td>
			<td align="center">$objs.memo</td>
      	#end
        </td>
      </tr>
      #end
    </table>
  </div>
  
      <div class="operate_bar">
     
    
    <div class="fenye"><input type="hidden" name="currentPage" id="currentPage" value="$!currentPage" />
      <input name="userId" type="hidden" id="userId" value="$!vo.userId" />
      <input name="nickname" type="hidden" id="nickname" value="$!vo.nickname" />
      <input name="mobile" type="hidden" id="mobile" value="$!vo.mobile" />
      <input name="startDate" type="hidden" id="sfwefws" value="$!vo.startDate" />
      <input name="endDate" type="hidden" id="ccccae" value="$!vo.endDate" />
	  $!gotoPageFormHTML</div>
             
 </div>
 </FORM>
</div>
