function export_excel(){
	#if($!objs.size()>0)	
		var order_id = jQuery("#order_id").val();
		var buyer_userName = jQuery("#buyer_userName").val();
		var beginTime = jQuery("#beginTime").val();
		var endTime = jQuery("#endTime").val();
		var order_status = jQuery("#order_status").val();
		var receiverMobile=jQuery("#receiverMobile").val();

		var ids="";
		$("input[name='checkbox']:checkbox").each(function(){
			if($(this).attr("checked")){
				ids += $(this).val()+","
			}
		});
		
		if(ids != "" && ids.length > 0){
			window.location.href="$!webPath/admin/order_excel.htm?orderId="+order_id+"&buyerUsername="+buyer_userName+"&receiverMobile="+receiverMobile+"&beginTime="+beginTime+"&endTime="+endTime+"&orderStatus="+order_status+"&ids="+ids;
		}else{
			if(beginTime!=""&&endTime!=""){
				window.location.href="$!webPath/admin/order_excel.htm?orderId="+order_id+"&buyerUsername="+buyer_userName+"&receiverMobile="+receiverMobile+"&beginTime="+beginTime+"&endTime="+endTime+"&orderStatus="+order_status+"&ids="+ids;
			}else{
				alert("请选择开始时间与结束时间！");
			}	
		}
		  	
	#else
		alert("当前没有可以导出的数据！");
	#end		
}
