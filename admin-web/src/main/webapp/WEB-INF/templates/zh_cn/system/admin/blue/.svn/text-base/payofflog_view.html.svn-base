    <div class="orderul">
      <ul>
        <li><span class="ordertitle">账单流水号:</span><span>$!obj.plSn</span></li>
        <li><span class="ordertitle"></span><span></span></li>
        #if($!obj.status==0)
          #set($status="未结算")
          #end
          #if($!obj.status==1)
          #set($status="可结算")
          #end
          #if($!obj.status==3)
          #set($status="待结算")
          #end
          #if($!obj.status==6)
          #set($status="结算完成")
          #end
        <li><span class="ordertitle">账单商家:</span><span>$!obj.seller.userName</span></li>
        <li><span class="ordertitle"></span><span></span></li>  
