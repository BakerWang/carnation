          #set($img="$!{goodsMainphotoPath}")
          #set($goodsCommissionPrice = "$!{map.get('goods_commission_price')}")
        <tr style="border-bottom:1px dotted #CCEAF4"> 
          <td><img src="$!img" width="45" height="45" /></td>
          <td><a href="$!shopPath/goods_$!{goodsId}.htm" target="_blank" class="blue">$!{goodsName}</a></td>
          <td>$!goodsGspVal</td>
          <td>¥$!goodsPrice</td>
          <td>$!goodsCount</td>
          <td>¥${goodsCommissionPrice}</td>
          <td> #if($!order.orderCat==2)¥$!obj.totalAmount #else ¥$!goodsAllPrice #end</td>
	        </tr>
        #end
      </table>
