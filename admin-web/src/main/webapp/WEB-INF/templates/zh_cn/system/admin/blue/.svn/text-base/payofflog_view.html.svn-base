          #foreach($map in $!payofflogTools.queryGoodsInfo($!goodsInfo))
          #set($goodsId = "$!{map.get('goods_id')}")	
          #set($goodsName = "$!{map.get('goods_name')}")
          #set($goodsCount = "$!{map.get('goods_count')}")
          #set($goodsPrice = "$!{map.get('goods_price')}")
          #set($goodsType = "$!{map.get('goods_type')}")
          #set($goodsAllPrice = "$!{map.get('goods_all_price')}")
          #set($goodsGspVal = "$!{map.get('goods_gsp_val')}")
