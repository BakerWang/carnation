          <td width="5%">&nbsp;</td>
          <td width="20%"><strong>商品信息</strong></td>
          <td width="10%"><strong>商品规格</strong></td>
          <td width="10%"><strong>单价</strong></td>
          <td width="10%"><strong>数量</strong></td>
          <td width="12%"><strong>商品佣金</strong></th>
          <td width="12%"><strong>商品总价</strong></th>
		  <th width="12%">退款金额</th>
        </tr>
            #set($goodsInfo="$!obj.returnGoodsInfo") 
          #foreach($map in $!payofflogTools.queryGoodsInfo($!goodsInfo))
          #set($goodsId = "$!{map.get('goods_id')}")	
          #set($goodsName = "$!{map.get('goods_name')}")
          #set($goodsCount = "$!{map.get('goods_count')}")
          #set($goodsPrice = "$!{map.get('goods_price')}")
          #set($goodsType = "$!{map.get('goods_type')}")
          #set($goodsAllPrice = "$!{map.get('goods_all_price')}")
          #set($goodsGspVal = "$!{map.get('goods_gsp_val')}")
          #set($goodsMainphotoPath = "$!{map.get('goods_mainphoto_path')}")
          #set($goodsPayoffPrice = $!{map.get('goods_payoff_price')})
          #set($img="$!{goodsMainphotoPath}")
          #set($goodsCommissionPrice = $!{map.get('goods_commission_price')})
                    #set($returnAmoutPrice = $goodsCommissionPrice - $goodsPayoffPrice )
        <tr style="border-bottom:1px dotted #CCEAF4"> 
          <td><img src="$!img" width="45" height="45" /></td>
          <td><a href="$!shopPath/goods_$!{goodsId}.htm" target="_blank" class="blue">$!{goodsName}</a></td>
          <td>$!goodsGspVal</td>
          <td>¥$!goodsPrice</td>
          <td>$!goodsCount</td>
          <td>¥${goodsCommissionPrice}</td>
          <td> #if($!order.orderCat==2)¥$!obj.totalAmount #else ¥$!goodsAllPrice #end</td>
			<td align="center">$!returnAmoutPrice</td>
	        </tr>
        #end
      </table>
    </div>
    #end


