  <div class="submit">
     #if($!obj.status==3)
    <input name="go" type="button" value="立即结算" onclick="window.location.href='$!webPath/admin/payofflog_edit.htm?id=$!{obj.id}&currentPage=$!currentPage'" />
          #end
  
    <input name="go" type="button" value="返回列表" onclick="window.location.href='$!webPath/admin/payofflog_list.htm?currentPage=$!{currentPage}&status=$!{obj.status}'" />
  </div>
</div>
