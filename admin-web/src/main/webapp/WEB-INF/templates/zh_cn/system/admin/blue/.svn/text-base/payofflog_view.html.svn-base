        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">结算时间:</span><span>$!CommUtil.formatLongDate($!obj.completeTime)</span></li>
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">销售金额:</span><span>$!obj.orderTotalPrice（元）</span></li>
        #set($order=$!payofflogTools.queryOrderInfo($!obj.orderId))
        #if($!order.couponInfo)
	        <li><span class="ordertitle"></span><span></span></li>
	        #set($coupon_map = $!orderFormTools.queryCouponInfo("$!order.couponInfo"))
      		#set($coupon_total_amount=$coupon_map.get("coupon_amount"))
	        <li><span class="ordertitle">优惠金额:</span><span>$!coupon_total_amount（元）</span></li>
        #end
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">邮费金额:</span><span>$!obj.shipAmount（元）</span></li>
        #if($!obj.returnAmount)
	        <li><span class="ordertitle"></span><span></span></li>
	        <li><span class="ordertitle">退款金额:</span><span>$!obj.returnAmount（元）</span></li>
          #end
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">销售佣金:</span><span>$!obj.commissionAmount（元）</span></li>
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">结算金额:</span><span>$!obj.totalAmount（元）</span></li>
        #if($!obj.status==6)
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">实际结算:</span><span style="color:#F00">$!obj.realityAmount（元）</span></li>
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">操作财务:</span><span>$!obj.financeUserName</span></li>
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">操作管理员:</span><span>$!obj.managerName</span></li>
        <li><span class="ordertitle"></span><span></span></li>
        <li><span class="ordertitle">结算备注:</span><span>$!obj.payoffRemark</span></li>
        #end
      </ul>
    </div>
    
    
     #set($order=$!payofflogTools.adminqueryOrderInfo($!obj.orderId))
      #if($!order.orderStatus==0)
          #set($orderStatus="已取消")
