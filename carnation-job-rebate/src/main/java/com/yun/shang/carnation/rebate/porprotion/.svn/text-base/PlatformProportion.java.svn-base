package com.yun.shang.carnation.rebate.porprotion;

import java.math.BigDecimal;

public class PlatformProportion {
	// 平台分润手续费
	private BigDecimal counterFee;
	// 供应商分润平台风险比例
	private BigDecimal supplierRisk;
	//艺龙订单分润比列
	private BigDecimal elongOrderFee;

	public BigDecimal getElongOrderFee() {
		return elongOrderFee;
	}

	public void setElongOrderFee(BigDecimal elongOrderFee) {
		this.elongOrderFee = elongOrderFee;
	}

	public BigDecimal getCounterFee() {
		return counterFee;
	}

	public void setCounterFee(BigDecimal counterFee) {
		this.counterFee = counterFee;
	}

	public BigDecimal getSupplierRisk() {
		return supplierRisk;
	}

	public void setSupplierRisk(BigDecimal supplierRisk) {
		this.supplierRisk = supplierRisk;
	}
}
