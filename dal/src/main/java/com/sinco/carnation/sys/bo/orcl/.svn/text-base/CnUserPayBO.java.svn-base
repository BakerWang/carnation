package com.sinco.carnation.sys.bo.orcl;

/**
 * CN账户奖金与重消 和 积分 请求支付结果
 * 
 * @author Administrator
 * 
 */
public class CnUserPayBO {
	private String ISPAY;// 0:不可支付,1可支付

	private String DEAL_NUMBER;// 支付交易流水号
	
	private String PAY_STATUS;//支付结果

	public String getISPAY() {
		return ISPAY;
	}

	public void setISPAY(String iSPAY) {
		ISPAY = iSPAY;
	}

	public String getDEAL_NUMBER() {
		return DEAL_NUMBER;
	}

	public void setDEAL_NUMBER(String dEAL_NUMBER) {
		DEAL_NUMBER = dEAL_NUMBER;
	}

	public String getPAY_STATUS() {
		return PAY_STATUS;
	}

	public void setPAY_STATUS(String pAY_STATUS) {
		PAY_STATUS = pAY_STATUS;
	}
	
	

}
