	<select id="findByVo" resultMap="BOResultMap">
		SELECT
		sz.*,
		sg.goods_main_photo_path goodsMainPhotoPath,
		sg.goods_current_price goodsCurrentPrice,
		sg.goods_price oldGoodsPrice

		FROM
		shop_zero_goods sz
		LEFT JOIN shop_goods sg
		ON sz.goods_id = sg.id
		where 1=1
		<if test="vo.goodsStatus != null">
			AND sz.goods_status = #{vo.goodsStatus}
		</if>
		ORDER BY goods_inventory DESC,
		goods_salenum DESC
	</select>

	<select id="findPageByVo" resultMap="BOResultMap">
		SELECT sg.goods_main_photo_path,sg.store_price,sg.goods_price
		old_goods_price,sg.self_add_price,sg.goods_current_price,sg.goods_inventory
		all_inventory,
		zg.*
		FROM `shop_zero_goods` zg
		INNER JOIN `shop_goods` sg ON sg.id = zg.goods_id
		where sg.delete_status = false
		<if test="vo.goodsName != null">
			and zg.goods_name like CONCAT('%',#{vo.goodsName},'%')
		</if>
