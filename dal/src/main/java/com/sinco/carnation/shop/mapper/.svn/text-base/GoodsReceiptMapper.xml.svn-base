	<select id="findGoodsReceiptList" resultMap="BOResultMap">
		SELECT gc.*,u.nickname AS buyerName,us.`name` AS sellerName,so.`add_time` as
		registerTime
		FROM shop_goods_receipt AS gc
		LEFT JOIN `user_customer` AS u ON u.`uid` = gc.`user_id`
		LEFT JOIN `user_seller` AS us ON us.`store_id` = gc.`store_id`
		LEFT JOIN shop_order_form AS so ON so.`id` = gc.`of_id`
		WHERE gc.`delete_status` = 0
		AND gc.goods_id = #{goodsId,jdbcType=BIGINT}
		ORDER BY so.`add_time` DESC
	</select>

