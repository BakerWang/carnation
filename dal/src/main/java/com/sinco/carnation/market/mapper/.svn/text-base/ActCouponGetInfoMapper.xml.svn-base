	<select id="findAllByPage" resultMap="BOResultMap">
		SELECT a.*,uc.mobile,uc.name,uc.nickname,ye.scheme_name
		FROM act_new_year_info a LEFT JOIN act_app_new_year ye ON
		a.act_coupon_id=ye.id LEFT JOIN user_customer uc ON a.user_id = uc.uid
		WHERE a.delete_status = 0 and ye.delete_status=0

		<if test="vo.startGetTimeStr != null  and vo.startGetTimeStr != '' ">
			AND a.get_time <![CDATA[ >= ]]>
			#{vo.queryStartTime}
		</if>
		<if test="vo.endGetTimeStr != null  and vo.endGetTimeStr != '' ">
			AND a.get_time <![CDATA[ <= ]]>
			#{vo.queryEndTime}
		</if>
		<if test="vo.isGet != null  and vo.isGet != '' ">
			AND a.is_get = #{vo.isGet}
		</if>
		<if test="vo.mobile != null  and vo.mobile != '' ">
			AND uc.mobile like "%"#{vo.mobile}"%"
		</if>
		<if test="vo.name != null  and vo.name != '' ">
			AND uc.name like "%"#{vo.name}"%"
		</if>
		<if test="vo.nickName != null  and vo.nickName != '' ">
			AND uc.nickname like "%"#{vo.nickName}"%"
		</if>
		and a.act_coupon_id = #{vo.actCouponId}
		ORDER BY a.get_time DESC
