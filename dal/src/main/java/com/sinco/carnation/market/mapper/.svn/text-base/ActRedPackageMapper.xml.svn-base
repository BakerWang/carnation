	<select id="findAllByPage" resultMap="BOResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM act_red_package
		WHERE delete_status = 0
		<if test="vo.schemeName != null and vo.schemeName != '' ">
			AND scheme_name = #{vo.schemeName}
		</if>
		<if test="vo.terminalType != null and vo.terminalType != -1">
			AND terminal_type =#{vo.terminalType}
		</if>
		<if test="vo.actPosition != null  and vo.actPosition != -1">
			AND act_position = #{vo.actPosition}
		</if>
		<if test="vo.startTimeStr != null  and vo.startTimeStr != '' ">
			AND create_time <![CDATA[ >= ]]>
			#{vo.startTime}
		</if>
		<if test="vo.endTimeStr != null  and vo.endTimeStr != '' ">
			AND create_time <![CDATA[ <= ]]>
			#{vo.endTime}
		</if>
		<choose>
			<!-- 1-未开始,2-进行中 ,3-已结束 -->
			<when test="vo.status == 1">
				and start_time <![CDATA[ > ]]>
				now()
			</when>
			<when test="vo.status == 2">
				and start_time <![CDATA[ <= ]]>
				now() and end_time <![CDATA[ >= ]]>
				now()
				and current_open_count <![CDATA[ < ]]>
				total_count
				and is_enabled = 1
			</when>
			<when test="vo.status == 3">
				and (
				end_time <![CDATA[ < ]]>
				now()
				or current_open_count <![CDATA[ >= ]]>
				total_count
				)
			</when>
			<when test="vo.status == 4">
				and is_enabled = 0
			</when>
			<when test="vo.status == 5">
				and is_enabled = 1
			</when>
		</choose>


		ORDER BY priority DESC
	</select>

