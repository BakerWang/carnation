	<select id="findListByVo" resultMap="BOResultMap">
		SELECT
		msg.`id`,
		msg.`title`,
		msg.`notice_title`,
		msg.`receive_type`,
		msg.`content`,
		msg.`receive_location`,
		msg.`priority`,
		msg.`top_index`,
		msg.`send_count`,
		msg.`send_status`,
		msg.`send_time`,
		msg.`delete_status`,
		msg.`create_by`,
		msg.`create_time`,
		msg.`modify_by`,
		msg.`modify_time`,
		u.name AS createName
		FROM msg_info AS
		msg
		LEFT JOIN user_manager AS u ON msg.`create_by` = u.`uid`
		WHERE
		msg.delete_status = 0
		<if test="vo.title != null and vo.title != '' ">
			AND msg.title = #{vo.title}
		</if>
		<if test="vo.receiveLocation != null and vo.receiveLocation != -1">
			AND msg.receive_location =#{vo.receiveLocation}
		</if>
		<if test="vo.receiveType != null  and vo.receiveType != -1">
			AND msg.receive_type = #{vo.actPosition}
		</if>
		<if test="vo.sendStatus != null  and vo.sendStatus != -1">
			AND msg.send_status = #{vo.sendStatus}
		</if>
		<if test="vo.topIndex != null  and vo.topIndex != -1">
			AND msg.top_index = #{vo.topIndex}
		</if>
		<if test="vo.startTimeStr != null  and vo.startTimeStr != '' ">
			AND msg.send_time <![CDATA[ >= ]]>
			#{vo.startTimeStr}
		</if>
		<if test="vo.endTimeStr != null  and vo.endTimeStr != '' ">
			AND msg.send_time <![CDATA[ <= ]]>
			#{vo.endTimeStr}
		</if>
		<if test="vo.createName != null  and vo.createName != '' ">
			AND u.name = #{vo.createName}
		</if>
		ORDER BY msg.top_index DESC,msg.priority DESC,msg.send_time DESC
	</select>

