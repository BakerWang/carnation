	<select id="getActRedPacketRecordUserList" parameterType="com.sinco.carnation.market.vo.RedPackageGetRecordVO"
		resultMap="BOResultMap">
		SELECT a.money,uc.head_img_path
		FROM red_package_get_record AS a
		LEFT
		JOIN `act_red_package` arp ON a.`package_id`= arp.`id`
		LEFT JOIN
		user_customer uc ON a.user_id = uc.uid
		WHERE 1=1
		<if test="status != null">
			AND a.STATUS = #{status,jdbcType=TINYINT}
		</if>
		<if test="vcode != null">
			AND arp.vcode = #{vcode,jdbcType=VARCHAR}
		</if>
		ORDER BY get_time DESC LIMIT 12
	</select>

