	<!-- 查询sql语句 -->
	<select id="findAllByPage" resultMap="BOResultMap">
		SELECT red.package_id, red.phone,
		sys_user.invitation_code,
		user_customer.nickname,
		red.get_time,red.money,
		red.STATUS,
		red.ip
		FROM
		red_package_get_record AS red LEFT JOIN user_customer AS
		user_customer
		ON red.phone = user_customer.mobile LEFT JOIN sys_user
		ON
		user_customer.uid = sys_user.id LEFT JOIN act_red_package
		ON
		red.package_id = act_red_package.id
		WHERE red.delete_status = 0
		<if test="vo.packageId != null and vo.packageId !=0  ">
			and red.package_id = #{vo.packageId}
		</if>
		<if test="vo.status == 0 ">
			and red.status = #{vo.status}
		</if>
		<if test="vo.status != null and vo.status != '' and vo.status != -1 ">
			and red.status = #{vo.status}
		</if>
		<if test="vo.createdTimeStr != null and vo.createdTimeStr != '' ">
			and red.created_time <![CDATA[ >= ]]>
			#{vo.createdTimeStr}
		</if>
		<if test="vo.offTimeStr != null and vo.offTimeStr != '' ">
			and red.off_time <![CDATA[ <= ]]>
			#{vo.offTimeStr}
		</if>
		<if
			test="vo.choose != null and vo.choose != '' and vo.mation != null and vo.mation != '' ">
			<if test="vo.choose == 3">
				and user_customer.nickname = #{vo.mation}
			</if>
			<if test="vo.choose == 1">
				and red.phone= #{vo.mation}
			</if>
			<if test="vo.choose == 2">
				and sys_user.invitation_code= #{vo.mation}
			</if>
		</if>
		ORDER BY red.package_id,red.phone,red.get_time DESC
	</select>

