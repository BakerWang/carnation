	<select id="search" resultMap="BOResultMap">
		select * from check_results_reply rr, user_customer uc where
		rr.create_by=uc.uid
		<if test="vo.userId != null">
			and uc.uid=#{vo.userId}
		</if>
		<if test="vo.status != null">
			and rr.status=#{vo.status}
		</if>
		<if test="vo.nickname != null and vo.nickname != ''">
			and uc.nickname=#{vo.nickname}
		</if>
		<if test="vo.mobile != null and vo.mobile != ''">
			and uc.mobile=#{vo.mobile}
		</if>
		<if test="vo.startDate != null and vo.startDate != ''">
