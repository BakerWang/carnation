	<select id="queryCircleInvitation" resultMap="QueryCircleInvitationMap">
		SELECT u.head_img_path,
		<include refid="Invitation_Column_List" />
		from sns_invitation a
		LEFT JOIN user_customer_favorites f ON f.fid=a.id
		LEFT JOIN user_customer u ON u.uid=a.user_id
		WHERE f.uid=#{uid,jdbcType=INTEGER} AND f.type=#{type,jdbcType=INTEGER} AND
		a.circle_type=#{circleType,jdbcType=INTEGER}
		<if test="timeStamp != null and timeStamp != ''">
			and a.update_time <![CDATA[<]]>
			#{timeStamp,jdbcType=TIMESTAMP}
		</if>
		ORDER BY a.update_time DESC
	</select>


