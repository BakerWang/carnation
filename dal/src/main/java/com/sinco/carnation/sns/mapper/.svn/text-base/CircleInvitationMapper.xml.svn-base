	<update id="updateInvitationPraise">
		UPDATE sns_invitation
		SET praise_count = praise_count + 1,
		praise_Info = CONCAT(praise_Info, #{praiseInfo,jdbcType=VARCHAR}, ',')
		WHERE
		id = #{id,jdbcType=BIGINT}
	</update>

	<update id="updateInvitationUnPraise">
		UPDATE sns_invitation
		SET praise_count = praise_count - 1,
		praise_Info = #{praiseInfo,jdbcType=VARCHAR}
		WHERE
		id = #{id,jdbcType=BIGINT}
	</update>

	<update id="updateInvitationUnFavo">
