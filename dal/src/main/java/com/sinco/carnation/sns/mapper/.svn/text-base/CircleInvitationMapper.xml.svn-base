	<select id="selectInvitationDetail" resultMap="QueryCircleInvitationMap">
		SELECT
		*
		FROM
		sns_invitation i
		left join user_customer u on i.user_id = u.uid
		WHERE
		i.delete_Status = 0
		AND i.id = #{invitationId}
		order by i.add_Time desc
	</select>

	<select id="queryByIds" resultMap="QueryCircleInvitationMap">
		SELECT
		i.id,i.title
		FROM
		sns_invitation i
		WHERE
		i.delete_Status = 0
		AND i.id
		in
		<foreach collection="ids" index="index" item="id" open="("
			separator="," close=")">
			#{id}
		</foreach>
	</select>

	<select id="selectInvitationByClassId" resultMap="BaseResultMap">
		SELECT
		i.*
		FROM
		sns_invitation i,
		sns_invitation_r_class r
		WHERE
		i.delete_Status = 0
		AND i.id = r.invitation_id
		AND r.class_id = #{classId}
		ORDER BY
		r.create_time DESC
	</select>
	<select id="selectPageByVO" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from sns_invitation
		where delete_Status = 0 AND circle_type=1
		<if test="vo.title != null and vo.title != ''">
			and title like concat('%', #{vo.title}, '%')
		</if>
		<if test="vo.userName != null and vo.userName != ''">
