	<select id="select_today_HealthTaskRelation" resultMap="BOResultMap">
		select
		<include refid="Base_Column_List" />
		from
		user_healthtask_relation
		where
		1=1
		and
		is_deleted = 0
		<if test="userID!=null">
			and
			user_id = #{userID,jdbcType=BIGINT}
		</if>
		and
		user_healthTask_id in
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item}
		</foreach>
	</select>

	<select id="getOne_HealthTaskRelation_By_uid_taskID" resultMap="BOResultMap">
		select
		<include refid="Base_Column_List" />
		from
		user_healthtask_relation
		where
		1=1
		<!-- and is_deleted = 0 -->
		and
		user_id = #{uid,jdbcType=BIGINT}
		and
		user_healthTask_id = #{taskID,jdbcType=BIGINT}
	</select>
	<!-- 用户健康任务签到 -->
	<update id="update_HealthTask_Sing">
		update
		user_healthtask_relation
		set
		do_days = do_days+1,
		finish_time = #{finishTime,jdbcType=VARCHAR}
		where
		1=1
		and
		id = #{id,jdbcType=BIGINT}
		and
		user_id = #{uid,jdbcType=BIGINT}
		and
		user_healthTask_id = #{taskID,jdbcType=BIGINT}
		and
		is_deleted = 0
	</update>

	<delete id="deleteHealthtaskRelation">
