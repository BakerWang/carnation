	<select id="checkGroupSn" resultMap="BOResultMapEmpty">
		SELECT i.* FROM
		o2o_group_info i
		INNER JOIN o2o_group_order o ON i.order_id=o.id
		INNER
		JOIN shop_store s ON s.id=o.store_id
		WHERE
		s.create_user_id=#{sellerUid} AND i.group_sn=#{groupSn}
	</select>


	<select id="checkMultiGroupSn" resultMap="BOResultMapEmpty">
		SELECT i.* FROM o2o_group_info i
		INNER JOIN o2o_group_order o ON
		i.order_id=o.id
		INNER JOIN shop_store s ON s.id=o.store_id
		WHERE
		s.create_user_id=#{sellerUid} AND i.group_sn in
		<foreach collection="groupSnList" item="groupSn" index="index"
			open="(" close=")" separator=",">
			#{groupSn}
		</foreach>
	</select>

	<select id="findExpiredGroupInfo" resultMap="BOResultMapEmpty">
		SELECT i.*
		FROM o2o_group_info i
		WHERE 1=1
		and i.status=0
    <![CDATA[AND DATE_FORMAT(i.end_time,'%Y-%m-%d') < DATE_FORMAT(now(),'%Y-%m-%d')]]>
		<!-- <![CDATA[AND DATE_FORMAT(i.end_time,'%Y-%m-%d') > #{startTime,jdbcType=VARCHAR}]]> -->
		<!-- <![CDATA[AND DATE_FORMAT(i.end_time,'%Y-%m-%d') < #{endTime,jdbcType=VARCHAR}]]> -->
	</select>

	<select id="queryGroupInfoList" resultMap="BOResultMap">
		SELECT
		i.id,i.group_sn,i.refund_reasion,i.`status`,i.`proportion` FROM
		o2o_group_info i
		LEFT JOIN o2o_group_goods g ON g.id=i.goods_id WHERE
		i.order_id=#{orderId,jdbcType=BIGINT}
	</select>

	<select id="findPageByStoreId" resultMap="ReturnBOResultMap">
		select info.goods_id,
		ord.order_id as orderStr,
		goods.gg_name,
		ord.group_info,
		info.group_sn,
		ord.user_nickName,
		info.status
		from
		o2o_group_order ord
		LEFT JOIN
		o2o_group_info info
		on ord.id =
		info.order_id
		LEFT JOIN
		o2o_group_goods goods
		on info.goods_id = goods.id
		where 1=1 and info.status>1
		<if test="vo.storeId != null ">
			AND ord.store_id=#{vo.storeId}
		</if>
		<if test="vo.groupName != null and vo.groupName != ''">
			AND goods.gg_name like concat('%',#{vo.groupName},'%')
		</if>
		<if test="vo.orderId != null and vo.orderId != ''">
			AND ord.order_id like concat('%',#{vo.orderId},'%')
		</if>
		<if test="vo.userNickName != null and vo.userNickName!= ''">
			AND ord.user_nickname like
			concat('%',#{vo.userNickName},'%')
		</if>
		order by info.add_time desc
	</select>
