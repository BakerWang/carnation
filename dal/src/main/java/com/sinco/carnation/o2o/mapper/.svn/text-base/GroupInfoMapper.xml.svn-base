	<select id="selectByApply" resultMap="BOResultMapEmpty">
		select gi.*, uc.nickname, ss.store_name,go.order_id as orderStr from
		o2o_group_info gi
		left join user_customer uc on uc.uid=gi.user_id
		left
		join o2o_group_order go on go.id=gi.order_id
		left join shop_store ss on
		ss.id=go.store_id
		<trim prefix="WHERE" prefixOverrides="and|or">
			<if test="vo.status == null">
				and gi.status<![CDATA[>=]]>3
			</if>
			<if test="vo.status != null and vo.status == 3">
				and gi.status=3
			</if>
			<if test="vo.status != null and vo.status == 5">
				and gi.status=5
			</if>
			<if test="vo.status != null and vo.status == 6">
				and gi.status=6
			</if>
			<if test="vo.status != null and vo.status == 7">
				and gi.status=7
			</if>
			<if test="vo.nickName != null and vo.nickName != ''">
				and uc.nickname=#{vo.nickName}
			</if>
			<if test="vo.groupName != null and vo.groupName != ''">
				and go.group_info like "%"#{vo.groupName}"%"
			</if>
			<if test="vo.beginTimeStr != null and vo.beginTimeStr != ''">
				AND DATE_FORMAT(gi.add_time,'%Y-%m-%d') <![CDATA[ >= ]]>
				DATE_FORMAT(#{vo.beginTimeStr},'%Y-%m-%d')
			</if>
			<if test="vo.endTimeStr != null and vo.endTimeStr != ''">
				AND DATE_FORMAT(gi.add_time,'%Y-%m-%d') <![CDATA[ < ]]>
				DATE_FORMAT(#{vo.endTimeStr},'%Y-%m-%d')
			</if>
		</trim>
		order by gi.status, gi.id desc
	</select>

	<select id="selectByRefund" resultMap="ReturnBOResultMap">
		select gi.*, uc.nickname, ss.store_name, um.name refundName
		from
		o2o_group_info gi
		left join user_customer uc on uc.uid=gi.user_id
		left
		join o2o_group_order go on go.id=gi.order_id
		left join shop_store ss on
		ss.id=go.store_id
		left join user_manager um on um.uid=gi.refund_id
		where 1=1
		<choose>
			<when test="vo.status != null and vo.status == 57">
				and (gi.status=5 or gi.status=7)
			</when>
			<otherwise>
				and gi.status=#{vo.status}
			</otherwise>
		</choose>
		<if test="vo.nickName != null and vo.nickName != ''">
			and uc.nickname=#{vo.nickName}
		</if>
		<if test="vo.groupName != null and vo.groupName != ''">
			and go.group_info like "%"#{vo.groupName}"%"
		</if>
		<if test="vo.localReturnStatus != null">
			and go.local_return_status=#{vo.localReturnStatus}
		</if>
		<if test="vo.orderType != null and vo.orderType != ''">
			and go.order_type=#{vo.orderType}
		</if>
		<if test="vo.storeName != null and vo.storeName != ''">
			and ss.store_name like concat('%',#{vo.storeName},'%')
		</if>
		<if test="vo.userNickName != null and vo.userNickName != ''">
			and uc.nickname like concat('%',#{vo.userNickName},'%')
		</if>
		<if test="vo.groupSn != null and vo.groupSn != ''">
			and gi.group_sn=#{vo.groupSn}
		</if>
		<if test="vo.startDate != null and vo.startDate != ''">
			and #{vo.startDate}<![CDATA[<=]]>date(gi.refund_complete_time)
		</if>
		<if test="vo.endDate != null and vo.endDate != ''">
			and date(gi.refund_complete_time)<![CDATA[<=]]>#{vo.endDate}
		</if>
		order by gi.status, gi.id desc
	</select>

	<select id="selectGroupGoodsByOrderId" resultMap="BOResultMap">
		select gi.*, gg.gg_name groupGoodsBO_gg_name
		from o2o_group_order go
		left join o2o_group_info gi on go.id=gi.order_id
		left join
		o2o_group_goods gg on gg.id=gi.goods_id
		where 1=1
		<if test="vo.orderId != null and vo.orderId != ''">
			and go.id = #{vo.orderId,jdbcType=VARCHAR}
		</if>
		order by gi.add_time asc
	</select>

