package com.sinco.carnation.weChat.model;

import com.sinco.mybatis.dal.model.BaseModel;

public class WeChatAuthInfo extends BaseModel {
	/**
	 * 
	 */
	private static final long serialVersionUID = -8314125014503800506L;

	/** ID **/
	private Long id;

	/** 用户OpenId **/
	private String openid;

	/** 用户ID **/
	private Long userId;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getOpenid() {
		return openid;
	}

	public void setOpenid(String openid) {
		this.openid = openid;
	}

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}
}