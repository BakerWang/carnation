html,body{font-family: sans-serif}
a,button,input,canvas{-webkit-tap-highlight-color:rgba(0,0,0,0)}
.top{background: #003894;width: 100%;border-bottom: solid 2px #00baff}
.top .main{width: 1010px;height: 76px;margin: 0 auto}
.top .main .logo{float: left;width: 192px;height: 52px;margin: 12px 0 0 0}
.top .main .domain{float:right;width: 127px;height: 34px;margin:21px 0 0 0;background: #003fa7;color: #fff;text-align: center;line-height: 34px;border-radius: 14px;font-size: 14px }
.center{width: 100%}
.center .main{width: 1010px;margin: 0 auto;overflow: hidden}
.center .main .three{margin: 34px 0 0 0;overflow: hidden}
.center .main .three ul{width: 1020px;overflow: hidden}
.center .main .three ul li{width: 328px;float: left;height: 466px;border: solid 1px #dfdfdf;margin-right: 10px}
.center .main .three ul li:hover{border: solid 1px #00baff}
.center .main .three ul li a img{width: 328px;height: 325px}
.center .main .three ul li a p.tit{text-align: center}
.center .main .three ul li a p.ch{font-size: 18px;font-family: "Microsoft YaHei";color: #23282c;margin-top: 14px;height: 28px;line-height: 28px}
.center .main .three ul li a p.en{font-size: 12px;font-family: "Microsoft YaHei";color: #ffa303;height: 16px;line-height: 16px}
.line{width: 308px;height: 10px;margin: 0 auto;margin-top: 6px;background: url(../images/line.png)}
.center .main .three ul li a p.des{color: #828282;font-size: 13px;font-family: "Microsoft YaHei";width: 312px;margin: 0 auto;line-height: 22px;margin-top: 10px}
.two{width: 100%;background: #f6f6f6;height: 340px;margin: 34px 0 0 0;overflow: hidden}
.two .main{width: 1010px;margin: 0 auto;margin-top: 26px}
.two .main ul{width: 1030px;overflow: hidden;}
.two .main ul li{width: 497px;float: left;height: 288px;border: solid 1px #dfdfdf;margin-right: 13px}
.two .main ul li:hover{border: solid 1px #00baff}
.two .main ul li a{display: block}
.two .main ul li a img{vertical-align: top;width: 497px;height: 176px}
.two .main p{width: 460px;margin: 0 auto}
.two .main p.ch{font-size: 18px;font-family: "Microsoft YaHei";color: #23282c;margin-top: 10px;height: 28px;line-height: 28px}
.two .main p.en{font-size: 12px;font-family: "Microsoft YaHei";color: #ffa303;height: 16px;line-height: 16px}
.two .main p.des{color: #828282;font-size: 13px;font-family: "Microsoft YaHei";line-height: 22px;margin-top: 4px}
.bottom{width: 100%;text-align: center;font-size: 14px;font-family: "Microsoft YaHei";color: #858585;height: 54px;line-height: 54px}

.mtop{background: #003894;width: 100%;border-bottom: solid 2px #00baff;height:40px}
.mtop .logo{float: left;width: 120px;height: 32px;margin: 4px 0 0 15px}
.mtop .domain{float:right;width: 90px;height: 20px;margin:10px 15px 0 0;background: #003fa7;color: #fff;text-align: center;line-height: 20px;border-radius: 10px;font-size: 12px }
.section{background: #e7e7e7;width: 90%;margin: 10px 5% 0 5%;overflow: hidden;display: block}
.section a{display: block}
.section a img{display: inline-block;vertical-align: top;width: 100%}
.section .tit{text-align: center}
.section .ch{font-size: 16px;color: #23282c;margin-top: 10px}
.section .en{font-size: 10px;color: #ffa303;margin-top: 4px}
.section .mline{width: 90%;margin: 5px auto;display: block;vertical-align: top}
.section .des{width: 90%;margin: 0 auto;font-size: 12px;color: #717171;margin-bottom: 10px}