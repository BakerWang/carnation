.cont-width,body { min-width: 1002px;}
    .nav-collapse {
        font-size: 16px;
        overflow: hidden;
    }

    .nav-collapse ul {
        width: 690px;
    }

    .nav-collapse li {
        width: auto;
    }

    .nav-collapse a {
        padding: 0.5em 1em;
        color: #555;
        background-color: transparent;
        border: none;
        border-bottom: 2px transparent solid;
    }

    .nav-collapse a:hover, .nav-collapse a.active {
        color: #19bf96;
        border-bottom: 2px #19bf96 solid;
    }
    .slide-05 .slide-pic-box .main-pic { max-width: 330px;}

    .footer a { float: none; padding: 0;}
    
    .col-md-12 { width: 100%;}.col-md-6 { width: 50%;}.col-md-4 { width: 33.33%;}.col-md-3 { width: 25%;}.col-md-2 { width: 16.66%;}
    .col-md-1,.col-md-2,.col-md-3,.col-md-4,.col-md-5,.col-md-6,.col-md-7,.col-md-8,.col-md-9,.col-md-10,.col-md-11,.col-md-12 { float: left; padding-right: 0;}