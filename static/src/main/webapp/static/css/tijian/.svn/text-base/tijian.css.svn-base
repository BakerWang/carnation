.quickTj .quickBtn{border-radius: 50%;width: 4rem;height: 4rem;position: relative;}
.quickTj .quickBtn a{color: #fff;font-size: 0.46rem;width: 4rem;height: 4rem;border-radius: 50%;text-align: center;line-height: 5.5rem;display: block;
background:#119aee url(../img/tj-icon@3x.png) no-repeat center 0.9rem;background-size: 1rem 1.3rem;position: relative;z-index: 3;
}
