.pageholder .tjList{border-top: solid 1px #f0f0f0}
.tjList em{display: block;width: 100%;background: #F7F7F7;height: 0.12rem;}
.tjList ul{font-size: 0;}
.tjList ul li{width: 50%;height: 1.4rem;border: solid 1px #F7F7F7;box-sizing: border-box;display: inline-block;border-left: none;border-top: none;}
.tjList ul li a{display: block;line-height: 1.4rem;text-indent: 1.5rem;
font-size:0.28rem;
color:#626262;
letter-spacing:0.58px;
text-align:left;}
.tjList ul li:nth-child(1) a{
	background: url(../img/icon-xueya@3x.png) no-repeat 0.56rem center;background-size:0.52rem 0.8rem ;
}
.tjList ul li:nth-child(2) a{
	background: url(../img/icon-xinlv@3x.png) no-repeat 0.54rem center;background-size: 0.8rem 0.72rem;
}
.tjList ul li:nth-child(3) a{
	background: url(../img/icon-feihuoliang@3x.png) no-repeat 0.4rem center;background-size: 0.8rem 0.74rem;
}
.tjList ul li:nth-child(4) a{
	background: url(../img/icon-xueyang@3x.png) no-repeat 0.64rem center;background-size: 0.54rem 0.8rem;
}
.tjList ul li:nth-child(4) a{
	background: url(../img/icon-xueyang@3x.png) no-repeat 0.64rem center;background-size: 0.54rem 0.8rem;
}
.tjList ul li:nth-child(5) a{
	background: url(../img/icon-shili@3x.png) no-repeat 0.4rem center;background-size: 0.8rem 0.44rem;
}
.tjList ul li:nth-child(6) a{
	background: url(../img/icon-tingli@3x.png) no-repeat 0.5rem center;background-size: 0.82rem 0.68rem;
}
.tjList ul li a.gzss{
	background: url(../img/gzss.png) no-repeat 0.5rem center;background-size: 0.8rem 0.8rem;
}
.tjList ul li a.tnb{
	background: url(../img/tnb.png) no-repeat 0.5rem center;background-size: 0.8rem 0.8rem;
}

.tjList ul li:active{background: rgba(0,0,0,0.04);}



