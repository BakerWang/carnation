ul,li{list-style-type: none}
.tiezi{margin-left: 10px;overflow: hidden}
.tiezi li{border-bottom: solid 1px #dcdcdc;position: relative;height: 63px;width: 100%;float: left}
.tiezi li img{width: 47px;height: 47px;border-radius: 5px;position:absolute;left:0;top:8px  }
.tiezi li .content{margin-left: 57px}
.tiezi li .content .tit{font-size: 15px;color: #333;line-height: 20px;width: 100%;overflow: hidden;margin-top: 5px}
.tiezi li .content .bot p{height: 33px;font-size: 12px;color: #999;line-height: 16px;overflow: hidden;padding-right: 60px}
.tiezi li .content .bot span{position: absolute;right: 10px;bottom: 8px;font-size: 12px;color: #999}
.goods{overflow: hidden;margin-top: 5px}
.goods li{width: 50%;float: left}
.goods li a div.goodsdiv{width: 100%;border: solid 1px #e3e3e3;border-top: none;border-left: none;text-align: center;overflow: hidden}
.goods li:nth-last-child(1) div.goodsdiv,.goods li:nth-last-child(2) div.goodsdiv {border-bottom: none}
.goods li div img{width: 90px;height: 90px;margin: 7px 0 7px 0;display: block;vertical-align: top;display: inline-block}
.goods li div p.tit{text-align: left;height: 36px;line-height: 18px;overflow: hidden;margin: 0 5px 0 10px}
.goods li div span.price{font-size: 16px;color: #fc1818;text-align: left;display: block;margin: 6px 0px 6px 10px}
.fn-rmb {font-family: arial;font-style: normal;padding-right: 4px}
.service{overflow: hidden}
.service li{float: left;height: 76px;margin: 0 0 0 10px;border-bottom: solid 1px #dcdcdc;position: relative;width: 100%}
.service li img{position: absolute;width: 60px;height: 60px;left: 0;top: 8px}
.service li .right{margin-left: 70px;position:relative;padding-right: 20px}
.service li .right .tit{font-size: 15px;height: 15px;margin-top: 12px;overflow: hidden}
.service li .right .star{color: #ffab34;overflow: hidden}
