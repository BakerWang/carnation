}
.smalltips .content{display:inline-block;margin:0 auto}   
.opacityModel{position: fixed;background:#fff;opacity:0;width: 100%;height: 100%;left: 0;top:0;display:none;z-index:999999999999998}
.paying{position: fixed;color: #fff;width: 100px;height: 100px;left: 50%;top:50%;margin: -50px 0 0 -50px;display:none;z-index:999999999999999}
.paying .bg{width: 100px;height:100px;border-radius: 10%;background: #000;opacity: 0.5;
    filter: alpha(opacity=50)}
.paying .des{position: absolute;width: 100px;height: 100px;line-height: 100px;text-align: center;left: 0;top: 0;font-size: 20px}
.paying .cirContainer{position: absolute;bottom: 14px;overflow: hidden;z-index: 2;width: 100%;text-align: center}
.paying .cirContainer .cir{display: inline-block;width: 10px;height: 10px;border-radius: 50%;background: #fff;opacity: 0.5;margin-left: 8px;transform-style: preserve-3d;-webkit-transform-style: preserve-3d;}
.paying .cirContainer .fir{margin-left: 0}
.paying .cirContainer .cir:nth-child(0) {
    -webkit-animation: scale 0.75s 0s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: scale 0.75s 0s infinite cubic-bezier(.2, .68, .18, 1.08); }
.paying .cirContainer .cir:nth-child(1) {
    -webkit-animation: scale 0.75s 0.12s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: scale 0.75s 0.12s infinite cubic-bezier(.2, .68, .18, 1.08); }
.paying .cirContainer .cir:nth-child(2) {
    -webkit-animation: scale 0.75s 0.24s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: scale 0.75s 0.24s infinite cubic-bezier(.2, .68, .18, 1.08); }
.paying .cirContainer .cir:nth-child(3) {
    -webkit-animation: scale 0.75s 0.36s infinite cubic-bezier(.2, .68, .18, 1.08);
    animation: scale 0.75s 0.36s infinite cubic-bezier(.2, .68, .18, 1.08); }
@-webkit-keyframes scale {
    0% {
        -webkit-transform: scale(1) translateZ(0);
        transform: scale(1) translateZ(0);
        opacity: 1; }

    45% {
        -webkit-transform: scale(0.1) translateZ(0);
        transform: scale(0.1);
        opacity: 0.5; }

    80% {
        -webkit-transform: scale(1);
        transform: scale(1);
        opacity: 1; } }
@keyframes scale {
    0% {
        -webkit-transform: scale(1);
        transform: scale(1);
        opacity: 1; }

    45% {
        -webkit-transform: scale(0.1);
        transform: scale(0.1);
        opacity: 0.5; }

    80% {
        -webkit-transform: scale(1);
        transform: scale(1);
        opacity: 1; } }















