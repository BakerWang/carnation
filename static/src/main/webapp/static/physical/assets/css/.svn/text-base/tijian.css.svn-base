.service li .right .pos{position: absolute;right: 20px;bottom: 0;color: #858585;font-size: 12px;text-align: right}
.service li .right .pos p{line-height: 15px}
.service li .right .pos p.icon_p{margin-bottom: 5px;display: inline-block;height: 14px;padding-left: 13px;background: url("data:image/png;base64,") no-repeat left bottom}

.history_wrap{background: #fff;width: 100%;overflow: hidden;margin-top:10px;border-bottom: solid 1px #dcdcdc }
.history_wrap .tit{height: 40px;line-height: 40px;font-size: 14px;padding:0 0 0 10px;border:solid 1px #dcdcdc;border-left: none;border-right: none}
.history_wrap .tit .total{display: inline-block;width: 42%}
