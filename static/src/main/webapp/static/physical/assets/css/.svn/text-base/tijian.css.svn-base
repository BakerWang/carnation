.taskTime{border-top:solid 1px #f0f0f0;background: #fff;width: 100%;overflow: hidden}
.taskTime .tit{height: 35px;line-height: 35px;text-indent: 30px;font-size: 14px;background: url("data:image/png;base64,") no-repeat 10px center;background-size: 15px 15px}

.taskDuring{border-top:solid 1px #f0f0f0;background: #fff;width: 100%;overflow: hidden;border-bottom: solid 1px #dcdcdc}
.taskDuring .tit{height: 35px;line-height: 35px;text-indent: 30px;font-size: 14px;background: url("data:image/png;base64,") no-repeat 10px center;background-size: 15px 15px}
.taskDuring .week{height: 30px;line-height: 30px;width: 100%}
