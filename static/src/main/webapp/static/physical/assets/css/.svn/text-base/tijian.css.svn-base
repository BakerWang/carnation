p,ul{margin: 0;padding: 0;word-break: break-all}
html,body{width: 100%;height:100%;margin: 0;padding: 0;position: relative;font-family: sans-serif;background: #f5f5f5;color: #333;font-size: 12px;-moz-user-select:none;-webkit-user-select:none;-ms-user-select:none; -khtml-user-select:none; user-select:none }
a,button,input,canvas{-webkit-tap-highlight-color:rgba(0,0,0,0)}
a{text-decoration: none;color: #333;display: block}
canvas{display: block}
.area{width: 100%;margin-top: 10px;border-top: solid 1px #dcdcdc;border-bottom: solid 1px #dcdcdc;background: #fff;overflow: hidden}
.title{height:40px;line-height: 40px;text-indent: 31px;font-size: 14px;color: #333;border-bottom: solid 1px #f1f1f1}
.result .title{background: url("data:image/png;base64,") no-repeat 10px center;background-size: 16px 16px;}
.result .des{margin:15px 10px 0 10px;color: #b4b4b4;font-size: 14px;line-height: 20px}
.result .des .red{color:#ec1b1b}
.result .des .green{color: #20B757}
.result .askOnline{float: right;margin: 10px 10px 10px 0;color: #f1a954;font-size: 12px;text-decoration: none}
.history .title{background: url("data:image/png;base64,") no-repeat 10px center;background-size: 16px 16px}
#history{background: rgba(162,135,231,0.08);margin-top: 6px }
.history .ico{height: 30px;line-height: 30px;text-align: center}
.history .ico span{display: inline-block;font-size: 12px}
.history .ico span.ico1{width: 10px;height: 10px;border-radius: 5px;background: #6e51b6}
.history .ico span.ico2{width: 10px;height: 10px;border-radius: 5px;background: #d974ff;margin-left: 15px}
.history .ico span.text{margin-left: 5px;vertical-align: bottom}
.suggest .title{background: url("data:image/png;base64,") no-repeat 10px center;background-size: 16px 16px}
.suggest .title .title_tips{float: right;height: 16px;line-height: 16px;margin: 12px 10px 0 0;font-size: 12px;color: #858585}
.suggest .title .title_tips img{width: 16px;height: 16px;vertical-align: top;margin: 0 1px}
.suggest .listTit{height: 33px;line-height: 33px;text-indent: 15px;color: #333;font-size: 14px;position: relative;background: #e8f8ff}
.suggest .listTit:before,.suggest .listTit:after{position: absolute;top: 0px;width: 1px;height: 10px;content: "|"}
.suggest .listTit:before{left: -7px}
.suggest .listTit:after{left: 28px}
.suggest .content{color: #858585;font-size: 13px;margin: 10px;line-height: 18px;overflow: hidden}
.suggest .content .task_btn{float: right;width: 16px;height: 16px;background: url("data:image/png;base64,") no-repeat;background-size: contain}
.suggest .content .task_add{background: url("data:image/png;base64,") no-repeat;background-size: contain}
.groom{margin-bottom: 10px}
.groom .title{border-bottom: none;background: url("data:image/png;base64,") no-repeat 10px center;background-size: 16px 16px;}

.jb{height: 17px;margin: 39px 6.25% 40px 6.25%;border-radius: 8px;position: relative}
.jb span{position: absolute}
.jb span.low{color: #999;left: 9px;top: -20px}
.jb span.high{color: #999;right: 9px;top: -20px}
.jb span.zero{left: 9px;top: 20px}
.jb span.hundred{right: 9px;top: 20px}
.jb span.amount .numberical{left: 20px;top: -8px;font-size: 18px}
.tingli_span{left: 0;top: 0;height: 17px;
    background: -webkit-linear-gradient(left, #c8f8ff , #64f2ff);
    background: -o-linear-gradient(right, #c8f8ff, #64f2ff);
    background: -moz-linear-gradient(right, #c8f8ff, #64f2ff);
    background: linear-gradient(to right, #c8f8ff , #64f2ff);
    border-radius: 8px;
    width: 100%;
}
.tingli_span,.tingli_icon1,.tingli_icon2{
    -webkit-transition: all 1s ease-out;
    -o-transition: all 1s ease-out;
    transition: all 1s ease-out;
}

.tingli_icon1{top: -15px;left: 0px;background: url("data:image/png;base64,") no-repeat;background-size: 11px 15px;width: 11px;height: 15px}
.tingli_icon2{top: 17px;left: 98%;-webkit-transform:rotate(180deg);transform:rotate(180deg);background: url("data:image/png;base64,") no-repeat;background-size: 11px 15px;width: 11px;height: 15px}
.tingli_icon1 span{left: 0;top: -15px;font-size: 14px}
.tingli_icon2 span{left: 0;top: -15px;font-size: 14px
-webkit-transform:rotate(180deg);transform:rotate(180deg);
}
.jb_feihuoliang span.amount .numberical{left: -7px;top: -20px;font-size: 18px;text-align: center}
.jb_xueyang span.amount{background: url("data:image/png;base64,") no-repeat center;
width: 15px;height: 37px;left: 0px;bottom: 0px}
.jb_feihuoliang span.amount{background: url("data:image/png;base64,") no-repeat center;
    width: 15px;height: 38px;left: 0px;bottom: 0px}
.jb_xueyang{
    background: -webkit-linear-gradient(left, #10e5fa , #698aff);
    background: -o-linear-gradient(right, #10e5fa, #698aff);
    background: -moz-linear-gradient(right, #10e5fa, #698aff);
    background: linear-gradient(to right, #10e5fa , #698aff);
}
.jb_feihuoliang{
    background: -webkit-linear-gradient(left, #ffa498 , #f36c60);
    background: -o-linear-gradient(right, #ffa498, #f36c60);
    background: -moz-linear-gradient(right, #ffa498, #f36c60);
    background: linear-gradient(to right, #ffa498 , #f36c60);
}
.jb_tingli{
    background: -webkit-linear-gradient(left, #0fe6fa , #41c7ff);
    background: -o-linear-gradient(right, #0fe6fa, #41c7ff);
    background: -moz-linear-gradient(right, #0fe6fa, #41c7ff);
    background: linear-gradient(to right, #0fe6fa , #41c7ff);
}
